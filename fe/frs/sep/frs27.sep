EXEC SQL INCLUDE SQLCA;

int main(argc,argv)
int  argc;
EXEC SQL BEGIN DECLARE SECTION;
char *argv[];
EXEC SQL END DECLARE SECTION;
{
EXEC SQL BEGIN DECLARE SECTION;
   int   intcol;
   char  lvchcol[21];
   short ind1, ind2;
EXEC SQL END DECLARE SECTION;

   if (argc < 2)
   {
      printf("Usage: %s <database name>\n",argv[0]);
      exit(1);
   }

   EXEC SQL WHENEVER SQLERROR STOP;

   /* Start up INGRES and the INGRES/FORMS system */
   EXEC SQL CONNECT :argv[1];

   EXEC FRS FORMS;
 
   EXEC SQL WHENEVER SQLERROR CALL SQLPRINT;

   EXEC FRS MESSAGE 'Initializing Blob Form with validation. . .';
   EXEC FRS FORMINIT jla01val;

   EXEC FRS MESSAGE 'Initializing Blob Form without validation. . .';
   EXEC FRS FORMINIT jla01noval;

   EXEC FRS DISPLAY jla01noval UPDATE;
   EXEC FRS INITIALIZE;

   EXEC FRS ACTIVATE MENUITEM 'Browse';
   EXEC FRS BEGIN;
      EXEC SQL DECLARE csr CURSOR FOR
	 SELECT intcol, lvchcol 
	      FROM jla01;

      EXEC SQL OPEN csr FOR READONLY;
      while(1)
      {
	 EXEC SQL FETCH csr
	      INTO :intcol:ind1, :lvchcol:ind2;

         if(sqlca.sqlcode != 0)
	 { 
            EXEC SQL CLOSE csr;
	    EXEC FRS MESSAGE 'No more rows' WITH STYLE=POPUP;
	    EXEC FRS CLEAR FIELD ALL;
	    break;
	 }

	 EXEC FRS PUTFORM (intcol = :intcol:ind1, lvchcol = :lvchcol:ind2);

	 EXEC FRS REDISPLAY;

	 EXEC FRS SUBMENU;
	    EXEC FRS ACTIVATE MENUITEM 'Next', FRSKEY4;
	    EXEC FRS BEGIN;
	       /* Continue with cursor loop */
	       EXEC FRS MESSAGE 'Next row ...';
	       EXEC FRS CLEAR FIELD ALL;
	    EXEC FRS END;

	    EXEC FRS ACTIVATE MENUITEM 'End';
	    EXEC FRS BEGIN;
	       EXEC SQL CLOSE csr;
	       EXEC FRS CLEAR FIELD ALL;
               break;
	    EXEC FRS END;
      }

   EXEC FRS END;

   EXEC FRS ACTIVATE MENUITEM 'Exit';
   EXEC FRS BEGIN;
      EXEC FRS BREAKDISPLAY;
   EXEC FRS END;

   EXEC FRS FINALIZE;

   /* Terminate the FORMS and INGRES */
   EXEC FRS ENDFORMS;
   EXEC SQL DISCONNECT;

   exit(0);
} /* Main Program */

!!
