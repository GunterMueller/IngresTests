	Test Name: jca01.sep
	Time: Sun Feb  4 22:43:12 1990
	User Name: dave
 
	Test ID : (jca01)
	Module  : 
	Filename:
	Purpose	:	Testing 80/132 feature at runtime.  This test covers
			the ability to do runtime overrides with 3GL.
	Input Files  :
	Output Files :	jca01.sc,jca01.frm
	Database     :	SEPPARAMDB
	Tables       :
	Synopsis     :	Checks that runtime overrides (via 3GL and 4GL state			ments works f-
or all three (current, wide and narrow)
			options.  This requires creating several forms in
			Vifred and using them in a 3GL application.  No 4GL
			application is included.

	History	: 04-feb-1990	Created				Dave Hung
		  14-jul-1992	Changed				bobs
			Added synopsis and cleaned up deletes at end of script.
	18-FEB-1993  (jeremyb)  inserted -s flag after SQL and COPYFORM
			commands where needed to eliminate duplicate CANONs
			and DIFFs when run over HETNET and STAR.
          17-Jan-1996	(parmi04)  Added an alternate canon for NT_GENERIC
          11-Apr-1996	(clate01)
		Modified canons to work with Windows NT
          30-Aug-1996	(Author)
          11-Mar-1997	(Author)
          30-Jan-2006	(boija02) Updated copyright info for Ingres Corp.

*/
? fill jca01.sc 
!!
void
w_to_c()
{
exec frs display 'jca01_2' with style = fullscreen(screenwidth = current);
exec frs initialize;
exec frs begin;
exec frs end;
exec frs activate menuitem 'End';
exec frs begin;
exec frs	breakdisplay;
exec frs end;
}



void
c_to_w()
{
exec frs display 'jca01_1' with style = fullscreen(screenwidth = wide);
exec frs initialize;
exec frs begin;
exec frs end;
exec frs activate menuitem 'End';
exec frs begin;
exec frs	breakdisplay;
exec frs end;
}



void
w_to_n()
{
exec frs display 'jca01_2' with style = fullscreen(screenwidth = narrow);
exec frs initialize;
exec frs begin;
exec frs end;
exec frs activate menuitem 'End';
exec frs begin;
exec frs	breakdisplay;
exec frs end;
}



main(argc, argv)
int	argc;
char	**argv;
{
exec sql begin declare section;
	int	error;
	char	*db;
exec sql end declare section;

exec frs forms;

	if (argc < 2)
	{
exec frs	message 'No database name on command line.  Test exiting.'
			with style = popup;
exec frs	endforms;
		return;
	}
	else
	{
		argv++;
		db = *argv;
	}

exec sql connect :db;

exec sql inquire_ingres (:error = errorno);

	if (error != 0)
	{
exec frs	message 'Error connecting to database.  Test exiting.'
			with style = popup;
exec frs	endforms;
		return;
	}

exec frs forminit jca01_0;
exec frs forminit jca01_1;
exec frs forminit jca01_2;

exec frs display jca01_0 fill;
exec frs initialize;
exec frs begin;
exec frs end;
exec frs activate menuitem 'Wide_To_Cur';
exec frs begin;
		w_to_c();
exec frs end;
exec frs activate menuitem 'Cur_To_Wide';
exec frs begin;
		c_to_w();
exec frs end;
exec frs activate menuitem 'Wide_To_Nar';
exec frs begin;
		w_to_n();
exec frs end;
exec frs activate menuitem 'Quit';
exec frs begin;
exec frs	breakdisplay;
exec frs end;

exec sql disconnect;

exec frs endforms;
}
