main(argc, argv)
int	argc;
char	**argv;
{
    exec sql begin declare section;
    int		error;
    char	*formname = "jba08";
    char	*dbname;
    exec sql end declare section;

    exec frs forms;

        if (argc < 2)
        {
exec frs        message 'No database name on command line.  Test exiting.'
                        with style = popup;
exec frs        endforms;
                return;
        }
        else
        {
                argv++;
                dbname = *argv;
        }
 
 
    exec sql connect :dbname;
 
    exec sql inquire_ingres (:error = errorno);
 
        if (error != 0)
        {
exec frs        message 'Error connecting to database.  Test exiting.'
                        with style = popup;
exec frs        endforms;
                return;
        }


    exec frs forminit :formname;
    exec frs inittable :formname tbl query;

    exec frs display :formname query;
    exec frs activate menuitem 'Ftf_fill';
    exec frs begin;
	exec frs set_frs form (mode(:formname) = fill);
	exec frs inittable :formname tbl fill;
    exec frs end;

    exec frs activate menuitem 'F_update';
    exec frs begin;
	exec frs set_frs form (mode(:formname) = update);
    exec frs end;

    exec frs activate menuitem 'F_query';
    exec frs begin;
	exec frs set_frs form (mode(:formname) = query);
    exec frs end;

    exec frs activate menuitem 'end';
    exec frs begin;
	exec frs breakdisplay;
    exec frs end;

    exec frs endforms;

    exec sql disconnect;
}
