EXEC SQL INCLUDE SQLCA;

int main(argc,argv)
int  argc;
EXEC SQL BEGIN DECLARE SECTION;
char *argv[];
EXEC SQL END DECLARE SECTION;
{
EXEC SQL BEGIN DECLARE SECTION;
   char  msgbuf[200];
   int   ivar;
EXEC SQL END DECLARE SECTION;

   if (argc < 2)
   {
      printf("Usage: %s <database name>\n",argv[0]);
      exit(1);
   }

   EXEC SQL WHENEVER SQLERROR STOP;

   /* Start up INGRES and the INGRES/FORMS system */
   EXEC SQL CONNECT :argv[1];

   EXEC SQL WHENEVER SQLERROR CALL SQLPRINT;

   EXEC FRS FORMS;

   EXEC FRS MESSAGE 'Initializing Form...';
   EXEC FRS FORMINIT jla05;

   EXEC FRS DISPLAY jla05 UPDATE;

   EXEC FRS INITIALIZE;

   EXEC FRS ACTIVATE MENUITEM 'NextField';
   EXEC FRS BEGIN;
      EXEC FRS MESSAGE 'Next...';
      EXEC FRS SLEEP 1;
      EXEC FRS RESUME NEXTFIELD;
   EXEC FRS END;

   EXEC FRS ACTIVATE MENUITEM 'PreviousField';
   EXEC FRS BEGIN;
      EXEC FRS MESSAGE 'Prev...';
      EXEC FRS SLEEP 1;
      EXEC FRS RESUME PREVIOUSFIELD;
   EXEC FRS END;

   EXEC FRS ACTIVATE MENUITEM 'Exit';
   EXEC FRS BEGIN;
      EXEC FRS MESSAGE 'Exiting...';
      EXEC FRS SLEEP 1;
      EXEC FRS BREAKDISPLAY;
   EXEC FRS END;

   EXEC FRS FINALIZE;

   /* Terminate the FORMS and INGRES */
   EXEC FRS ENDFORMS;
   EXEC SQL DISCONNECT;

   exit(0);
} /* Main Program */
!!
