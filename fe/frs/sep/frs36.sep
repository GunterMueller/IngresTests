EXEC SQL INCLUDE SQLCA;

EXEC SQL BEGIN DECLARE SECTION;
   int state;
   char msg[81];
EXEC SQL END DECLARE SECTION;

int main(argc,argv)
int  argc;
EXEC SQL BEGIN DECLARE SECTION;
char *argv[];
EXEC SQL END DECLARE SECTION;
{

int del_row_count;

   if (argc < 2)
   {
      printf("Usage: %s <database name>\n",argv[0]);
      exit(1);
   }

   EXEC SQL WHENEVER SQLERROR STOP;

   /* Start up INGRES and the INGRES/FORMS system */
   EXEC SQL CONNECT :argv[1];

   EXEC FRS FORMS;

   EXEC FRS MESSAGE 'Initializing Form...';
   EXEC FRS FORMINIT jla10_frm;

   EXEC FRS INITTABLE jla10_frm jla10_tbl;

   EXEC FRS DISPLAY jla10_frm UPDATE;
   EXEC FRS INITIALIZE;
   EXEC FRS BEGIN;
      EXEC FRS PUTROW jla10_frm jla10_tbl (col1 = 'Some data here...');

      EXEC FRS GETROW jla10_frm jla10_tbl (:state = _STATE);
      EXEC FRS PUTFORM jla10_frm (cur_test = 'start = new(1)',
				  was = NULL,
				  now = :state);
   EXEC FRS END;

   EXEC FRS ACTIVATE MENUITEM 'State Test';
   EXEC FRS BEGIN;
      /* set to new(1) */
      EXEC FRS deleterow jla10_frm jla10_tbl;
      EXEC FRS PUTROW jla10_frm jla10_tbl (col1 = 'Some data here...');

      do_it("new(1)->new(1)",1);
      EXEC FRS SLEEP 1;
      do_it("new(1)->undefined(0) **ERROR**",0);
      do_it("new(1)->deleted(4) **ERROR**",4);
      do_it("new(1)->unchaged(2)",2);
      EXEC FRS SLEEP 1;

      do_it("unchanged(2)->unchanged(2)",2);
      EXEC FRS SLEEP 1;
      do_it("unchanged(2)->new(1) **ERROR**",1);
      do_it("unchanged(2)->undefined(0) **ERROR**",0);
      do_it("unchanged(2)->deleted(4) **ERROR**",4);
      do_it("unchanged(2)->changed(3)",3);
      EXEC FRS SLEEP 1;

      do_it("changed(3)->changed(3)",3);
      EXEC FRS SLEEP 1;
      do_it("changed(3)->new(1) **ERROR**",1);
      do_it("changed(3)->undefined(0) **ERROR**",0);
      do_it("changed(3)->deleted(4) **ERROR**",4);
      do_it("changed(3)->unchanged(2)",2);
      EXEC FRS SLEEP 1;

      /* set back to new(1) */
      EXEC FRS deleterow jla10_frm jla10_tbl;
      EXEC FRS PUTROW jla10_frm jla10_tbl (col1 = 'Some data here...');
      do_it("new(1)->changed(3)",3);
      EXEC FRS SLEEP 1;

      /* do undefined(0) -> ??? */
      EXEC FRS DELETEROW jla10_frm jla10_tbl;
      do_it("undefined(0)->undefined(0) **ERROR**",0);
      do_it("undefined(0)->new(1) **ERROR**",1);
      do_it("undefined(0)->unchanged(2) **ERROR**",2);
      do_it("undefined(0)->changed(3) **ERROR**",3);
      do_it("undefined(0)->deleted(4) **ERROR**",4);

   EXEC FRS END;

   EXEC FRS ACTIVATE MENUITEM 'Purgetable Test';
   EXEC FRS BEGIN;
      EXEC FRS PURGETABLE jla10_frm jla10_tbl;

      /* add a "deleted" row */
      EXEC FRS INSERTROW jla10_frm jla10_tbl;
      EXEC FRS PUTROW jla10_frm jla10_tbl (_STATE = 3);
      EXEC FRS deleterow jla10_frm jla10_tbl;

      /* add another "deleted" row */
      EXEC FRS INSERTROW jla10_frm jla10_tbl;
      EXEC FRS PUTROW jla10_frm jla10_tbl (_STATE = 3);
      EXEC FRS deleterow jla10_frm jla10_tbl;

      EXEC FRS REDISPLAY;
      EXEC FRS SLEEP 1;

      del_row_count = 0;
      EXEC FRS UNLOADTABLE jla10_frm jla10_tbl (:state = _STATE);
      EXEC FRS BEGIN;
	 if (state == 4)
	    del_row_count++;
      EXEC FRS END;

      sprintf(msg,"deleted rows found=%d (should be >= 1)",del_row_count);
      EXEC FRS MESSAGE :msg;
      EXEC FRS SLEEP 1;

      EXEC FRS MESSAGE 'now doing PURGETABLE';
      EXEC FRS SLEEP 1;

      EXEC FRS PURGETABLE jla10_frm jla10_tbl;

      del_row_count = 0;
      EXEC FRS UNLOADTABLE jla10_frm jla10_tbl (:state = _STATE);
      EXEC FRS BEGIN;
	 if (state == 4)
	    del_row_count++;
      EXEC FRS END;

      sprintf(msg,"deleted rows found=%d (should be = 0)",del_row_count);
      EXEC FRS MESSAGE :msg;
      EXEC FRS SLEEP 1;
   EXEC FRS END;

   EXEC FRS ACTIVATE MENUITEM 'Deleted State';
   EXEC FRS BEGIN;
      EXEC FRS PURGETABLE jla10_frm jla10_tbl;

      /* add a "deleted" row */
      EXEC FRS INSERTROW jla10_frm jla10_tbl;
      EXEC FRS PUTROW jla10_frm jla10_tbl (_STATE = 3);
      EXEC FRS deleterow jla10_frm jla10_tbl;

      EXEC FRS REDISPLAY;

      EXEC FRS UNLOADTABLE jla10_frm jla10_tbl (:state = _STATE);
      EXEC FRS BEGIN;
	 if (state == 4)
	 {
	    do_del("deleted(4)->deleted(4)",4);
	    EXEC FRS SLEEP 1;
	    do_del("deleted(4)->new(1) **ERROR**",1);
	    EXEC FRS SLEEP 1;
	    do_del("deleted(4)->unchanged(2) **ERROR**",2);
	    EXEC FRS SLEEP 1;
	    do_del("deleted(4)->changed(3) **ERROR**",3);
	    EXEC FRS SLEEP 1;
	    do_del("deleted(4)->undefined(0) **ERROR**",0);
	    EXEC FRS SLEEP 1;

	    EXEC FRS MESSAGE 'Done with changing of deleted row states.';
	    EXEC FRS ENDLOOP;
	 }
      EXEC FRS END;

   EXEC FRS END;

   EXEC FRS ACTIVATE MENUITEM 'Get State';
   EXEC FRS BEGIN;
      EXEC FRS UNLOADTABLE jla10_frm jla10_tbl (:state = _STATE);
      EXEC FRS BEGIN;
	 sprintf(msg,"State=%d",state);
         EXEC FRS MESSAGE :msg;
	 EXEC FRS SLEEP 1;
      EXEC FRS END;
   EXEC FRS END;

   EXEC FRS ACTIVATE MENUITEM 'Exit';
   EXEC FRS BEGIN;
      EXEC FRS BREAKDISPLAY;
   EXEC FRS END;

   EXEC FRS FINALIZE;

   /* Terminate the FORMS and INGRES */
   EXEC FRS ENDFORMS;
   EXEC SQL DISCONNECT;

   exit(0);
} /* Main Program */

do_it(msg, new_state)
EXEC SQL BEGIN DECLARE SECTION;
   char *msg;
   int new_state;
EXEC SQL END DECLARE SECTION;
{
   EXEC FRS GETROW jla10_frm jla10_tbl (:state = _STATE);
   EXEC FRS PUTFORM jla10_frm (cur_test = :msg, was = :state);
   EXEC FRS REDISPLAY;

   EXEC FRS PUTROW jla10_frm jla10_tbl (_STATE = :new_state);

   EXEC FRS GETROW jla10_frm jla10_tbl (:state = _STATE);
   EXEC FRS PUTFORM jla10_frm (now = :state);
   EXEC FRS REDISPLAY;
}

do_del(msg, new_state)
EXEC SQL BEGIN DECLARE SECTION;
   char *msg;
   int new_state;
EXEC SQL END DECLARE SECTION;
{
   EXEC FRS PUTFORM jla10_frm (cur_test = :msg, was = 4, now = NULL);
   EXEC FRS REDISPLAY;

   EXEC FRS PUTROW jla10_frm jla10_tbl (_STATE = :new_state);
}
!!
