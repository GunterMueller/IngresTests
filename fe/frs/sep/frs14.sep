void
narrow()
{
exec sql begin declare section;
	char	nbuf[400];
	char	npbuf[200];
exec sql end declare section;

	nbuf[0] = '\000';

exec frs display 'jca04_1' fill;
exec frs initialize;
exec frs begin;
exec frs end;
exec frs activate menuitem 'Message';
exec frs begin;
exec frs	getform (:nbuf = msgfld);
exec frs	message :nbuf with style = popup;
exec frs end;
exec frs activate menuitem 'Prompt';
exec frs begin;
exec frs	getform (:nbuf = msgfld);
exec frs	prompt (:nbuf, :npbuf) with style = popup;
exec frs end;
exec frs activate menuitem '3Menu';
exec frs begin;
exec frs end;
exec frs activate menuitem '4Menu';
exec frs begin;
exec frs end;
exec frs activate menuitem '5Menu';
exec frs begin;
exec frs end;
exec frs activate menuitem '6Menu';
exec frs begin;
exec frs end;
exec frs activate menuitem '7Menu';
exec frs begin;
exec frs end;
exec frs activate menuitem '8Menu';
exec frs begin;
exec frs end;
exec frs activate menuitem '9Menu';
exec frs begin;
exec frs end;
exec frs activate menuitem 'End';
exec frs begin;
exec frs	breakdisplay;
exec frs end;
}



void
wide()
{
exec sql begin declare section;
	char	wbuf[400];
	char	wpbuf[200];
exec sql end declare section;

	wbuf[0] = '\000';

exec frs display 'jca04_2' fill;
exec frs initialize;
exec frs begin;
exec frs end;
exec frs activate menuitem 'Message';
exec frs begin;
exec frs	getform (:wbuf = msgfld);
exec frs	message :wbuf with style = popup;
exec frs end;
exec frs activate menuitem 'Prompt';
exec frs begin;
exec frs	getform (:wbuf = msgfld);
exec frs	prompt (:wbuf, :wpbuf) with style = popup;
exec frs end;
exec frs activate menuitem '3Menuitem_xyz';
exec frs begin;
exec frs end;
exec frs activate menuitem '4Menuitem_xyz';
exec frs begin;
exec frs end;
exec frs activate menuitem '5Menuitem_xyz';
exec frs begin;
exec frs end;
exec frs activate menuitem '6Menuitem_xyz';
exec frs begin;
exec frs end;
exec frs activate menuitem '7Menuitem_xyz';
exec frs begin;
exec frs end;
exec frs activate menuitem '8Menuitem_xyz';
exec frs begin;
exec frs end;
exec frs activate menuitem '9Menuitem_xyz';
exec frs begin;
exec frs end;
exec frs activate menuitem 'End';
exec frs begin;
exec frs	breakdisplay;
exec frs end;
}



main(argc, argv)
int	argc;
char	**argv;
{
exec sql begin declare section;
	int	error;
	char	*db;
exec sql end declare section;

exec frs forms;

	if (argc < 2)
	{
exec frs	message 'No database name on command line.  Test exiting.'
			with style = popup;
exec frs	endforms;
		return;
	}
	else
	{
		argv++;
		db = *argv;
	}

exec sql connect :db;

exec sql inquire_ingres (:error = errorno);

	if (error != 0)
	{
exec frs	message 'Error connecting to database.  Test exiting.'
			with style = popup;
exec frs	endforms;
		return;
	}

exec frs forminit jca04_0;
exec frs forminit jca04_1;
exec frs forminit jca04_2;

exec frs display jca04_0 fill;
exec frs initialize;
exec frs begin;
exec frs end;
exec frs activate menuitem 'Narrow';
exec frs begin;
		narrow();
exec frs end;
exec frs activate menuitem 'Wide';
exec frs begin;
		wide();
exec frs end;
exec frs activate menuitem 'Quit';
exec frs begin;
exec frs	breakdisplay;
exec frs end;

exec sql disconnect;

exec frs endforms;
}
