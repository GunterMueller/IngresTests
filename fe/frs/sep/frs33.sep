EXEC SQL INCLUDE SQLCA;

int main(argc,argv)
int  argc;
EXEC SQL BEGIN DECLARE SECTION;
char *argv[];
EXEC SQL END DECLARE SECTION;
{
EXEC SQL BEGIN DECLARE SECTION;
   int  ivar;
   char msgbuf[100];
EXEC SQL END DECLARE SECTION;

   if (argc < 2)
   {
      printf("Usage: %s <database name>\n",argv[0]);
      exit(1);
   }

   EXEC SQL WHENEVER SQLERROR STOP;

   /* Start up INGRES and the INGRES/FORMS system */
   EXEC SQL CONNECT :argv[1];

   EXEC FRS FORMS;

   EXEC FRS MESSAGE 'Initializing Form...';
   EXEC FRS FORMINIT jla07;

   EXEC FRS INITTABLE jla07 scrolltable read;

   EXEC FRS DISPLAY jla07 UPDATE;
   EXEC FRS INITIALIZE;
   EXEC FRS BEGIN;
      EXEC FRS LOADTABLE jla07 scrolltable
	     (intcol = 1, charcol = 'row 1');
      EXEC FRS LOADTABLE jla07 scrolltable
	     (intcol = 2, charcol = 'row 2');
      EXEC FRS LOADTABLE jla07 scrolltable
	     (intcol = 3, charcol = 'row 3');
      EXEC FRS LOADTABLE jla07 scrolltable
	     (intcol = 4, charcol = 'row 4');
      EXEC FRS LOADTABLE jla07 scrolltable
	     (intcol = 5, charcol = 'row 5');
      EXEC FRS LOADTABLE jla07 scrolltable
	     (intcol = 6, charcol = 'row 6');
      EXEC FRS LOADTABLE jla07 scrolltable
	     (intcol = 7, charcol = 'row 7');
   EXEC FRS END;

   EXEC FRS ACTIVATE MENUITEM 'Scroll_1';
   EXEC FRS BEGIN;
      EXEC FRS SET_FRS FRS (OUTOFDATAMESSAGE = 1);    /* default is ON */
      EXEC FRS INQUIRE_FRS FRS (:ivar = OUTOFDATAMESSAGE);
      sprintf(msgbuf,"OUTOFDATAMESSAGE = %d",ivar);
      EXEC FRS MESSAGE :msgbuf;
      EXEC FRS SLEEP 2;
   EXEC FRS END;

   EXEC FRS ACTIVATE MENUITEM 'Scroll_0';
   EXEC FRS BEGIN;
      EXEC FRS SET_FRS FRS (OUTOFDATAMESSAGE = 0);
      EXEC FRS INQUIRE_FRS FRS (:ivar = OUTOFDATAMESSAGE);
      sprintf(msgbuf,"OUTOFDATAMESSAGE = %d",ivar);
      EXEC FRS MESSAGE :msgbuf;
      EXEC FRS SLEEP 2;
   EXEC FRS END;

   EXEC FRS ACTIVATE MENUITEM 'Scroll_2';
   EXEC FRS BEGIN;
      EXEC FRS SET_FRS FRS (OUTOFDATAMESSAGE = 2);
      EXEC FRS INQUIRE_FRS FRS (:ivar = OUTOFDATAMESSAGE);
      sprintf(msgbuf,"OUTOFDATAMESSAGE = %d",ivar);
      EXEC FRS MESSAGE :msgbuf;
      EXEC FRS SLEEP 2;
   EXEC FRS END;

   EXEC FRS ACTIVATE MENUITEM 'Scroll_ON';
   EXEC FRS BEGIN;
      EXEC FRS SET_FRS FRS (OUTOFDATAMESSAGE = ON);
      EXEC FRS INQUIRE_FRS FRS (:ivar = OUTOFDATAMESSAGE);
      sprintf(msgbuf,"OUTOFDATAMESSAGE = %d",ivar);
      EXEC FRS MESSAGE :msgbuf;
      EXEC FRS SLEEP 2;
   EXEC FRS END;

   EXEC FRS ACTIVATE MENUITEM 'Scroll_OFF';
   EXEC FRS BEGIN;
      EXEC FRS SET_FRS FRS (OUTOFDATAMESSAGE = OFF);
      EXEC FRS INQUIRE_FRS FRS (:ivar = OUTOFDATAMESSAGE);
      sprintf(msgbuf,"OUTOFDATAMESSAGE = %d",ivar);
      EXEC FRS MESSAGE :msgbuf;
      EXEC FRS SLEEP 2;
   EXEC FRS END;

   EXEC FRS ACTIVATE MENUITEM 'Scroll_BELL';
   EXEC FRS BEGIN;
      EXEC FRS SET_FRS FRS (OUTOFDATAMESSAGE = BELL);
      EXEC FRS INQUIRE_FRS FRS (:ivar = OUTOFDATAMESSAGE);
      sprintf(msgbuf,"OUTOFDATAMESSAGE = %d",ivar);
      EXEC FRS MESSAGE :msgbuf;
      EXEC FRS SLEEP 2;
   EXEC FRS END;

   EXEC FRS ACTIVATE MENUITEM 'Scroll_var';
   EXEC FRS BEGIN;
      /* This setting does not change the previous value of OUTOFDATAMESSAGE */
      ivar = -5;
      EXEC FRS SET_FRS FRS (OUTOFDATAMESSAGE = :ivar);             
      EXEC FRS INQUIRE_FRS FRS (:ivar = OUTOFDATAMESSAGE);
      sprintf(msgbuf,"OUTOFDATAMESSAGE = %d",ivar);
      EXEC FRS MESSAGE :msgbuf;
      EXEC FRS SLEEP 2;
   EXEC FRS END;

   EXEC FRS ACTIVATE MENUITEM 'Exit';
   EXEC FRS BEGIN;
      EXEC FRS BREAKDISPLAY;
   EXEC FRS END;

   EXEC FRS FINALIZE;

   /* Terminate the FORMS and INGRES */
   EXEC FRS ENDFORMS;
   EXEC SQL DISCONNECT;

   exit(0);
} /* Main Program */
!!
