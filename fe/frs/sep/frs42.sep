!!
main(argc, argv)
int	argc;
char	**argv;
{
exec sql begin declare section;
	int	error;
	char	*db;
exec sql end declare section;

exec frs forms;

	if (argc < 2)
	{
exec frs	message 'No database name on command line.  Test exiting.'
			with style = popup;
exec frs	endforms;
		return;
	}
	else
	{
		argv++;
		db = *argv;
	}

exec sql connect :db;

exec sql inquire_ingres (:error = errorno);

	if (error != 0)
	{
exec frs	message 'Error connecting to database.  Test exiting.'
			with style = popup;
exec frs	endforms;
		return;
	}

exec frs forminit jqa02;

exec frs display jqa02 fill;
exec frs initialize;
exec frs begin;
exec frs	inittable jqa02 tf read;
exec frs	loadtable jqa02 tf (col1 = 'row0', col2 = 0);
exec frs	loadtable jqa02 tf (col1 = 'row1', col2 = 1);
exec frs	loadtable jqa02 tf (col1 = 'row2', col2 = 2);
exec frs	loadtable jqa02 tf (col1 = 'row3', col2 = 3);
exec frs	loadtable jqa02 tf (col1 = 'row4', col2 = 4);
exec frs	loadtable jqa02 tf (col1 = 'row5', col2 = 5);
exec frs	loadtable jqa02 tf (col1 = 'row6', col2 = 6);
exec frs	loadtable jqa02 tf (col1 = 'row7', col2 = 7);
exec frs	loadtable jqa02 tf (col1 = 'row8', col2 = 8);
exec frs end;
exec frs activate menuitem 'Fill_Set';
exec frs begin;
exec frs	set_frs field '' (mode = fill);
exec frs end;
exec frs activate menuitem 'Update_Set';
exec frs begin;
exec frs	set_frs field '' (mode = update);
exec frs end;
exec frs activate menuitem 'Query_Set';
exec frs begin;
exec frs	set_frs field '' (mode = query);
exec frs end;
exec frs activate menuitem 'Init_To_Read';
exec frs begin;
exec frs	inittable jqa02 tf read;
exec frs	loadtable jqa02 tf (col1 = 'row0', col2 = 0);
exec frs	loadtable jqa02 tf (col1 = 'row1', col2 = 1);
exec frs	loadtable jqa02 tf (col1 = 'row2', col2 = 2);
exec frs	loadtable jqa02 tf (col1 = 'row3', col2 = 3);
exec frs	loadtable jqa02 tf (col1 = 'row4', col2 = 4);
exec frs	loadtable jqa02 tf (col1 = 'row5', col2 = 5);
exec frs	loadtable jqa02 tf (col1 = 'row6', col2 = 6);
exec frs	loadtable jqa02 tf (col1 = 'row7', col2 = 7);
exec frs	loadtable jqa02 tf (col1 = 'row8', col2 = 8);
exec frs end;
exec frs activate menuitem 'Quit';
exec frs begin;
exec frs	breakdisplay;
exec frs end;

exec sql disconnect;

exec frs endforms;
}
