main(argc, argv)
int	argc;
char	**argv;
{
    exec sql begin declare section;
    int		error;
    char	*formname = "jba10";
    char	*dbname;
    exec sql end declare section;

    exec frs forms;
        if (argc < 2)
        {
exec frs        message 'No database name on command line.  Test exiting.'
                        with style = popup;
exec frs        endforms;
                return;
        }
        else
        {
                argv++;
                dbname = *argv;
        }
 
 
    exec sql connect :dbname;
 
    exec sql inquire_ingres (:error = errorno);
 
        if (error != 0)
        {
exec frs        message 'Error connecting to database.  Test exiting.'
                        with style = popup;
exec frs        endforms;
                return;
        }
 

    exec frs forminit :formname;

    exec frs display :formname;
    exec frs activate menuitem 'end';
    exec frs begin;
	exec frs enddisplay;
    exec frs end;

    exec frs endforms;

    exec sql disconnect;
}
