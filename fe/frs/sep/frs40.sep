exec frs	loadtable jqa00 tf (col1 = 'row4_r', col2 = 4);
exec frs	loadtable jqa00 tf (col1 = 'row5_r', col2 = 5);
exec frs	loadtable jqa00 tf (col1 = 'row6_r', col2 = 6);
exec frs	loadtable jqa00 tf (col1 = 'row7_r', col2 = 7);
exec frs	loadtable jqa00 tf (col1 = 'row8_r', col2 = 8);
exec frs end;
exec frs activate menuitem 'Fill_Init';
exec frs begin;
exec frs	inittable jqa00 tf read;
exec frs	loadtable jqa00 tf (col1 = 'row0_f', col2 = 0);
exec frs	loadtable jqa00 tf (col1 = 'row1_f', col2 = 1);
exec frs	loadtable jqa00 tf (col1 = 'row2_f', col2 = 2);
exec frs	loadtable jqa00 tf (col1 = 'row3_f', col2 = 3);
exec frs	loadtable jqa00 tf (col1 = 'row4_f', col2 = 4);
exec frs	loadtable jqa00 tf (col1 = 'row5_f', col2 = 5);
exec frs	loadtable jqa00 tf (col1 = 'row6_f', col2 = 6);
exec frs	loadtable jqa00 tf (col1 = 'row7_f', col2 = 7);
exec frs	loadtable jqa00 tf (col1 = 'row8_f', col2 = 8);
exec frs end;
exec frs activate menuitem 'Update_Init';
exec frs begin;
exec frs	inittable jqa00 tf read;
exec frs	loadtable jqa00 tf (col1 = 'row0_u', col2 = 0);
exec frs	loadtable jqa00 tf (col1 = 'row1_u', col2 = 1);
exec frs	loadtable jqa00 tf (col1 = 'row2_u', col2 = 2);
exec frs	loadtable jqa00 tf (col1 = 'row3_u', col2 = 3);
exec frs	loadtable jqa00 tf (col1 = 'row4_u', col2 = 4);
exec frs	loadtable jqa00 tf (col1 = 'row5_u', col2 = 5);
exec frs	loadtable jqa00 tf (col1 = 'row6_u', col2 = 6);
exec frs	loadtable jqa00 tf (col1 = 'row7_u', col2 = 7);
exec frs	loadtable jqa00 tf (col1 = 'row8_u', col2 = 8);
exec frs end;
exec frs activate menuitem 'Query_Init';
exec frs begin;
exec frs	inittable jqa00 tf read;
exec frs	loadtable jqa00 tf (col1 = 'row0_u', col2 = 0);
exec frs	loadtable jqa00 tf (col1 = 'row1_u', col2 = 1);
exec frs	loadtable jqa00 tf (col1 = 'row2_u', col2 = 2);
exec frs	loadtable jqa00 tf (col1 = 'row3_u', col2 = 3);
exec frs	loadtable jqa00 tf (col1 = 'row4_u', col2 = 4);
