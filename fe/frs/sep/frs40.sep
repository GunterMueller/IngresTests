exec frs	loadtable jqa00 tf (col1 = 'row8_u', col2 = 8);
exec frs end;
exec frs activate menuitem 'Quit';
exec frs begin;
exec frs	breakdisplay;
exec frs end;

exec sql disconnect;

exec frs endforms;
}
