EXEC SQL INCLUDE SQLCA;

int main(argc,argv)
int  argc;
EXEC SQL BEGIN DECLARE SECTION;
char *argv[];
EXEC SQL END DECLARE SECTION;
{
EXEC SQL BEGIN DECLARE SECTION;
   char  msgbuf[200];
   int   ivar;
   int   counter=0;
   char  menuname[20],
	 newname[20];
EXEC SQL END DECLARE SECTION;

   if (argc < 2)
   {
      printf("Usage: %s <database name>\n",argv[0]);
      exit(1);
   }

   EXEC SQL WHENEVER SQLERROR STOP;

   /* Start up INGRES and the INGRES/FORMS system */
   EXEC SQL CONNECT :argv[1];

   EXEC FRS FORMS;

   sprintf(menuname,"Menu%d",counter++);

   EXEC FRS MESSAGE 'Initializing Form...';
   EXEC FRS FORMINIT jla09;

   EXEC FRS DISPLAY jla09 UPDATE;

   EXEC FRS INITIALIZE;

   EXEC FRS ACTIVATE MENUITEM 'Rename';
   EXEC FRS BEGIN;
      sprintf(newname,"Menu%d",counter++);
      sprintf(msgbuf,"renaming menu '%s' to '%s'...",menuname,newname);
      EXEC FRS MESSAGE :msgbuf;
      EXEC FRS SLEEP 2;
      EXEC FRS SET_FRS MENU jla09 (RENAME(:menuname) = :newname);
      strcpy(menuname,newname);
   EXEC FRS END;

   EXEC FRS ACTIVATE MENUITEM :menuname;
   EXEC FRS BEGIN;
      sprintf(msgbuf,"Hello from '%s'",menuname);
      EXEC FRS MESSAGE :msgbuf;
      EXEC FRS SLEEP 2;
   EXEC FRS END;

   EXEC FRS ACTIVATE MENUITEM 'Menu_ON';
   EXEC FRS BEGIN;
      EXEC FRS SET_FRS MENU jla09 (ACTIVE(:menuname) = ON);
      EXEC FRS INQUIRE_FRS MENU jla09 (:ivar = ACTIVE(:menuname));
      sprintf(msgbuf,"Menu '%s' is ON: %d",menuname,ivar);
      EXEC FRS MESSAGE :msgbuf;
      EXEC FRS SLEEP 2;
   EXEC FRS END;

   EXEC FRS ACTIVATE MENUITEM 'Menu_OFF';
   EXEC FRS BEGIN;
      EXEC FRS SET_FRS MENU jla09 (ACTIVE(:menuname) = OFF);
      EXEC FRS INQUIRE_FRS MENU jla09 (:ivar = ACTIVE(:menuname));
      sprintf(msgbuf,"Menu '%s' is OFF: %d",menuname,ivar);
      EXEC FRS MESSAGE :msgbuf;
      EXEC FRS SLEEP 2;
   EXEC FRS END;

   EXEC FRS ACTIVATE MENUITEM 'Menu_Toggle';
   EXEC FRS BEGIN;
      EXEC FRS INQUIRE_FRS MENU '' (:ivar = ACTIVE(:menuname));
      if(ivar)
	 ivar = 0;
      else
	 ivar = 1;

      EXEC FRS SET_FRS MENU '' (ACTIVE(:menuname) = :ivar);
      EXEC FRS INQUIRE_FRS MENU '' (:ivar = ACTIVE(:menuname));
      sprintf(msgbuf,"Menu '%s' is: %d",menuname,ivar);
      EXEC FRS MESSAGE :msgbuf;
      EXEC FRS SLEEP 2;
   EXEC FRS END;

   EXEC FRS ACTIVATE MENUITEM 'Errors';
   EXEC FRS BEGIN;
      EXEC FRS MESSAGE 'Renaming non-existant menuitem';
      EXEC FRS SLEEP 2;
      EXEC FRS SET_FRS MENU jla09 (RENAME(foo) = foo2);

      EXEC FRS MESSAGE 'Disabling non-existant menuitem';
      EXEC FRS SLEEP 2;
      EXEC FRS SET_FRS MENU jla09 (ACTIVE(foo) = 0);

      EXEC FRS MESSAGE 'Enabling non-existant menuitem';
      EXEC FRS SLEEP 2;
      EXEC FRS SET_FRS MENU jla09 (ACTIVE(foo) = 1);
   EXEC FRS END;

   EXEC FRS ACTIVATE MENUITEM 'All_OFF';
   EXEC FRS BEGIN;
      EXEC FRS SET_FRS MENU '' (ACTIVE('Rename')      = OFF);
      EXEC FRS SET_FRS MENU '' (ACTIVE(:menuname)     = OFF);
      EXEC FRS SET_FRS MENU '' (ACTIVE('Menu_ON')     = OFF);
      EXEC FRS SET_FRS MENU '' (ACTIVE('Menu_OFF')    = OFF);
      EXEC FRS SET_FRS MENU '' (ACTIVE('Menu_Toggle') = OFF);
      EXEC FRS SET_FRS MENU '' (ACTIVE('Errors')      = OFF);
      EXEC FRS SET_FRS MENU '' (ACTIVE('All_OFF')     = OFF);

      EXEC FRS SET_FRS MENU '' (ACTIVE('Exit')        = OFF);
   EXEC FRS END;

   EXEC FRS ACTIVATE MENUITEM 'Exit';
   EXEC FRS BEGIN;
      EXEC FRS MESSAGE 'Exiting.........';
      EXEC FRS SLEEP 2;
      EXEC FRS BREAKDISPLAY;
   EXEC FRS END;

   EXEC FRS FINALIZE;

   /* Terminate the FORMS and INGRES */
   EXEC FRS ENDFORMS;
   EXEC SQL DISCONNECT;

   exit(0);
} /* Main Program */
!!
