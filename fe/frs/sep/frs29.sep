EXEC SQL INCLUDE SQLCA;

int main(argc,argv)
int  argc;
EXEC SQL BEGIN DECLARE SECTION;
char *argv[];
EXEC SQL END DECLARE SECTION;
{
EXEC SQL BEGIN DECLARE SECTION;
   char  msgbuf[100];
   int   datatype,
         type,
	 length,
	 intcol;
   double fld1,
	  fld2,
	  fld3;
   short ind1, ind2, ind3, ind4;
EXEC SQL END DECLARE SECTION;

   if (argc < 2)
   {
      printf("Usage: %s <database name>\n",argv[0]);
      exit(1);
   }

   EXEC SQL WHENEVER SQLERROR STOP;

   /* Start up INGRES and the INGRES/FORMS system */
   EXEC SQL CONNECT :argv[1];

   EXEC SQL WHENEVER SQLERROR CALL SQLPRINT;

   EXEC FRS FORMS;

   EXEC FRS MESSAGE 'Initializing Form...';
   EXEC FRS FORMINIT jla03;

   EXEC FRS DISPLAY jla03;
   EXEC FRS INITIALIZE;

   EXEC FRS ACTIVATE MENUITEM 'Select';
   EXEC FRS BEGIN;
      EXEC SQL DECLARE csr CURSOR FOR
	 SELECT intcol, fltcol2, fltcol104, fltcol20
	     FROM jla03;

      EXEC SQL OPEN csr FOR READONLY;

      while(1)
      {
	 EXEC SQL FETCH csr
	      INTO :intcol:ind1, :fld1:ind2, :fld2:ind3, :fld3:ind4;

         if(sqlca.sqlcode != 0)
	 { 
            EXEC SQL CLOSE csr;
	    EXEC FRS MESSAGE 'No more rows' WITH STYLE=POPUP;
	    EXEC FRS CLEAR FIELD ALL;
	    break;
	 }

	 EXEC FRS PUTFORM (intcol    = :intcol:ind1,
			   fltcol2   = :fld1:ind2,
			   fltcol104 = :fld2:ind3,
			   fltcol20  = :fld3:ind4);

	 EXEC FRS REDISPLAY;

	 EXEC FRS SUBMENU;
	    EXEC FRS ACTIVATE MENUITEM 'Next', FRSKEY4;
	    EXEC FRS BEGIN;
	       /* Continue with cursor loop */
	       EXEC FRS MESSAGE 'Next row ...';
	       EXEC FRS CLEAR FIELD ALL;
	    EXEC FRS END;

	    EXEC FRS ACTIVATE MENUITEM 'End';
	    EXEC FRS BEGIN;
	       EXEC SQL CLOSE csr;
	       EXEC FRS CLEAR FIELD ALL;
               break;
	    EXEC FRS END;
      }

   EXEC FRS END;

   EXEC FRS ACTIVATE MENUITEM 'Save';
   EXEC FRS BEGIN;
      EXEC FRS GETFORM (:intcol:ind1 = intcol,
			:fld1:ind2   = fltcol2,
			:fld2:ind3   = fltcol104,
			:fld3:ind4   = fltcol20);
      EXEC SQL INSERT INTO jla03 
	    VALUES (:intcol:ind1, :fld1:ind2, :fld2:ind3, :fld3:ind4);
      EXEC SQL COMMIT;
   EXEC FRS END;

   EXEC FRS ACTIVATE MENUITEM 'Inquire';
   EXEC FRS BEGIN;
      EXEC FRS INQUIRE_FRS FIELD jla03
	   (:datatype = DATATYPE, :type = TYPE, :length = LENGTH);

      sprintf(msgbuf,"Datatype(+/-31) is: %d   Type(2) is: %d   Length is: %d",
	     datatype, type, length);

      EXEC FRS MESSAGE :msgbuf WITH STYLE=POPUP;

   EXEC FRS END;

   EXEC FRS ACTIVATE MENUITEM 'Exit';
   EXEC FRS BEGIN;
      EXEC FRS BREAKDISPLAY;
   EXEC FRS END;

   EXEC FRS FINALIZE;

   /* Terminate the FORMS and INGRES */
   EXEC FRS ENDFORMS;
   EXEC SQL DISCONNECT;

   exit(0);
} /* Main Program */

!!
