main(argc, argv)
int	argc;
char	**argv;
{
    exec sql begin declare section;
    int		error;	
    char	*dbname;
    char	*formname = "jpa00";
    char	col[100];
    exec sql end declare section;

    exec frs forms;

        if (argc < 2)
        {
exec frs        message 'No database name on command line.  Test exiting.'
                        with style = popup;
exec frs        endforms;
                return;
        }
        else
        {
                argv++;
                dbname = *argv;
        }
 
 
    exec sql connect :dbname;
 
    exec sql inquire_ingres (:error = errorno);
 
        if (error != 0)
        {
exec frs        message 'Error connecting to database.  Test exiting.'
                        with style = popup;
exec frs        endforms;
                return;
        }
 

    exec frs forminit :formname;
    exec frs inittable :formname tbl;

    exec frs display :formname;
    exec frs activate menuitem 'tbl_visible';
    exec frs begin;
	exec frs set_frs field :formname (invisible(tbl) = 0);
    exec frs end;

    exec frs activate menuitem 'tbl_invisible';
    exec frs begin;
	exec frs set_frs field :formname (invisible(tbl) = 1);
    exec frs end;

    exec frs activate menuitem 'col_visible';
    exec frs begin;
	/* Assumes no typo's, and accurate column names. */
	exec frs prompt('Which column? ', :col);
	exec frs set_frs column :formname tbl (invisible(:col) = 0);
    exec frs end;

    exec frs activate menuitem 'col_invisible';
    exec frs begin;
	/* Assumes no typo's, and accurate column names. */
	exec frs prompt('Which column? ', :col);
	exec frs set_frs column :formname tbl (invisible(:col) = 1);
    exec frs end;

    exec frs activate menuitem 'end';
    exec frs begin;
	exec frs breakdisplay;
    exec frs end;

    exec frs endforms;

    exec sql disconnect;
}

