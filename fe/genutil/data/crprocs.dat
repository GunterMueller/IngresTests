create procedure proc1
	(base integer, n integer, label varchar(1000)) as
declare
	limit integer;
	err integer;
begin
	limit = base + n;
	err = 0;
	while (base < limit) and (err = 0) do
		message 'dummy message';
		base = base + 1;
	endwhile;
	return;
end
