.NAME	rga06_labels
.SHORTREMARK	RBF Style Labels
.DECLARE
	ii_rbf_date_fmt = varchar(255) with value 'd''03-feb-1901''',
	ii_rbf_time_fmt = varchar(255) with value 'd''16:05:06''',
	ii_rbf_pageno_fmt = varchar(255) with value '''\-zzzn \-''',
	linecnt = int,
	maxfldsiz = int,
	maxperlin = int,
	in_block = c3
.QUERY
	SELECT  *
	FROM	rga06 RBF
		/* WIDTH  132
		*  DO NOT MODIFY.  DOING SO MAY CAUSE REPORT TO BE UNUSABLE.
		*/
.SORT	xsmallint : a

.HEADER	report
	.FORMAT xsmallint(f10)
	.FORMAT xinteger(f10)
	.FORMAT xfloat(n10.3)
	.FORMAT xchar10(c10)
	.FORMAT xvarchar10(c10)
	.RM 132
	.LM 0
	.LET linecnt=0
	.LET maxfldsiz=13
	.LET maxperlin=10
	.LET in_block='no'
	.LEFT 
	.PRINT current_time($ii_rbf_date_fmt)
	.RIGHT 
	.PRINT current_time($ii_rbf_time_fmt)
	.NEWLINE 1
	.NEED 3
	.NEWLINE 1
	.BEGIN BLOCK
		.TAB 55
		.UNDERLINE 
			.PRINT 'Report on Table: rga06'
		.NOUNDERLINE 
		.NEWLINE 
	.END BLOCK
	.NEWLINE 1

.HEADER	page
	.IF $in_block='yes' .THEN
		.LET in_block='no'
	.END BLOCK
.ENDIF 
.LEFT 
.PRINT current_time($ii_rbf_date_fmt)
.CENTER 
.PRINT 'Report: rga06_labels'
.RIGHT 
.PRINT current_time($ii_rbf_time_fmt)
.NEWLINE 1

.FOOTER	page
	.IF $in_block='yes' .THEN
		.LET in_block='no'
	.END BLOCK
.ENDIF 
.NEWLINE 1
.CENTER 
.PRINT page_number($ii_rbf_pageno_fmt)
.NEWLINE 1

.DETAIL
	.IF $in_block='no' .THEN
		.LET in_block='yes'
		.BEGIN BLOCK
		.ENDIF 
		.TOP 
		.PRINT xsmallint
		.NEWLINE 1
		.PRINT xinteger
		.NEWLINE 1
		.PRINT xfloat
		.NEWLINE 1
		.PRINT xchar10
		.NEWLINE 1
		.PRINT xvarchar10
		.NEWLINE 1
		.LET linecnt=$linecnt+1
		.IF $linecnt=$maxperlin .THEN
			.LET linecnt=0
			.NEWLINE 2
		.END BLOCK
		.LM 0
		.LET in_block='no'
	.ELSE 
		.LM +($maxfldsiz)
		.TOP 
		.LINESTART 
	.ENDIF 

.FOOTER	report
	.IF $in_block='yes' .THEN
		.LET in_block='no'
	.END BLOCK
.ENDIF 
.NEED 2
.NEWLINE 2


