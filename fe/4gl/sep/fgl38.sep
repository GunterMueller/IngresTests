initialize ( h_char = varchar(40), h_num = smallint ) =
{
	message 'Testing inquire_forms column...';
	sleep 2;
}

'TableName' =
{
	inquire_forms table pja24uf1 ( h_char = name );
	message 'Table name : ' + :h_char;
	sleep 2;
}

'RowNo' = 
{ 
  inquire_forms table pja24uf1 ( h_num = rowno ); 
  message 'current row # : ' + varchar(:h_num); 
	sleep 2;
}

'MaxRow' = 
{ 
  inquire_forms table pja24uf1 ( h_num = maxrow ); 
  message '# of displayed rows defined in vifred : ' + varchar(:h_num);
	sleep 2;
}

'LastRow' = 
{ 
  inquire_forms table pja24uf1 ( h_num = lastrow ); 
  message '# of displayed rows that contain data  : ' + varchar(:h_num);
	sleep 2;
}

'DataRow' = 
{ 
  inquire_forms table pja24uf1 ( h_num = datarows ); 
  message '# of nondeleted rows : ' + varchar(:h_num);
	sleep 2;
}

'MaxCol' = 
{ 
  inquire_forms table pja24uf1 ( h_num = maxcol ); 
  message '# of displayed columns : ' + varchar(:h_num);
	sleep 2;
}

'Column' =
{
	inquire_forms table pja24uf1 ( h_char = column );
	message 'Column name : ' + :h_char;
	sleep 2;
}

'End', key frskey3 =
{
	return;
}

