/*
** Name:	pba04uf1.osq -	Test various local procedure operations.
**
** Description:
**	Tests various local procedure operations.
*/

initialize
(
)	= 
declare
	:tblfld.x	= integer;
	x	= integer;
	a	= varchar( 256 );
	b	= varchar( 256 );
	c	= varchar( 256 );
	str	= varchar( 256 ) not null;
	sub0	= procedure;
	sub1	= procedure returning integer;
	sub11	= procedure returning integer;
	sub_null = procedure;
{
	x	= 0;
	x	= x + x;
	str	= 'x' + 'y';
	a	= 'lpf.a';
	b	= 'lpf.b';
	c	= 'lpf.c';
}

'Select_tf' =
{
	clear field tblfld;
	tblfld = select name, student_id, class 
		 from pba04 where age >= 11
}

'Select_form' =
{	
	clear field tblfld;
	pba04uf1 = select name, student_id, class 
		    from pba04 
		    where age >= 11
	{
		'next' = { next; }
		'end'  = { endloop; }
	}
}

'Pass' =
{
	x = callproc pba04_4p1 ( pba04uf1.all );	
	message 'back in pba04uf1 from pba04_4p1; x = ' + char( x ) 
		with style = popup;
}

'Call sub0' =
{
	callproc 'sub0';
}

'Call sub1' =
{
	message 'a = ' + a + '; b = ' + b + '; c = ' + c with style = popup;
	x = callproc sub1( x = 1 );	
	message 'back in lpf from sub1; x = ' + char( x ) with style = popup;
	message 'a = ' + a + '; b = ' + b + '; c = ' + c with style = popup;
}

'Call sub_null 32K times' =
{
	x = 32768;
	while x > 0 do
		callproc sub_null( x = 0 );
		x = x - 1;
	endwhile;
}

End, key frskey3 =
{
	return
}

procedure sub0 =
declare
	student_id = integer not null;
{
	message 'well, here I am in sub0' with style = popup;
	display submenu
	{
		'name' =
		{
			resume field name;
		}

		field name =
		{
			validate field name;
			message 'in field name in sub0 submenu'
				with style = popup;
			resume next;
		}

		'select_tf' =
		{
			tblfld = select name, student_id, class 
				 from pba04
				 where age > 11
		}

		'select_form' =
		{
			pba04uf1 = select name, student_id, class 
				    from pba04
				    where age > 11
			{
				'next' =
				{
					message 'sub0.student_id = '
						+ char( student_id )
						with style = popup;
					next;
				}
				'end'  = { endloop; }
			}
		}

		'select_form_*' =
		{
			pba04uf1 = select * from pba04
				    where age > 11
			{
				'next' =
				{
					message 'sub0.student_id = '
						+ char( student_id )
						with style = popup;
					next;
				}
				'end'  = { endloop; }
			}
		}

		'end'  = { endloop; }
	};
	message 'leaving sub0 submenu' with style = popup;
}

procedure sub1
(
	x	= integer;
) = 
declare
	b	= varchar( 256 );
	c	= varchar( 256 );
{
	b	= 'sub1.b';
	c	= 'sub1.c';

	message 'entering sub1; x = ' + char( x ) with style = popup;
	message 'a = ' + a + '; b = ' + b + '; c = ' + c with style = popup;
	x = callproc sub11( x = 11 );	
	message 'back in sub1 from sub11; x = ' + char( x ) with style = popup;
	message 'a = ' + a + '; b = ' + b + '; c = ' + c with style = popup;
	return -1;
}

procedure sub11
(
	x	= integer;
) = 
declare
	c	= varchar( 256 );
{
	c	= 'sub11.c';

	message 'entering sub11; x = ' + char( x ) with style = popup;
	message 'a = ' + a + '; b = ' + b + '; c = ' + c with style = popup;
	if x = 0 then
		return 0;
	endif;
	x = callproc sub11( x = 0 );	
	return -11;
}

procedure sub_null
(
	x	= integer;
) = 
{
	return;
}
