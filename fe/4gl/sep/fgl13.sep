/*
** Name:	pba04_4p1.osq -	Test various local procedure operations.
**
** Description:
**	Tests various local procedure operations.
*/

procedure pba04_4p1
(
	name = varchar(16);
	other = varchar(16);
) =
declare
	x	= integer;
	a	= varchar( 256 );
	c	= varchar( 256 );
	lpf	= varchar(16);
	sub1	= procedure returning integer;
	sub11	= procedure returning integer;
{
	lpf	= name;
	x	= 0;
	a	= 'lpp.a';
	c	= 'lpp.c';
	message 'a = ' + a + '; lpf = ' + lpf + '; c = ' + c with style = popup;
	display submenu
	{
		'a' =
		{
			message 'in a' with style = popup;
		}
		'b' =
		{
			message 'in b' with style = popup;
		}
		'end' =
		{
			endloop;
		}
	};
	x = callproc sub1( x = 1 );	
	message 'back in lpp from sub1; x = ' + char( x ) with style = popup;
	message 'a = ' + a + '; lpf = ' + lpf + '; c = ' + c with style = popup;
	return 888;
}

procedure sub1
(
	x	= integer;
) = 
declare
	lpf	= varchar( 256 );
	c	= varchar( 256 );
{
	lpf	= 'sub1.lpf';
	c	= 'sub1.c';

	message 'entering sub1; x = ' + char( x ) with style = popup;
	message 'a = ' + a + '; lpf = ' + lpf + '; c = ' + c with style = popup;
	x = callproc sub11( x = 11 );	
	message 'back in sub1 from sub11; x = ' + char( x ) with style = popup;
	message 'a = ' + a + '; lpf = ' + lpf + '; c = ' + c with style = popup;
	return -1;
}

procedure sub11
(
	x	= integer;
) = 
declare
	c	= varchar( 256 );
{
	c	= 'sub11.c';

	message 'entering sub11; x = ' + char( x ) with style = popup;
	message 'a = ' + a + '; lpf = ' + lpf + '; c = ' + c with style = popup;
	callproc nonexistent;
	message 'a = ' + a + '; lpf = ' + lpf + '; c = ' + c with style = popup;
	return -11;
}
