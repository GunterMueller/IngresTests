? fill pja18uf1.osq 
!!
initialize ( h_field = varchar(20) not null ) =
{
  message 'Testing cursor positioning';
  sleep 2;
}

'Clear' =
{
  clear field all;
}

'Resume' =
{
  resume;
}

'GoToValue1' =
{
  resume field value1;
}

'GoToValue2' =
{
  resume field value2;
}

'GoToResult' =
{
  resume field result;
}

'GoToWhich' =
{
  h_field = prompt 'Enter the name of the field you want to go to' +
    ' ( choices are : value1, value2, result )'
    with style = popup;
  resume field :h_field;
}

'Next' =
{
  resume next;
}

'GotoMenu' =
{
  resume menu;
}

'End' =
{
  return;
}
!!
