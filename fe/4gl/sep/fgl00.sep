initialize (indx=integer) = {

/* Initialize the Global Variables: */
   oia04_var1 = 1;
   oia04_var2.oia04_1_attr1 = 'One';
   oia04_var3.oia04_2_attr1 = 1.1;
   oia04_var3.oia04_2_attr2 = 'ONE';

   oia04_var4 [1].oia04_1_attr1 = 'One';
   oia04_var4 [2].oia04_1_attr1 = 'Two';
   oia04_var4 [3].oia04_1_attr1 = 'Three';
   oia04_var4 [4].oia04_1_attr1 = 'Four';

   oia04_var5 [1].oia04_2_attr1=1.1;
   oia04_var5 [1].oia04_2_attr2='ONE';

   oia04_var5 [2].oia04_2_attr1=2.2;
   oia04_var5 [2].oia04_2_attr2='TWO';

   oia04_var5 [3].oia04_2_attr1=3.3;
   oia04_var5 [3].oia04_2_attr2='THREE';

   oia04_var5 [4].oia04_2_attr1=4.4;
   oia04_var5 [4].oia04_2_attr2='FOUR';

/* Set up the display fields: */
   var1 = oia04_var1;
   var2 = oia04_var2.oia04_1_attr1;
   var3_1 = oia04_var3.oia04_2_attr1;
   var3_2 = oia04_var3.oia04_2_attr2;
   inittable var4;
   indx = 0;
   while indx < 4 do
      insertrow var4[indx] (var4_col1 = oia04_var4[indx+1].oia04_1_attr1);
      indx = indx + 1;
   endwhile;
   inittable var5;
   indx = 0;
   while indx < 4 do
      insertrow var5[indx] (var5_col1 = oia04_var5[indx+1].oia04_2_attr1,
                            var5_col2 = oia04_var5[indx+1].oia04_2_attr2);
      indx = indx + 1;
   endwhile;
}

'End', key frskey3 = {
   clear screen;
   return;
}
!!
