!!
drop procedure pka17_proc0;\p\g
drop procedure pka17_proc1;\p\g
drop procedure pka17_proc2;\p\g
drop procedure pka17_proc3;\p\g

create procedure pka17_proc0 (value1 decimal(10,3) not null) as declare
tot integer not null; begin tot = 0; tot = value1; message :tot 'the
 message number is the value passed to tot'; value1 = 25.5; tot =
value1; return :tot; end
\p\g

create procedure pka17_proc1 (s1 char(20) not null, int1 integer not null,
flt1 float(8) not null, d1 decimal(10,3) not null, s2 char(20) not
null, int2 integer not null, flt2 float(8) not null, d2 decimal(10,3)
not null) as declare iret integer not null; begin iret = 0; s1 = 'by
value'; int1 = 99; flt1 = 99.99; d1 = 99.99; s2 = 'I am pka17_proc1'; int2 =
int2 + 10; flt2 = flt2 + 10.1; d2 = d2 + 10.5; iret = d2; return
:iret; end
\p\g

create procedure pka17_proc2 (s1 char(20) not null, int1 integer not null,
flt1 float(8) not null, d1 decimal(10,3) not null, s2 char(20) not
null, int2 integer not null, flt2 float(8) not null, d2 decimal(10,3)
not null) as declare iret integer not null; begin iret = 0; s1 =
'by value'; int1 = 99; flt1 = 99.99; d1 = 99.99; s2 = 'I am pka17_proc2';
int2 = int2 + 10; flt2 = flt2 + 10.1; d2 = d2 + 10.5; iret = d2;
return :iret; end
\p\g

create procedure pka17_proc3 (s1 char(20) not null, int1 integer not null,
flt1 float(8) not null, d1 decimal(10,3) not null) as begin message
'this is a message from procedure pka17_proc3'; s1 = 'by value'; int1 = int1
+ 99; flt1 = flt1 + 99.99; d1 = d1 + 99.99; end
\p\g\q

