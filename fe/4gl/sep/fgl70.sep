? fill pka17_uf.osq 
!!
/*  Test pka17 - 4GL test of database procedure byref and by value */

initialize (retval = integer not null,
            decvar = decimal(10,3) not null) =
{
      pka17_uf = select str1='hello', ival1=123, fval1=123.22, dec1=123.33,
                 str2='pass byref', ival2=123, fval2=123.22, dec2=123.33;
}

'proc0' =
{
      decvar = 5.5;
      message 'calling db proc pka17_proc0, declared to ABF, using CALLPROC '
            + 'and using a var instead of a form field to pass the value ...'
               with style = popup;
      retval = callproc pka17_proc0 (value1 = byref(:decvar));
      message 'decvar should = 25.5: ' + varchar(:decvar) with style = popup;
      message 'retval should = 25: ' + varchar(:retval) with style = popup;
}

'proc1' =
{
      message 'calling db proc pka17_proc1, declared to ABF, using CALLPROC...';
      sleep 2;
      retval = callproc pka17_proc1 (s1 = :str1, int1 = :ival1, flt1 = :fval1,
               d1 = :dec1, s2 = byref(:str2), int2 = byref(:ival2),
               flt2 = byref(:fval2), d2 = byref(:dec2));
      message 'The procedure should return 133: ' + varchar(:retval)
               with style = popup;
}

'proc2' =
{
      message 'calling db proc pka17_proc2, using EXECUTE PROCEDURE...';
      sleep 2;
      retval = execute procedure pka17_proc2 (s1 = :str1, int1 = :ival1,
               flt1 = :fval1, d1 = :dec1, s2 = byref(:str2),
               int2 = byref(:ival2), flt2 = byref(:fval2), d2 = byref(:dec2));
      message 'The procedure should return 144: ' + varchar(:retval)
               with style = popup;
}

'proc3' =
{
      message 'calling db proc pka17_proc3, using EXECUTE PROCEDURE with '
      + 'EXECUTE IMMEDIATE...' with style = popup;
      execute immediate 'execute procedure pka17_proc3 (s1 = '' + :str1 + '''
      + ', int1 = ' + varchar(:ival1) + ', flt1 = ' + varchar(:fval1)
      + ', d1 = ' + varchar(:dec1) + ')';
      message 'back from the procedure ...';
      sleep 2;
}

'proc_error' =
{
      message 'calling db proc pka17_proc2, using EXECUTE IMMEDIATE '
      + 'with BYREF - this should give an error...' with style = popup;
      execute immediate 'execute procedure pka17_proc2 (value1 = byref('
      + varchar(:ival1) + '))';
      message 'back from the procedure ...';
      sleep 2;
}

'End' =
{
      return;
}

