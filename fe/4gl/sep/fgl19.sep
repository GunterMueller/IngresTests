? fill pha14uf1.osq 
!!
initialize (rangevalue = integer, this_lname = varchar(20),
            this_fname = varchar(20), this_number=integer,
            this_state=integer, rowcount = integer,
            c26uf1_ar = ARRAY OF TYPE OF TABLE FIELD 
                           pha14uf1.c26uf1_tbl) = {
   inittable c26uf1_tbl;
}

'Append' = {
   unloadtable c26uf1_tbl (this_lname=lname, this_fname=fname,
               this_state = _state) 
   {
      if this_state > 0 and this_state < 4 then
         this_number = sequence_value(table_name  = 'pha14_table',
				      column_name = 'pha14_number',
				      increment = 1);
         insert into pha14_table (pha14_number,lname,fname)
                     values (this_number, this_lname, this_fname);
         commit;
      endif;
   };
   inittable c26uf1_tbl;
   redisplay;
}

'Proc' = {
   rowcount = 0;
   unloadtable c26uf1_tbl (this_lname=lname, this_fname=fname,
               this_state = _state) {
      insertrow c26uf1_ar [rowcount]
                (lname=this_lname, fname=this_fname);
      rowcount = rowcount + 1;
   };
   callproc pha14_4p1 (c26uf1_ar = c26uf1_ar, rowcount = rowcount);
   inittable c26uf1_tbl;
   redisplay;
}

'View'= {
   clear screen;
   callframe pha14uf2;
   redisplay;
}

'End', key frskey3 = {
   clear screen;
   return;
}
!!
? fill pha14uf2.osq 
!!
initialize () = {
   inittable c26uf2_table read;
   c26uf2_table = select * from pha14_table
   order by pha14_number;
   }

'Resume' = {
   clear screen;
   return;
   }
!!
? fill pha14_4p1.osq 
!!
procedure pha14_4p1 (rowcount = integer, this_lname = varchar(20),
                      this_fname = varchar(20), this_number = integer,
                      c26uf1_ar = ARRAY OF TYPE OF TABLE FIELD 
                                  pha14uf1.c26uf1_tbl) = {

   unloadtable c26uf1_ar (this_lname=lname, this_fname=fname) {
         this_number = sequence_value(table_name  = 'pha14_table',
				      column_name = 'pha14_number',
				      increment = 2);
         insert into pha14_table (pha14_number,lname,fname)
                     values (this_number, this_lname, this_fname);
         commit;
   };
   return;
   }
!!
