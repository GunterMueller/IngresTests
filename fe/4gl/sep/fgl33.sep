initialize ( h_tempvar = smallint ) =
{
  message 'Testing set_forms/inquire_forms formobject(display)';
  vendorno = 27;
  insertrow partbl[0] (partno=1,descrip='Part One');
  insertrow partbl[1] (partno=2,descrip='Part Two');
  insertrow partbl[2] (partno=3,descrip='Part Three');
  insertrow partbl[3] (partno=4,descrip='Part Four');
  insertrow partbl[4] (partno=5,descrip='Part Five');
}

'Reverse' = 
{
  message 'Testing set_forms/inquire_forms field(reverse)';
  sleep 2;

  inquire_forms field pja19uf1 (h_tempvar=reverse(vendorno));
  h_tempvar = mod ( :h_tempvar + 1, 2 );
  set_forms field pja19uf1 (reverse(vendorno)=  :h_tempvar );
  redisplay;

  message 'Testing set_forms/inquire_forms column(field)';
  sleep 2;

  inquire_forms column pja19uf1 partbl ( h_tempvar = reverse(descrip) );
  h_tempvar = mod ( :h_tempvar + 1, 2 );
  set_forms column pja19uf1 partbl ( reverse(descrip)= :h_tempvar );
  redisplay;
}


