? delobj SEPPARAMDB -application pma06 
<<
~
>>
? delobj SEPPARAMDB -form pma06uf1 
<<
~
>>
? fill pma06uf1.osq 
!!
initialize ( h_rows = smallint, h_result = float, h_count = integer ) =
{
  message 'Testing various aggregate functions and ifnull function...';
  sleep 2;

  /* clear the database table */
  delete from pma06_tbl;
}

'Test1' =
{
  message 'Testing ifnull() when the first parameter is null...';
  sleep 3; 

  h_rows = NULL;
  h_result = ifnull (:h_rows, 100);

  if :h_result =  100 then
    message 'Testing successful';
    sleep 2;
  else
    message 'Testing failed';
    sleep 2;
  endif;

}

'Test2' =
{
  message 'Testing ifnull() when the first parameter is not null...';
    sleep 3; 

  h_rows = 10;
  h_result = ifnull (:h_rows, 100);

  if :h_result !=  100 then
    message 'Testing successful'; 
    sleep 2;
  else
    message 'Testing failed'; 
    sleep 2;
  endif;

}

'Test3' =
{
  message 'Testing ifnull() using an aggregate which is not null...';
    sleep 3; 

  pma06uf1 = select h_result = ifnull (count(*), 100) from pma06_tbl;

  if :h_result !=  100 then
    message 'Testing successful'; 
    sleep 2;
  else
    message 'Testing failed'; 
    sleep 2;
  endif;

}

'End', key frskey3 =
{
  return;
}
