? fill oka00_1.osq 
!!
initialize (oka00ar1 = ARRAY OF TYPE OF TABLE FIELD oka00tf,
            rowstate = integer, counter = integer,
            col1a=char(10), col2a=char(10), col3a=integer) = {
   inittable oka00tf;
   insertrow oka00tf (col1='One',col2='one',col3=1);
   insertrow oka00tf (col1='Two',col2='two',col3=2);
   insertrow oka00tf (col1='Three',col2='three',col3=3);
   insertrow oka00tf (col1='Four',col2='four',col3=4);
   }

'Go' = {
   counter = 0;
   unloadtable oka00tf (rowstate = _state, col1a=col1,
                         col2a=col2, col3a=col3)
      begin
      insertrow oka00ar1[counter] (col1=col1a,col2=col2a,col3=col3a);
      counter = counter + 1;
      end;
   callframe oka00_2(oka00ar2=oka00ar1;totalrows=counter);
   }

'End', key frskey3 = {
   clear screen;
   return;
   }
!!
? fill oka00_2.osq 
!!
initialize (oka00ar2 = ARRAY OF TYPE OF TABLE FIELD oka00_1.oka00tf) = {}

'Go' = {
   oka00ar2[rowno].col1 = newcol1;
   oka00ar2[rowno].col2 = newcol2;
   oka00ar2[rowno].col3 = newcol3;
   callframe oka00_3(oka00ar3 = oka00ar2; totalrows = totalrows);
   }

'End', key frskey3 = {
   clear screen;
   return;
   }
!!
? fill oka00_3.osq 
!!
initialize (oka00ar3 = ARRAY OF TYPE OF TABLE FIELD oka00_1.oka00tf,
            counter = integer, totalrows = integer) = {
   inittable oka00tf;
   counter = 1;
   while counter <= totalrows do
      insertrow oka00tf[counter-1] (col1 = oka00ar3[counter].col1,
                          col2 = oka00ar3[counter].col2,
                          col3 = oka00ar3[counter].col3);
      counter = counter + 1;
   endwhile;
   }

'End', key frskey3 = {
   clear screen;
   return;
   }
!!
