!!
initialize (recno = integer, sstring = varchar(20),
            theform=varchar(15), thetable=varchar(15), 
            thecolumn=varchar(15), thestring=varchar(15)) = {
   inittable pja13_tf1 read;
   inittable pja13_tf2 read;
   inittable pja13_tf3 read;

/* Load the tablefields: */

   insertrow pja13_tf1[0]
             (c27_column1='One', c27_column2='First Column');
   insertrow pja13_tf1[1]
             (c27_column1='Two', c27_column2='Second Column');
   insertrow pja13_tf1[2]
             (c27_column1='Three', c27_column2='Third Column');
   insertrow pja13_tf1[3]
             (c27_column1='Four', c27_column2='Fourth Column');
   insertrow pja13_tf1[4]
             (c27_column1='Five', c27_column2='Fifth Column');

   insertrow pja13_tf2[0]
             (c27_2_col1='Zero',c27_2_col2='Column Zero');
   insertrow pja13_tf2[1]
             (c27_2_col1='One',c27_2_col2='Column One');
   insertrow pja13_tf2[2]
             (c27_2_col1='Two',c27_2_col2='Column Two');
   insertrow pja13_tf2[3]
             (c27_2_col1='Three',c27_2_col2='Column Three');
   insertrow pja13_tf2[4]
             (c27_2_col1='Four',c27_2_col2='Column Four');

   insertrow pja13_tf3[0]
             (c27_3_col1=0,c27_3_col2=0.0,c27_3_col3='zero');
   insertrow pja13_tf3[1]
             (c27_3_col1=1,c27_3_col2=1.1,c27_3_col3='one');
   insertrow pja13_tf3[2]
             (c27_3_col1=2,c27_3_col2=2.2,c27_3_col3='two');

}

/* MenuItems: */

/* Find a row in the current column. This search is case insensitive. */
'GenericFind' = {
   recno = callproc find_record();
   message 'The dataset record number returned is: ' + varchar(recno)
            with style = popup;
}

/* Search in the current column for the literal 'Three'. */
'FindThree' = {
   recno = callproc find_record(value=varchar('Three'));
   message 'The dataset record number returned is: ' + varchar(recno)
            with style = popup;
}

/* Let user specify which tablefield and column, and what searchstring */
/* to find. The search is for an EXACT match, including case.          */
'Search' = {
   callframe pja13uf2(theform=byref(theform); thetable=byref(thetable);
             thecolumn=byref(thecolumn); thestring=byref(thestring));

   recno = callproc find_record(form=theform, table_field=thetable,
                    column=thecolumn, value=thestring);
   message 'The dataset record number returned is: ' + varchar(recno)
            with style = popup;
}

'End', key frskey3 = {
   clear screen;
   return;
}
!!
