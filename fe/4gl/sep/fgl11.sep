? fill oka04_proc1.osq 
!!
procedure oka04_proc1 (tfcol1 = integer, tfcol2 = char(9), dummy = integer, 
		       i = integer, r = integer, s = array of type of table
		       field oka04uf2.tf) =
{ 
   if (r=1) then
      i=1;
      dummy = arrayclear(arry4);
      dummy = arrayclear(s);
      select tfcol1 = m.col1, tfcol2 = m.col2 from oka04_tbl m
      {
        arry4 [i].attr1 = tfcol1;
        arry4 [i].attr2 = tfcol2;
	i = i + 1;
	if (i=9) then 
	    endloop;
	endif;
      };

      i=1;
      unloadtable arry4 
      {
        s[i].col1 = arry4 [i].attr1;
        s[i].col2 = arry4 [i].attr2;
	i = i + 1;
      };

   endif;


   if (r=2) then
      dummy = arrayremoverow(s, rownumber = 8); 
      dummy = arrayremoverow(s, rownumber = 7); 
      dummy = arrayremoverow(s, rownumber = 6); 
   endif;


   if (r=3) then
      dummy = arrayinsertrow(s,rownumber = 2);
      s[2].col1 = 11;
      s[2].col2 = 'Eleven';

      dummy = arrayinsertrow(s,rownumber = 4);
      s[4].col1 = 22;
      s[4].col2 = 'Twentytwo';

      dummy = arrayinsertrow(s,rownumber = 6);
      s[6].col1 = 33;
      s[6].col2 = 'Thirtythr';

      dummy = arrayinsertrow(s,rownumber = 8);
      s[8].col1 = 44;
      s[8].col2 = 'Fourtyfou';

      insertrow s[9] (col1 = 55, col2 = 'Fiftyfive');
   endif;

}
!!
? fill oka04uf1.osq 
!!
initialize (x = integer,
	    y = array of type of table field oka04uf2.tf) =
{

}

'1stCall' = 
{
   callproc oka04_proc1(r=1,s=y);
   callframe oka04uf2(b=y);
}

'2ndCall' =
{
   callproc oka04_proc1(r=2,s=y);
   callframe oka04uf2(b=y);
}


'3rdCall' = 
{
   callproc oka04_proc1(r=3,s=y);
   callframe oka04uf2(b=y);
}

'End' = 
{ 
   return; 
}
!!
? fill oka04uf2.osq 
!!
initialize ( b = array of type of table field oka04uf2.tf, i = integer ) =
{

}

'ReShuffle' = 
{
   clear field tf;
   i = 1;
   unloadtable b
   {
      if (b._state <> 4) then
        insertrow tf (col1 = b[i].col1, col2 = b[i].col2);
      endif;
      i = i + 1;
   };
   scroll tf to 1;
}


'End' = 
{
  return;
}
!!
