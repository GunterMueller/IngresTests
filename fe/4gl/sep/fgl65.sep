? fill pna09uf1.osq 
!!
initialize ( 
h_row = varchar(6), h_partno = varchar(10), h_record = integer4
) =
{
  vendorno = 1;
  insertrow pricetbl[0] (partno=1, vendorno=1, catno='xyz', price=5.99);
  insertrow pricetbl[1] (partno=2, vendorno=2, catno='abd', price=6.99);
  insertrow pricetbl[2] (partno=3, vendorno=3, catno='tuv', price=7.99);
  insertrow pricetbl[3] (partno=4, vendorno=4, catno='mno', price=8.99);
  insertrow pricetbl[3] (partno=5, vendorno=5, catno='qrs', price=9.99);
  insertrow pricetbl[3] (partno=6, vendorno=6, catno='klm', price=10.99);
  insertrow pricetbl[3] (partno=7, vendorno=7, catno='nop', price=11.99);

  resume field pricetbl.partno;
}

'GoTo' =
{
  h_row = prompt 'Enter the row you want to scroll to' with style = popup;
  unloadtable pricetbl ( h_record = _RECORD )
  {
    if h_record = int2(:h_row) then
      scroll pricetbl to :h_record;
    endif;
  }
}

'Find' =
{
  h_partno = prompt 'Enter a partno to find in the dataset :'
    with style = popup;
  unloadtable pricetbl ( h_record = _RECORD )
  {
    if pricetbl.partno = int4(:h_partno) then
      scroll pricetbl to :h_record;
    endif;
  }
}

'Bottom' =
{
  scroll pricetbl to end;
}

'Top' =
{
  scroll pricetbl to 1;
}

'Up' =
{
  scroll pricetbl up;
}

'Down' =
{
  scroll pricetbl down;
}

'End', key frskey3 =
{
  return;
}
!!
