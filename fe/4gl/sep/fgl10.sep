? fill oka03uf1.osq 
!!
initialize (
   total = integer, 
   dummy = integer, 
   tfcol1 = integer, 
   i = integer, 
   r = integer) = 
{
   total = 10000000;
   inquire_forms table oka03uf1 (r=maxrow(tf));
}

'CountRows' =
{
   total = callproc arrayallrows (arry2);
   message 'There are ' + varchar(total) + ' rows in the array' 
      with style = popup;
}

'FillArry' =
{
   i=1;
   dummy = callproc arrayclear(arry2);
   tf := select col1 = m.col1 from oka03_tbl m;

  /* This code must be used until bug 34846 is fixed */
   select tfcol1 = m.col1 from oka03_tbl m
   {
     arry2 [i].attr1 = tfcol1; 
     i = i + 1;
   } ;

   /* When bug 34846 is fixed, try following code */
   /* 
   select arry2 [i].attr1 = m.col1 from oka03_tbl m { i = i + 1; }
   */
}

'InsertRows' =
{
   callproc oka03_proc1;
   clear field tf;

   i = 0;
   unloadtable arry2
   {
     if (arry2._state = 4) then
       insertrow tf[0] (col1 = arry2 [i+1].attr1, col2 = 'Blank');
       deleterow tf[i];
     else
       insertrow tf[i] (col1 = arry2 [i+1].attr1, col2 = 'Blank', 
		 	_state = arry2._state);
       if i < r then
          i = i + 1;
       else
	  endloop;
       endif;
     endif;
   };
   scroll tf to 1;
}

'RemoveRows' =
{
   i = 9;
   while i > 1 do
      dummy = callproc arrayremoverow (arry2, rownumber = i);
      i = i - 3;
   endwhile;

   i=0;
   clear field tf;
   unloadtable arry2
   {
     if (arry2._state = 4) then
        insertrow tf[0] (col1 = arry2 [i+1].attr1, col2 = 'blank');
        deleterow tf[i];
     else
        insertrow tf[i] (col1 = arry2 [i+1].attr1, col2 = 'blank',
      			 _state = arry2._state);
        if i < r then
	   i = i + 1;
	else
	   endloop;
        endif;
     endif;
   };
   scroll tf to 1;
}

'SetToDel' =
{
   total = callproc arrayallrows(arry2);
   clear field tf; 

   i = 0;
   unloadtable arry2
   {
     i = i + 1;
     if (mod(arry2 [i].attr1,2)=0) then
        insertrow tf[0] (col1 = arry2 [i].attr1, col2 = 'BLANK');
     else
/* Note:  Can't use deleterow or arraysetrow deleted here because that
   would reorder the elements in the array. Instead, I'm setting the array
   value to an arbitrary number, 55 */
        arry2 [i].attr1 = 55;
     endif;
   };

   i = 1;
   while (i = i) do
      if (arry2 [i].attr1 = 55) then
 	 dummy = callproc arraysetrowdeleted (arry2, rownumber=i);
	 i = 1;
	 total = callproc arraylastrow(arry2);
      endif;
      if (i = total) then 
         endloop;
      endif;
      i = i + 1;
   endwhile;

   scroll tf to 1;
}

'Last-First' =
{
total = arraylastrow(arry2) - arrayfirstrow(arry2) + 1;
message 'Total number of rows ' + varchar(total) with style = popup;
}

'ClearOneRow' =
{
   total = arraylastrow(arry2);
   clearrow arry2 [total];

   clear field tf;

   i = 1;
   unloadtable arry2
   {
     if (arry2._state <> 4) then
         insertrow tf[0] (col1 = arry2 [i].attr1, col2 = 'clear');
      endif;
     i = i + 1;
   };
}

'ClearAll' = 
{
  dummy = callproc arrayclear(arry2);
  clear field tf;
  i = 1;
  unloadtable arry2
  {
    if (arry2._state <> 4) then
       insertrow tf[0] (col1 = arry2 [i].attr1, col2 = 'clrall');
    endif;
    i = i + 1;
  };
}

'End' = { return; }
!!
? fill oka03_proc1.osq 
!!

procedure oka03_proc1 (c=integer, z=integer) =
{
   z = 2;
   while z < 12 do
      c=callproc arrayinsertrow(arry2, rownumber=z);
      arry2 [z].attr1 = z;
      z = z + 2;
   endwhile;
}
