? fill pha23uf1.osq 
!!
initialize (h_err = smallint ) =
{
  message 'Enter retrieval parameters and select Query menuitem'  +
    ' or add a new vendor and select AddVendor menuitem.';
  sleep 3;
  mode query;
}

'Query' =
{
  commit;
  set lockmode on pha23_tbl where readlock = exclusive;
  pha23uf1 = select  * from pha23_tbl
    where qualification ( vendorno = :vendorno, vname = :vname, 
    street = :street, city = :city, state = :state, zip = :zip, 
    contact = :contact )
    order by vendorno
    {
      initialize =
      {
        set_forms form( mode = 'update' );
      }
      'Next' =
      {
        next;
      }

      'Update' =
      {
        message 'Testing REPEATED UPDATE...' +
          '<Update based on vendorno only>';
        sleep 2;
        repeated update pha23_tbl 
        set vendorno = :vendorno,  vname = :vname,  street = :street,  
        city = :city,  state = :state,  zip = :zip,  contact = :contact
        where vendorno = :vendorno;
        inquire_ingres ( h_err = errorno );
        if h_err > 0 then
          rollback;
          message 'Operation failed.';
          sleep 2;
          endloop;
        else
          commit;
          message 'Emp # ' + varchar(vendorno) + ' updated';
          sleep 2;
        endif;
       
      }
      'Delete' = 
      { 
        message 'Testing REPEATED DELETE...';
        sleep 2;
        repeated delete from  pha23_tbl
          where vendorno = :vendorno 
          and  vname = :vname 
          and  street = :street 
          and  city = :city 
          and  state = :state 
          and  zip = :zip 
          and  contact = :contact; 
        inquire_ingres ( h_err = errorno );
        if h_err > 0 then
          rollback;
          message 'Operation failed.';
          sleep 2;
          endloop; 
        else
          commit;
          message 'Emp #' + varchar(vendorno) + ' deleted.';
          sleep 2;
        endif;
        next;
      }

      'End', key frskey3 =
      {
        endloop;
      }
    }; /* End of submenu */
    commit;
    clear field all;
    resume field vendorno;
} /* end of menu item */

'AddVendor' =
{

  insert into pha23_tbl
  values (:vendorno, :vname, :street, :city, :state, :zip, :contact);
  commit;

  message 'Adding vendor # ' + varchar(vendorno);
  sleep 1;
  clear field all;
  resume field vendorno;
}


'End', key frskey3 =
{
  return;
}
!!
