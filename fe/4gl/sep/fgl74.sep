/*  4GL test pja28 - FRS enhancements (RENAME/ACTIVE/EXISTS/RESUME).
**  Tests renaming menuitems; tests enabling and disabling menuitems; 
**  determines the existence of a form, field, and column; and tests RESUME 
**  nextfield and previousfield.
*/
initialize(
	ivar1 = integer not null,
	ivar2 = integer not null) =
{
SET_FORMS MENU pja28_f1 (ACTIVE('OFF_menu') = OFF);
SET_FORMS MENU pja28_f1 (ACTIVE('0_menu') = 0);
}

'Rename' = 
{
message 'renaming menuitem Rename to Newname...'; sleep 2;
SET_FORMS MENU pja28_f1 (RENAME('Rename') = 'Newname');
}

'ON_menu' =
{
SET_FORMS MENU pja28_f1 (ACTIVE('OFF_menu') = ON);
INQUIRE_FORMS MENU pja28_f1 (ivar1 = ACTIVE('OFF_menu'));
message 'Enabling menuitem OFF_menu, 1 =  ' + varchar(:ivar1)
with style = popup;
}

'OFF_menu' =
{
/* turns itself off */
SET_FORMS MENU pja28_f1 (ACTIVE('OFF_menu') = OFF);
INQUIRE_FORMS MENU pja28_f1 (ivar1 = ACTIVE('OFF_menu'));
message 'Disabling menuitem OFF_menu, 0 =  ' + varchar(:ivar1)
with style = popup;
}

'1_menu' =
{
SET_FORMS MENU '' (ACTIVE('0_menu') = 1);
INQUIRE_FORMS MENU '' (ivar1 = ACTIVE('0_menu'));
message 'Enabling menuitem 0_menu, 1 =  ' + varchar(:ivar1)
with style = popup;
}

'0_menu' =
{
/* turns itself off */
SET_FORMS MENU pja28_f1 (ACTIVE('0_menu') = 0);
INQUIRE_FORMS MENU pja28_f1 (ivar1 = ACTIVE('0_menu'));
message 'Disabling menuitem 0_menu, 0 =  ' + varchar(:ivar1)
with style = popup;
}

'Var_menu' =
{
ivar2 = 0;
SET_FORMS MENU '' (ACTIVE('Newname') = ivar2);
INQUIRE_FORMS MENU '' (ivar1 = ACTIVE('Newname'));
message 'Disabling menuitem Newname, 0 =  ' + varchar(:ivar1)
with style = popup;
}

'Invalid_var_menu' =
{
/* remains unchanged from last valid setting */
ivar2 = -5;
SET_FORMS MENU '' (ACTIVE('1_menu') = ivar2);
INQUIRE_FORMS MENU '' (ivar1 = ACTIVE('1_menu'));
message 'Menuitem 1_menu is still enabled, 1 =  ' + varchar(:ivar1)
with style = popup;
}

'Exists' =
{
ivar1 = 0;
/* test existence of the form */
INQUIRE_FORMS FORM (ivar1 = EXISTS(pja28_f1));
message 'The form exists so 1 is returned: ' + varchar(:ivar1)
with style = popup;

/* test existence of a field on the form */
INQUIRE_FORMS FIELD '' (ivar1 = EXISTS(charcol));
message 'The field charcol exists so 1 is returned: ' + varchar(:ivar1)
with style = popup;

/* test existence of a field that is NOT on the form */
INQUIRE_FORMS FIELD '' (ivar1 = EXISTS(dummyfield));
message 'The field dummyfield does not exist so 0 is returned: '
+ varchar(:ivar1) with style = popup;

/* test existence of a column in the tablefield */
INQUIRE_FORMS COLUMN '' pja28tbl (ivar1 = EXISTS(intcol));
message 'The column intcol exists so 1 is returned: ' 
+ varchar(:ivar1) with style = popup;

/* test existence of a column that is NOT in the tablefield */
INQUIRE_FORMS COLUMN '' pja28tbl (ivar1 = EXISTS(dummycolumn));
message 'The column dummycolumn does not exist so 0 is returned: ' 
+ varchar(:ivar1) with style = popup;
}

'next' =
{
resume nextfield;      
}

'previous' =
{
resume previousfield; 
}

'End' =
{
	return;
}

