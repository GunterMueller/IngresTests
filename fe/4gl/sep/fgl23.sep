!!
initialize (h_err = smallint ) =
{
  message 'Enter retrieval parameters and select Query menuitem'  +
    ' or add a new vendor and select AddVendor menuitem.';
  sleep 3;
  mode query;
}

'Query' =
{
  commit;
  set lockmode on pha25_tbl where readlock = exclusive;
  pha25uf1 = 
    select  vendorno, vname, street, city, state, zip, contact  from pha25_tbl
    where qualification ( vendorno = :vendorno, vname = :vname, 
      street = :street, city = :city, state = :state, zip = :zip, 
      contact = :contact )
    order by   vendorno
    {
      initialize =
      {
        set_forms form( mode = 'query' );
      }
      'Update' =
      {
        message 'Testing UPDATE with WITH QUALIFICATION clause...';
        sleep 2;
        update pha25_tbl
          set vendorno = :newvendorno,  vname = :newvname,  
            street = :newstreet, city = :newcity,  state = :newstate,  
            zip = :newzip,  contact = :newcontact
          where qualification ( vendorno = :vendorno, vname = :vname, 
            street = :street, city = :city, state = :state, zip = :zip, 
            contact = :contact );
        message 'Updating emp ' + varchar(vendorno);
        sleep 2;
        inquire_ingres ( h_err = errorno );
        if h_err > 0 then
          rollback;
          message 'Operation failed.';
          sleep 2;
          endloop;
        endif;
      }

      'Delete' = 
      { 
        message 'Testing DELETE with WITH QUALIFICATION clause...';
        sleep 2;
        delete from  pha25_tbl
          where qualification ( vendorno = :vendorno, vname = :vname, 
          street = :street, city = :city, state = :state, zip = :zip, 
          contact = :contact );
        message 'Deleting emp ' + varchar(vendorno);
        sleep 2; 
        inquire_ingres ( h_err = errorno );
        if h_err > 0 then
          rollback;
          message 'Operation failed.';
          sleep 2;
          endloop; 
        endif;
        commit;
        next;
      }
      'Next' =
      {
        next;
      }
      'Clear'=
      {
        clear field all;
      }
      'End', key frskey3 =
      {
        endloop;
      }
    }; /* End of submenu */

    commit;
    clear field all;
    resume field vendorno;
} /* end of menu item */


'AddVendor' =
{

  insert into pha25_tbl
  values (:vendorno, :vname, :street, :city, :state, :zip, :contact);
  commit;

  message 'Adding vendor # ' + varchar(vendorno);
  sleep 1;
  clear field all;
  resume field vendorno;
}


'End', key frskey3 =
{
  return;
}
!!
