/*  pha29 - 4GL test of miscellaneous dbms statements
**  Tests SET, Session-scope tables, file extend, comments
**
**  NOTE: This test includes CREATE TABLE statements as part of the test
*/

initialize (long_remark1 = char(500) not null,
            long_remark2 = char(2000) not null,
            int_value = integer not null) =
{
long_remark1 = 'this is a long remark var that has < 500 characters. ';
long_remark2 = 'A long remark has a maximum size limit of 1600 characters, '
+ 'but this var holds more than the maximum and should give a runtime error '
+ 'This document contains unpublished information that is proprietary to '
+ 'Ingres.  It is supplied for identification, maintenance, engineering, '
+ 'and inspection purposes only, and shall not be duplicated or disclosed '
+ 'without prior written permission from an authorized representative of '
+ 'Ingres.  This document and any other confidential information shall not '
+ 'be released to any third party without a valid Confidential Information '
+ 'Agreement or Confidential Information Exchange Agreement signed by the '
+ 'third party and an authorized Ingres representative.  In accepting this '
+ 'document, the recipient agrees to make every reasonable effort to prevent '
+ 'the unauthorized use of this information. '
+ 'This is the begining of more of the same text so that the max is exceeded. '
+ 'A long remark has a maximum size limit of 1600 characters, '
+ 'but this var holds more than the maximum and should give a runtime error '
+ 'This document contains unpublished information that is proprietary to '
+ 'Ingres.  It is supplied for identification, maintenance, engineering, '
+ 'and inspection purposes only, and shall not be duplicated or disclosed '
+ 'without prior written permission from an authorized representative of '
+ 'Ingres.  This document and any other confidential information shall not '
+ 'be released to any third party without a valid Confidential Information '
+ 'Agreement or Confidential Information Exchange Agreement signed by the '
+ 'third party and an authorized Ingres representative.  In accepting this '
+ 'document, the recipient agrees to make every reasonable effort to prevent '
+ 'the unauthorized use of this information. '
+ 'This is the begining of more of the same text so that the max is exceeded. '
+ 'This is the begining of more of the same text so that the max is exceeded. '
+ 'This is the begining of more of the same text so that the max is exceeded. '
+ 'This is the begining of more of the same text so that the max is exceeded.';

int_value = 2;
}

'SET' =
{
/* New SET statement */
MESSAGE 'testing SET statements' with style = popup; 
SET SESSION WITH ON_ERROR = ROLLBACK STATEMENT;
SET SESSION WITH ON_ERROR = ROLLBACK TRANSACTION;
commit;
}

'TempTbl' =
{
/* Creation of Session-scope Tables */
MESSAGE 'testing global temp table, creating pha29_gtt' with style = popup;
DECLARE GLOBAL TEMPORARY TABLE SESSION.pha29_gtt
     (col1 varchar(20), col2 float(3), col3 decimal)
     ON COMMIT PRESERVE ROWS
     WITH NORECOVERY;

modify session.pha29_gtt to btree on col1;
commit;
}

'Extend/Allocation' =
{
/* File Extend Enhancements */
MESSAGE 'testing create table with allocation' with style = popup;
CREATE TABLE pha29_tbl2 (col1, col2, col3) AS SELECT * FROM pha29_tbl1 
	WITH ALLOCATION = 50;

MESSAGE 'testing create table with extend' with style = popup;
CREATE TABLE pha29_tbl3 (col1, col2, col3) AS SELECT * FROM pha29_tbl1
	WITH EXTEND = 10;
commit;

MESSAGE 'testing modify table with allocation' with style = popup;
MODIFY pha29_tbl2 TO btree ON col1 WITH ALLOCATION = 50;

MESSAGE 'testing modify table with extend' with style = popup;
MODIFY pha29_tbl3 TO hash ON col1 WITH EXTEND = 10;
commit;

MESSAGE 'testing create index with allocation' with style = popup;
CREATE INDEX index1 ON pha29_tbl2 (col1, col2) WITH ALLOCATION = 50;

MESSAGE 'testing create index with extend' with style = popup; 
CREATE INDEX index2 ON pha29_tbl3 (col1, col2) WITH EXTEND = 10;
commit;
}

'Comments' =
{
/* Table and Column Comments */
MESSAGE 'testing comment on table' with style = popup; 
COMMENT ON TABLE pha29_tbl2 IS :long_remark1;                             
COMMENT ON TABLE pha29_tbl3 IS 'This is a long remark that is quite short'; 

MESSAGE 'deleting comment on table' with style = popup; 
COMMENT ON TABLE pha29_tbl2 IS '';

MESSAGE 'testing comment on column' with style = popup; 
COMMENT ON COLUMN pha29_tbl2.col3 IS :long_remark1;  
COMMENT ON COLUMN pha29_tbl2.col1 IS 'this is a column comment';

/*  --- expect a runtime error for next statement since the long remark
**  --- is greater than the max of 1600 characters
*/
COMMENT ON COLUMN pha29_tbl2.col1 IS :long_remark2;   
}

'End' =
{
	return;
}
