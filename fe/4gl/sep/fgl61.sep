? fill pma11uf1.osq 
!!
/* using strform */
initialize ( h_positions = varchar(3) )  =
{
  clear screen;
  message 'Enter values for string 1 and string 2 and select operation.';
  sleep 2;
}

'Clear' =
{
  clear field all;
  resume field string1;
}

'Concat' =
{
  message 'Testing concat() using string1 and string2...'; sleep 2;
  result = concat (:string1, :string2);
}

'Hex' =
{
  message 'Testing hex() using string1 and string2...'; sleep 2;
  result = hex(:string1) + '/' + hex(:string2);
}

'UptoFirstA' =
{
  message 'Testing left() & locate() using string1...'; 
  sleep 2;
  if :string1 is null   then
    message 'String 1 field cannot be null to test...';
    sleep 2;
  else
    result = left(:string1, locate(:string1,'a') );
  endif;
}

'AfterFirstA' =
{
  message 'Testing right(),locate(), & len()  using string1...'; 
  sleep 2;
  if :string1 is null   then
    message 'String 1 field cannot be null to test...';
    sleep 2;
  else
    result = right(:string1, length(:string1) - locate(:string1,'a') );
  endif;
}

'ShiftPos' =
{
  message 'Testing shift() using string1...'; sleep 2;
  h_positions = prompt 'Enter # of positions to shift: ' +
    'Positive # shifts to the right and negative # shifts to left.'
    with style = popup;
  result = shift(:string1, int2(:h_positions) );
}

'LowerCase' =
{
  message 'Testing lowercase() using string1...';  sleep 2;
  string1 = lowercase(:string1);
  string2 = lowercase(:string2);
  result = lowercase(:result);
}

'UpperCase' =
{
  message 'Testing uppercase() using string1...';  sleep 2;
  string1 = uppercase(:string1);
  string2 = uppercase(:string2);
  result = uppercase(:result);
}

'End', key frskey3 =
{
  return;
}
