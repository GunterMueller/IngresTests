!!
initialize (h_file = varchar(30)) =
{
  message 'Testing printscreen statement...';
  sleep 2;
}

'SendToFile' =
{
  h_file = prompt 'Name the file : ' with style = popup;
  printscreen ( file = h_file );
}

'End', key frskey3 =
{
  return;
}
