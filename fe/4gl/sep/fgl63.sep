!!
initialize ( h_state = smallint, h_row = varchar(6), 
h_saved = smallint, h_changed = smallint, h_prompt = varchar(1),
h_choice = varchar(6), h_selected = varchar(1)  ) =
{
  message 'Enter a Vendor Number and then select QUERY ITEM';
  sleep 3;

  h_selected = 'n';
  h_changed = 0; /* flag to indicate that there were changes to the table */
  h_saved = 0;  /* flag to indicate whether the changes have been saved or not */

  while h_selected != 'y' do
    h_choice = prompt 'Enter desired mode :r(ead), u(update), f(ill)'
      with style = popup;
    if h_choice like 'r%' then
      inittable pricetbl read;
      message 'User cannot modify or add rows on the table...';
      sleep 2;
      h_selected = 'y';
    elseif h_choice like 'u%' then
      inittable pricetbl update;
      message 'User can modify but NOT add rows on the table...';
      sleep 2;
      h_selected = 'y';
    elseif h_choice like 'f%' then
      inittable pricetbl fill;
      message 'User can modify or add rows on the table...';
      sleep 2;
      h_selected = 'y';
    endif;
  endwhile;
}

'Query' =
{
  commit;
  set lockmode on pna07_price where readlock = exclusive;
  pricetbl = select partno, vendorno, catno, price from pna07_price
            where vendorno = :vendorno;
  commit;
  resume field pricetbl.partno;
}

'Insert'=
{
  insertrow pricetbl ( vendorno = :vendorno );
  h_changed = 1;
}

'DeleteRow' =
{
  deleterow pricetbl;
  h_changed = 1;
}

'ClearRow' =
{
  clearrow pricetbl;
  h_changed = 1;
}

'InsertAfter' =
{
  h_row = prompt 'Enter the row you want to insert the new line after: '
    with style = popup;
  insertrow pricetbl[int2(h_row)];
  h_changed = 1;
}

'DeleteRow#' =
{
  h_row = prompt 'Enter the row you want to insert the new line after: '
    with style = popup;
  deleterow pricetbl[int2(h_row)];
  h_changed = 1;
}

'IncPrice' =
{
  h_changed = 1;
  unloadtable pricetbl 
  {
    pricetbl.price = :pricetbl.price * 1.5;
  }
  
}

'ClearPrice' =
{
  h_row = prompt 'Enter the row you want to clear the price: ' 
    with style = popup;
  clearrow pricetbl[int2(h_row)] (price);
  h_changed = 1;
}

'Save' =
{
  h_saved = 1;
  unloadtable pricetbl ( h_state = _state )
  {
    if h_state = 2 then
      message 'Row unchanged';
      sleep 2;
    elseif h_state = 1 then
      repeated insert into pna07_price(partno, vendorno, catno, price )
        values (:pricetbl.partno, :vendorno, :pricetbl.catno, :pricetbl.price );
      message 'Row added';
    elseif h_state = 3 then
      repeated update pna07_price
        set price = :pricetbl.price,
        catno = :pricetbl.catno
        where vendorno = :pricetbl.vendorno
        and partno = :pricetbl.partno;
      message 'Row changed';
    elseif h_state = 4 then
      repeated delete from pna07_price 
        where pna07_price.partno = :pricetbl.partno
        and pna07_price.vendorno = :pricetbl.vendorno;
      message 'Row deleted';
    endif;
  };
  rollback;
  message 'Transaction rolled back to restore prev condition...';
  clear field all;
  resume field pricetbl.partno;

}

'End', key frskey3 =
{
  if h_changed = 1 and h_saved = 0 then
    h_prompt = 
      prompt 'Changes have not been saved. Do you want to end anyhow (y/n) ?';
    if h_prompt = 'y' then
      return;
    endif;
  else
    return;
  endif;
}
