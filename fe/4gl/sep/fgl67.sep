? fill paa05_uf.osq 
!!
initialize (
              sret = char(25) not null,
              strpassed = char(200) not null
           ) =
{
}

'Call_C' =
   {
     /*  str1, str2, str3 are all varchar fields internally     */

     sret = callproc paa05_c(str1, ival1, fval1, dec1, byref(str2), byref(str3),
                     byref(ival2), byref(fval2), byref(dec2), byref(strpassed));

     message 'The procedure received: ' + strpassed with style = popup;
     message 'The procedure returned: ' + sret with style = popup;
   }

'End' =
   {
     return;
   }

!!
? fill paa05_c.sc 
!!
char *  paa05_c (s1, int1, flt1, d1, s2, s3, int2, flt2, d2, passed)
char    *s1;
char    *s2;
char    *s3;
int     int1;
int     *int2;
double  flt1;
double  *flt2;
double  d1;
double  *d2;
char    *passed;
{
        static char    sret[25];

        sprintf(passed,"%s %d %lf %lf %s %s %d %lf %lf",s1,int1,flt1,d1,s2,
                s3,*int2,*flt2,*d2);

        strcpy (s1, "By Value");
        strcpy (s2, "Hello There");
        strcpy (s3, "Hi");
        int1 = int1 + 99;
        *int2 += 10;
        flt1 += 99.99;
        *flt2 = *flt2 + 10.1;
        d1 += 99.99;
        *d2 += 10.5;
        strcpy (sret, "I am a c string");

	return (sret);
}
!!
