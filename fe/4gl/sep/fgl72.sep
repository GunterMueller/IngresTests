? fill pia04_f1.osq 
!!
/*  pia04 - 4GL test of float precision and decimal
**  NOTE: this test tests CREATE TABLE with float(n) and decimal columns.
*/

initialize (
            fvar1 = float(5) not null,
            fvar2 = float(7) not null,
            dvar1 = decimal(15,4) not null,
            dvar2 = dec(31) not null,            
            dvar3 = decimal(10,0) not null,
            dvar4 = numeric,
	    dvar5 = decimal(5,2) not null,
            dectbl.hcol1 = decimal not null,                             
            dectbl.hcol2 = dec(1) not null,
	    dec_array = array of type of table pia04_tbl1,
            dec_rec = type of table pia04_tbl2,
	    ret = decimal(12,3) not null
            ) = 
	    declare fvar3 = float(3) not null;
{
/*   Test creating tables using float(n) precision and decimal   */
        message 'creating table ftemp...'; sleep 2; 
        create table ftemp (fcol1 float(5), fcol2 float(7), fcol3 float(10),
			    dcol1 decimal(10,2), dcol2 numeric, dcol3 dec(1,0));
        message 'dropping table ftemp...'; sleep 2; 
        drop table ftemp;

        fvar1 = 10.7;
        fvar2 = 888888.88;
	fvar3 = 999999.99;
        dvar1 = 1234567890.12;
        dvar2 = 1234567890123456789012345678901;
        dvar3 = 1234567890;
        dvar4 = 1234;
	dvar5 = 5.1;
	dec_array[1].deccol2 = 100.05;
	dec_rec.d1 = 111.222;
        pia04_f1 = select deccol1 = intcol + :dvar5, 
	  		  deccol2 = :dvar4,   
	 	          intcol = deccol2 + :dvar3,   
		          charcol from pia04_tbl1;   
}

'leading_0' =
{
/* Test leading zeroes */
        dvar4 = dvar4 + 000001;
        message 'dvar4 = 1235: ' + varchar(:dvar4) 
	with style = popup;
}

'assign' =
{
/* Test decimal variable on left-hand side (LHS) of an assignment statement */
        select :dvar1 = deccol1 from pia04_tbl1;
        message 'dvar1 = 900000000.0010: ' + varchar(:dvar1) 
	with style = popup;
}

'unload' =
{
/* Test decimal variable on left-hand side of an assignemt in target list of 
** an unloadtable
*/
        dectbl = select d1, d2 from pia04_tbl2;
        dvar1 = 0.0;
        unloadtable dectbl
                {
                        dvar1 = dvar1 + dectbl.d1;
                };
        message 'dvar1 = 56.6660: ' + varchar(:dvar1) 
	with style = popup;
}

'call_frame' =
{
	ret = callframe pia04_f2 (decvar1 = deccol1, decvar2 = byref(deccol2)); 
        message 'returned value = 55.500: ' + varchar(:ret)
	with style = popup;
}

'End' =
{
        return;
}
!!
? fill pia04_f2.osq 
!!
initialize( 
	decvar1 = decimal(10,3) not null,
	decvar2 = decimal(10,3),
	ret = decimal(10,3) not null
	) =
{
	message 'decvar1 = 14.100: ' + varchar(:decvar1)
	with style = popup;
	message 'decvar2 = 1234.000: ' + varchar(:decvar2)
	with style = popup;

	decvar1 = 55.5;
        decvar2 = decvar2 + 1.1;    
        ret = decvar1;
	return ret;
}
 
'End' = 
{
	return ret;    
}
!!
