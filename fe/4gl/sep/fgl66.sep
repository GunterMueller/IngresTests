? fill pna10uf1.osq 
!!
initialize ( h_row = varchar(6) ) =
{
  insertrow pricetbl[0] (partno=1,vendorno=1,catno='abc',price=9.95);
  insertrow pricetbl[0] (partno=2,vendorno=2,catno='def',price=8.95);
  insertrow pricetbl[0] (partno=3,vendorno=3,catno='ghi',price=7.95);
  insertrow pricetbl[0] (partno=0,vendorno=0,catno='jkl',price=10001);

  resume field pricetbl.partno;
}

'ValidAll' =
{
  message 'Validating the entire table...';
  sleep 2;
  validate field  pricetbl;
}

'ValidCurrentRow' =
{
  message 'Validating current row...';
  sleep 2;
  validrow pricetbl;
}

'ValidRow#' =
{
  h_row = prompt 'Enter the row you want to validate :'
    with style = popup;
  message 'Validating row # ' + h_row + '...';
  sleep 2;
  validrow pricetbl[int2(h_row)];
}

'ValidPriceCol' =
{
  message 'Validating the price column...';
  sleep 2;
  validrow pricetbl ( price );
}


'End', key frskey3 =
{
  return;
}
