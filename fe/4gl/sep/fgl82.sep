#include <stdio.h>

#define ISARRAY		1
#define NOTARRAY 	0
#define CHARLEN		25
#define STRLEN		600

    EXEC SQL BEGIN DECLARE SECTION;
	static int obj;

	static char   	cvar[CHARLEN];
	static char   	charvar[CHARLEN];
	static char   	textvar[CHARLEN];
	static char   	varvar[CHARLEN];

	static double 	decvar;
	static double 	decivar;
	static double 	numvar;
	static double 	f4var;
	static double 	f8var;
	static double 	floatvar;
	static int 	intvar;
	static int 	smallvar;

	static double	moneyvar;

	static char  	datevar[CHARLEN+10];

   	static char  	str[STRLEN];
   EXEC SQL END DECLARE SECTION;

   EXEC SQL INCLUDE SQLCA;

   EXEC SQL WHENEVER SQLERROR STOP;


int
arrays(intarg1, strarg1, farg1)
int     *intarg1;
char    *strarg1;
double  *farg1;
{
        /* dummy proc that we declare to ABF as a non-library */
        return *intarg1;
}

void
cleararr(arr)
EXEC SQL BEGIN DECLARE SECTION;
	int     arr;
EXEC SQL END DECLARE SECTION;
{
    EXEC 4GL CLEAR ARRAY :arr;

    inq4gl(arr, ISARRAY);
}

void
getrw(arr, rownum)
EXEC SQL BEGIN DECLARE SECTION;
	int     arr;
	int     rownum;
EXEC SQL END DECLARE SECTION;
{
    EXEC SQL BEGIN DECLARE SECTION;
   	static char  	str[STRLEN];
    EXEC SQL END DECLARE SECTION;

    EXEC 4GL GETROW :arr :rownum (:obj = ROW);

    inq4gl(arr, ISARRAY);

    EXEC 4GL GET ATTRIBUTE :obj 
	(:decvar = deccol1, :decivar = deccol2, :numvar = deccol3,
	 :f4var = f4col, :f8var = f8col, :floatvar = floatcol, 
	 :intvar = intcol, :smallvar = smallcol, :cvar = ccol, 
	 :charvar = charcol, :textvar = textcol, :varvar = varcol, 
	 :moneyvar = moneycol, :datevar = datecol); 

    inq4gl(obj, NOTARRAY);

    sprintf(str,"GET ATTRIBUTE (array [%d]): %s %s %s %s %lf %lf %lf %lf %lf %lf %d -
%d %lf %s", rownum, cvar, charvar, varvar, textvar, decvar, decivar, numvar, floatva-
r, f4var, f8var, intvar, smallvar, moneyvar, datevar);

    EXEC 4GL CALLPROC viewstr(string = :str);
}

void
insertrw(arr, rownum)
EXEC SQL BEGIN DECLARE SECTION;
	int     arr;
	int     rownum;
EXEC SQL END DECLARE SECTION;
{
    EXEC 4GL INSERTROW :arr (ROWNUMBER = :rownum);

    inq4gl(arr, ISARRAY);
}

void
removerw(arr, rownum)
EXEC SQL BEGIN DECLARE SECTION;
	int     arr;
	int     rownum;
EXEC SQL END DECLARE SECTION;
{
    EXEC 4GL REMOVEROW :arr (ROWNUMBER = :rownum);

    inq4gl(arr, ISARRAY);
}

void
setrwdel(arr, rownum)
EXEC SQL BEGIN DECLARE SECTION;
	int     arr;
	int     rownum;
EXEC SQL END DECLARE SECTION;
{
    EXEC 4GL SETROW DELETED :arr (ROWNUMBER = :rownum);

    inq4gl(arr, ISARRAY);
}
