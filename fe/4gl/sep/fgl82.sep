? fill inqset.sc 
!!
#include <stdio.h>

#define ISARRAY		1
#define NOTARRAY 	0
#define STRLEN		600
#define CHARLEN		25
#define INTLEN		10
#define ERRLEN		100

   EXEC SQL BEGIN DECLARE SECTION;
	static int 	errno;
	static char  	errtxt[ERRLEN];
	static int  	allr;
	static int  	firstr;
	static int  	lastr;
	static int  	isarr;
	static char  	rectype[CHARLEN];

   	static char  	str[STRLEN];
   EXEC SQL END DECLARE SECTION;

   EXEC SQL INCLUDE SQLCA;

   EXEC SQL WHENEVER SQLERROR STOP;

int
inqset(intarr1)
int     *intarr1;
{
        /* dummy proc that we declare to ABF as a non-library */
        return *intarr1;
}

void
inq4gl(obj, type)
EXEC SQL BEGIN DECLARE SECTION;
	int     obj;
	int	type;
EXEC SQL END DECLARE SECTION;
{
    EXEC 4GL INQUIRE_4GL (:errno = errorno);

    EXEC 4GL INQUIRE_4GL (:errtxt = errortext);

    EXEC 4GL INQUIRE_4GL (:isarr = IsArray(:obj));

    EXEC 4GL INQUIRE_4GL (:rectype = RecordTypename(:obj));

 if (type == ISARRAY) {
    EXEC 4GL INQUIRE_4GL (:allr = allrows(:obj));

    EXEC 4GL INQUIRE_4GL (:firstr = firstrow(:obj));

    EXEC 4GL INQUIRE_4GL (:lastr = lastrow(:obj));

    sprintf(str, "INQUIRE_4GL: errorno = %d, errortext = %s, IsArray = %d, RecordTyp-
ename = %s, allrows = %d, firstrow = %d, lastrow = %d", errno, errtxt, isarr, rectyp-
e, allr, firstr, lastr);
 }
 else
    sprintf(str, "INQUIRE_4GL: errorno = %d, errortext = %s, IsArray = %d, RecordTyp-
ename = %s", errno, errtxt, isarr, rectype);

    EXEC 4GL CALLPROC viewstr (string = :str);
}

void
set4gl(messtype)
EXEC SQL BEGIN DECLARE SECTION;
	int     messtype;
EXEC SQL END DECLARE SECTION;
{
    EXEC 4GL SET_4GL (messages = :messtype);
}
