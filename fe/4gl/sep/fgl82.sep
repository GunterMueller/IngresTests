? fill e4gluf4.osq 
!!
initialize (myarr = array of type of table tblarr) = 
	declare
		initarr = procedure,
		viewarr = procedure, {
}

'Array Ops' = {
	callproc initarr;
	callproc viewarr;
	display submenu {
		'Getrow' = {
			callproc getrw(myarr, :rownum);
			callproc viewarr;
		}

		'ArrayInsertRow' = {
			callproc arrayinsertrow(myarr, rownumber = :rownum);
			callproc viewarr;
		}

		'Insertrow' = {
			callproc insertrw(myarr, :rownum);
			callproc viewarr;
		}

		'Removerow' = {
			callproc removerw(myarr, :rownum);
			callproc viewarr;
		}

		'Setrowdelete' = {
			callproc setrwdel(myarr, :rownum);
			callproc viewarr;
		}

   	 	'Cleararray' = {
			callproc cleararr (myarr);
			callproc viewarr;
    		}  

		'Initarray' = {
			callproc initarr;
    		}  

		'Viewarray' = {
			callproc viewarr;
		}

		/* EXEC 4GL INQUIRE_4GL will be meshed in w/in 		*/
		/* these 3gl procs, as well as some when       		*/
		/* EXEC 4GL GET/SET ATTRIBUTE/DESCRIBE for records and  */
		/* arrays.                                     		*/

    		'end Arrays Ops' = {
			endloop;
    		}  
  	}
}

'Display Error Messages on Screen' = {
	callproc set4gl(1);
}

'Suppress Error Messages' = {
	callproc set4gl(0);
}

'End' = {
	exit;
}

procedure initarr = {
	myarr = select ccol = ccol,
			   charcol = charcol,
			   varcol = varcol,
			   textcol = textcol,
			   floatcol = floatcol,
			   f4col = f4col,
			   f8col = f8col,
			   intcol = intcol,
			   smallcol = smallcol,
			   moneycol = moneycol,
			   datecol = datecol,
			   deccol1 = deccol1,
			   deccol2 = deccol2,
			   deccol3 = deccol3
		      from tblarr;
}	

procedure viewarr = 
declare
	r = integer not null,
	i = integer not null, {

	clear field tblarr;
	inquire_forms table e4gluf4 (r = maxrow(tblarr));

	i = 0;
	unloadtable myarr {
		if (myarr._state = 4) then
		    /* deleted: insert it and then delete it */
		    insertrow tblarr[0] (ccol = myarr.ccol,
					charcol = myarr.charcol,
					varcol = myarr.varcol,
					textcol = myarr.textcol,
					floatcol = myarr.floatcol,
					f4col = myarr.f4col,
					f8col = myarr.f8col,
					intcol = myarr.intcol,
					smallcol = myarr.smallcol,
					moneycol = myarr.moneycol,
					datecol = myarr.datecol,
					deccol1 = myarr.deccol1,
					deccol2 = myarr.deccol2,
					deccol3 = myarr.deccol3);
		    deleterow tblarr[1];
		else
		    /* insert it and then delete it */
		    insertrow tblarr[i] (ccol = myarr.ccol,
					charcol = myarr.charcol,
					varcol = myarr.varcol,
					textcol = myarr.textcol,
					floatcol = myarr.floatcol,
					f4col = myarr.f4col,
					f8col = myarr.f8col,
					intcol = myarr.intcol,
					smallcol = myarr.smallcol,
					moneycol = myarr.moneycol,
					datecol = myarr.datecol,
					deccol1 = myarr.deccol1,
					deccol2 = myarr.deccol2,
					deccol3 = myarr.deccol3,
					_state = myarr._state);
		    if i < r then
			i = i + 1;
		    endif;

		endif;
	};
	scroll tblarr to 1;
}
