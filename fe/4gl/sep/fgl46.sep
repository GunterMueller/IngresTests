? fill topaz8.osq 
!!
initialize =
declare
	books = array of type of table field topaz8.booktbl;
	ret_val = smallint;
{
	books = select book_no, title, author, price, category
		from book_list
		where title like 'T%';
}

'tablook_up' = {
	ret_val = callframe look_up ( 
		ii_query = select * from book_list;
		ii_rows = 12,
		book_no = byref(booktbl.book_no),
		title = byref(booktbl.title),
		author = byref(booktbl.author),
		price = byref(booktbl.price),
		category = byref(booktbl.category),
		ii_field1 = 'title',
		ii_field2 = 'category');
	message 'callframe look_up returned ' + varchar(:ret_val);
}

'arrlook_up' = {
	ret_val = callframe look_up (
		ii_array = books,
		ii_rows = 12,
		book_no = byref(booktbl.book_no),
		title = byref(booktbl.title),
		author = byref(booktbl.author),
		price = byref(booktbl.price),
		category = byref(booktbl.category),
		ii_field1 = 'title',
		ii_field2 = 'category');
	message 'callframe look_up returned ' + varchar(:ret_val);
}

'end', key frskey3 = {
	exit;
}
