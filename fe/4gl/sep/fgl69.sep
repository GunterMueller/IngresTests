? fill pka16_f1.osq 
!!
/* Topframe of application */

'Test_Go' =
{
   callframe pka16_f3;
}

'Test_Image' =
{
   callframe pka16_f3;
}

'Test_Image_nodb' =
{
   callframe pka16_f2;
}

'End' =
{
   return;
}
!!
? fill pka16_f2.osq 
!!
/* This frame is for testing image with the -nodatabase flag 
** If you are not using compiled forms and there are no user sessions active,
** you will get error: "Unable to initialize form for frame.  The form
** `pka16_f1' for the frame `pka16_f1' could not be initialized as there are
** no current database session active.  The frame will not be executed. 
** (E_AR006A)".
*/

initialize(
        connect_name = char(25) not null,
        session_id = integer not null
        ) =
{
	SET_SQL (PROGRAMQUIT = 0);
}


'Call_QBF' =
{
   	call qbf (table = 'pka16_tbl');
}

'Call_RBF' =
{
      	call rbf (flags = '-e');
}

'Go' =
{
   INQUIRE_SQL (session_id = SESSION);
   MESSAGE 'The session id = 0: ' + varchar(:session_id) WITH STYLE = POPUP;
   INQUIRE_SQL (connect_name = CONNECTION_NAME);
   MESSAGE 'The connection name should be left blank: ' + :connect_name
   WITH STYLE = POPUP;

   MESSAGE 'setting up a db connection ...';
   SLEEP 2;
   CONNECT TO abf4gldb;

   INQUIRE_SQL (session_id = SESSION);
   MESSAGE 'The session id = 1: ' + varchar(:session_id)
   WITH STYLE = POPUP;
   INQUIRE_SQL (connect_name = CONNECTION_NAME);
   MESSAGE 'The connection name is: ' + :connect_name
   WITH STYLE = POPUP;

   MESSAGE 'disconnecting the session...'; SLEEP 2;
   DISCONNECT CURRENT;
   INQUIRE_SQL (session_id = SESSION);
   MESSAGE 'The current session id = 0: ' + varchar(:session_id)
   WITH STYLE = POPUP;

   DISCONNECT 'ii-1';              /* error, invalid connection name */
   INQUIRE_SQL (session_id = SESSION);
   MESSAGE 'The current session id should still = 0: ' + varchar(:session_id)
   WITH STYLE = POPUP;
}

'End'=
{
   return;
}
 
!!
? fill pka16_f3.osq 
!!
initialize(
        connect_name = char(25) not null,
        cvar = char(20) not null,
        dbnamevar = char(20) not null,
        intvar = integer not null,
	session_id = integer not null,
        svar = integer not null,
        intquit = integer not null
	) =
{
   INQUIRE_SQL (intquit = PROGRAMQUIT);
   MESSAGE 'programquit must = 0 for this test, and is: ' 
      + varchar(:intquit) WITH STYLE = POPUP;
   svar = 9;
   connect_name = 'abf4gldb DB';
}
 
'Go' =
{
   INQUIRE_SQL (session_id = SESSION);
   MESSAGE 'The default session id = -1: ' + varchar(:session_id)
   WITH STYLE = POPUP;
   INQUIRE_SQL (connect_name = CONNECTION_NAME);                               
   MESSAGE 'The connection name for the default session is ii-1: '
   + :connect_name WITH STYLE = POPUP; 

   /* Any connect/disconnect error should leave you in the previous session */
   MESSAGE 'attempting to disconnect the initial/default session...'; SLEEP 2;
   DISCONNECT CURRENT;        
   INQUIRE_SQL (session_id = SESSION);
   MESSAGE 'The current session id should still = -1: ' + varchar(:session_id)
   WITH STYLE = POPUP;

   DISCONNECT 'ii-1';              /* error, invalid connection name */ 
   INQUIRE_SQL (session_id = SESSION);
   MESSAGE 'The current session id should still = -1: ' + varchar(:session_id)
   WITH STYLE = POPUP;

   MESSAGE 'setting up database connections... '; SLEEP 2;
   CONNECT abf4gldb SESSION 2;     
   INQUIRE_SQL (connect_name = CONNECTION_NAME);                               
   MESSAGE 'The connection name for session 2 = ii2: ' + :connect_name 
   WITH STYLE = POPUP; 
   CONNECT abf4gldb SESSION 3;       

   MESSAGE 'testing connect_name = ii- ...'; SLEEP 2;
   connect_name = 'ii-';             /* error, invalid connection name */
   CONNECT TO abf4gldb AS :connect_name;
   INQUIRE_SQL (connect_name = CONNECTION_NAME);
   MESSAGE 'The connection name is ii3: ' + :connect_name
   WITH STYLE = POPUP;       

   MESSAGE 'testing connect_name = II-5 ...'; SLEEP 2;
   connect_name = 'II-5';
   CONNECT TO iidbdb AS :connect_name;
   INQUIRE_SQL (connect_name = CONNECTION_NAME);
   MESSAGE 'The connection name for iidbdb is II-5: ' + :connect_name
   WITH STYLE = POPUP;                             

   MESSAGE 'connecting to session svar, where svar = 9...'; SLEEP 2;
   CONNECT TO abf4gldb SESSION :svar;   

   /* The following three statements will fail at runtime.  After each error,
   ** the session id remains unchanged.
   */
   CONNECT TO abf4gldb SESSION 0;     
   CONNECT TO abf4gldb as 'ii-3';     
   CONNECT TO abf4gldb SESSION -5;    

   INQUIRE_SQL (session_id = SESSION);
   MESSAGE 'The current session id should still = 9: ' + varchar(:session_id)
   WITH STYLE = POPUP;

   /* This connect will fail since the db name does not exist  */
   CONNECT TO nonexistentdb;            
   INQUIRE_SQL (session_id = SESSION);
   MESSAGE 'The current session id should still = 9: ' + varchar(:session_id)
   WITH STYLE = POPUP;

   SET CONNECTION 'ii3';                               
   SET_SQL (SESSION = 3);
   SELECT cvar = charcol, intvar = intcol FROM pka16_tbl;
   commit;
   MESSAGE 'The value of cvar is: ' + :cvar WITH STYLE = POPUP;

   DISCONNECT SESSION 2;                /* disconnect a non-current session */
   INQUIRE_SQL (session_id = SESSION);
   MESSAGE 'The current session id should still = 3: ' + varchar(:session_id)
   WITH STYLE = POPUP;

   SET_SQL (SESSION = 2);    /* error, non-existent session  */
   SET_SQL (SESSION = 3);
   SET_SQL (SESSION = 10);   /* error, non-existent session  */

   MESSAGE 'connecting to default session...'; SLEEP 2;
   CONNECT abf4gldb;            
   INQUIRE_SQL (session_id = SESSION);
   MESSAGE 'The current session id is assigned the value of 2: '
   + varchar(:session_id) WITH STYLE = POPUP;

   INQUIRE_SQL (connect_name = CONNECTION_NAME);                               
   MESSAGE 'The connection name is the database name and is: '                 
       + :connect_name WITH STYLE = POPUP;  

   DISCONNECT SESSION 10;    /* error, non-existent session  */ 
   INQUIRE_SQL (session_id = SESSION);
   MESSAGE 'The current session id should still = 2: ' + varchar(:session_id)
   WITH STYLE = POPUP;

   DISCONNECT 'ii-1';        /* error, invalid connection name    */ 
   INQUIRE_SQL (session_id = SESSION);
   MESSAGE 'The current session id should still = 2: ' + varchar(:session_id)
   WITH STYLE = POPUP;

   MESSAGE 'disconnecting the current session...'; SLEEP 2;
   DISCONNECT CURRENT;
   INQUIRE_SQL (session_id = SESSION);
   MESSAGE 'The current session id should = 0: ' + varchar(:session_id)
   WITH STYLE = POPUP;      

   CONNECT abf4gldb;
   DISCONNECT ALL;
   INQUIRE_SQL (session_id = SESSION);
   MESSAGE 'There is no active sessions, so session id = 0: ' 
   + varchar(:session_id) WITH STYLE = POPUP;

   MESSAGE 'disconnecting the current session...'; SLEEP 2;
   DISCONNECT CURRENT;       /* error, no current session is active */
}

'Call_frame' =
{
/* NOTE: This menuitem should be selected AFTER the 'Go' menuitem, then the 
** session id = 0, but if it's selected BEFORE 'Go', the session id = -1
** for the default connection.
*/     

   CALLFRAME pka16_f4;
   INQUIRE_SQL (session_id = SESSION);
   MESSAGE 'The current session id should still = 0: ' + varchar(:session_id)
   WITH STYLE = POPUP;
}

'Call_proc' =
{
   CALLPROC pka16_pr();
   INQUIRE_SQL (session_id = SESSION);
   /* the procedure switched sessions to 20, this should remain in effect */
   MESSAGE 'The current session id should = 20: ' + varchar(:session_id)
   WITH STYLE = POPUP;
}

'End' =
{
   return;
}

!!
? fill pka16_f4.osq 
!!
initialize (session_id = integer not null) = 
{
}

'Go' =
{
/* NOTE: The session id will be 0 if 'Go' from the pka16_f3 frame was first
** selected before calling this frame, since there were no active sessions
** left.  But, if 'Go' was not selected before calling this frame, the session
** id will be -1, to indicate the default connection.
*/

INQUIRE_SQL (session_id = SESSION);
MESSAGE 'The current session id should still = 0: ' + varchar(:session_id)
WITH STYLE = POPUP;
return;
}

'End' =
{
   return;
}
!!
? fill pka16_pr.sc 
!!
EXEC SQL BEGIN DECLARE SECTION;
   float  fvar;   
EXEC SQL END DECLARE SECTION;
pka16_pr()
{
EXEC SQL DISCONNECT ALL;
EXEC SQL CONNECT abf4gldb SESSION 20;
EXEC SQL SELECT fcol INTO :fvar FROM pka16_tbl;
return;
}

