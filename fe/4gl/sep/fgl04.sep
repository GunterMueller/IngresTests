? fill oia11uf1.osq 
!!
initialize () = {
   oia11_variable.attr1 = 1;
   oia11_variable.attr2 = 'Global';
   disp_attr1 = oia11_variable.attr1;
   disp_attr2 = oia11_variable.attr2;
   local_attr1 = 2;
   local_attr2 = 'Local';
}

'Go' = {
   callproc oia11_4p1 (disp_attr1 = disp_attr1, disp_attr2=disp_attr2,
                       local_attr1=local_attr1, local_attr2=local_attr2);
}

'End', key frskey3 = {
   clear screen;
   return;
}
!!
? fill oia11uf2.osq 
!!
initialize (oia11_variable = oia11_record) = {
   disp_attr1 = oia11_variable.attr1;
   disp_attr2 = oia11_variable.attr2;
}

'End', key frskey3 = {
   clear screen;
   return;
}
!!
? fill oia11_4p1.osq 
!!
procedure oia11_4p1 (disp_attr1 = integer, disp_attr2 = char(10),
                     local_attr1 = integer, local_attr2 = char(10),
                     oia11_variable = oia11_record) = {

   oia11_variable.attr1 = 999;
   oia11_variable.attr2 = 'LOCAL';
   callframe oia11uf2 (oia11_variable = oia11_variable, local_attr1 = 
                        local_attr1, local_attr2 = local_attr2);
   return;
}
!!
