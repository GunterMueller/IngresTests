? fill oka02uf1.osq 
!!
initialize (countr=integer) = {
   inittable v1_tf;
   inittable v2_tf;
   inittable v3_tf;
   countr=1;
   while countr < 9 do
      insertrow v1_tf[0];
      insertrow v2_tf[0];
      insertrow v3_tf[0];
      countr=countr+1;
   endwhile;
}

'CallProc' = {
   callproc oka02_4p1 ();
   countr=1;
   while countr < 9 do
      v1_tf[countr].col1 = variable1[countr].r1_attr1;
      v2_tf[countr].col1 = variable1[countr].r1_attr1;
      v3_tf[countr].col1 = variable3[1].r3_attr1[countr].r2_attr2.r1_attr1;
      countr=countr+1
   endwhile;
   v2_attr1 = variable2.r2_attr1;
   v3_attr1 = variable3[1].r3_attr1[1].r2_attr1;   
}

'End' = {
   clear screen;
   return;
}
!!
? fill oka02_4p1.osq 
!!
procedure oka02_4p1 (sub = integer) = {
/* Initialize variable1: */
   variable1[1].r1_attr1 = 'One';
   variable1[2].r1_attr1 = 'Two';
   variable1[3].r1_attr1 = 'Three';
   variable1[4].r1_attr1 = 'Four';
   variable1[5].r1_attr1 = 'Five';
   variable1[6].r1_attr1 = 'Six';
   variable1[7].r1_attr1 = 'Seven';
   variable1[8].r1_attr1 = 'Eight';
   
/* Initialize variable2: */
   variable2.r2_attr1 = 1;
   sub=1;
   while sub < 9 do
      variable2.r2_attr2.r1_attr1 = variable1[sub].r1_attr1;
      sub = sub+1;
   endwhile;

/* Initialize variable3: */
   variable3[1].r3_attr1[1].r2_attr1 = variable2.r2_attr1;
   sub=1;
   while sub < 9 do
      variable3[1].r3_attr1[sub].r2_attr2.r1_attr1 = 
                variable1[sub].r1_attr1;
      sub = sub+1;
   endwhile;

   return;
}
