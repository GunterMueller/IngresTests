FIELD:
	0	value1	-31	9	0	10	1	20	0	0	10	0	10	Value 1 :	0	0	0	16	0	0	0		-f10.2			0	0
	1	value2	-31	9	0	10	1	20	1	0	10	0	10	Value 2 :	0	0	0	16	0	0	0		-f10.2			0	1
	2	result	-31	5	0	15	1	25	3	0	15	0	10	Result  :	0	0	0	0	0	0	0		-f15.3			0	2
TRIM:
!!
? fill pba03uf1.osq 
!!
initialize ( h_operator = varchar(10), h_stat = smallint  ) =
{
  message 'Tests Calling 4GL procedures with paramters...' +
    'Enter values for Value 1 and Value 2 and then choose ChooseOperation';
  sleep 3;
}

'ChooseOperation' =
{
  h_operator = prompt 'Enter the operator. The available operators are :' +
    '(a)ddition, (s)ubtraction, (m)ultiplication, (d)ivision, (e)exponentiation'
    with style = popup;
    
  h_stat = callproc pba03_4p1 ( op = :h_operator, value1 = :value1, 
    value2 = :value2, result = byref(result));
  if h_stat < 0 then
    clear field all;
  endif;
}

'Clear' =
{
  clear field all;
  resume field value1;
}


'End', key frskey3 =
{
  return;
}
!!
? fill pba03_4p1.osq 
!!
procedure pba03_4p1 ( op = varchar(10), value1 = float, value2 = float ,
result = float ) =
{
  if op like 'a%'  then
    result = value1 + value2;
  elseif op like 's%'  then
    result = value1 - value2;
  elseif op like 'm%'  then
    result = value1 * value2;
  elseif op like 'd%'  then
    result = value1 / value2;
  elseif op like 'e%'  then
    result = value1 ** value2;
  else
    message 'Invalid operator';
    sleep 2;
    return -1;
  endif;
  return 0;
}
!!
