initialize () = 

declare
select_name = varchar(32)
begin
end

'Variable defined in code' =
begin

	select_name = '%'; 
	inittable liketable;   
	liketable = 
	select 
		item = table_name,
		type = table_type
	from
		iitables
	where	
		table_name like :select_name  
	order by
		item
end

'Variable defined on form' =
begin
	inittable liketable;
	message 'Please enter a % in the simplefield above for the like clause';
	sleep 5;
	Liketable = 
	select 
		item = table_name,
		type = table_type
	from
		iitables
	where	
		table_name like :field_like  
	order by
		item

end

'Variable Defined as literal' =
begin
	inittable liketable;
	liketable = 
	select 
		item = table_name,
		type = table_type
	from
		iitables
	where	
		table_name like '%'  
	order by
		item
end


'End' =
begin
	return;
end
