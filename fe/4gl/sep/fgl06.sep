? fill oja03uf1.osq 
!!
initialize (y = array of type of table field oja03uf1.c23_tf;
	    x = oja03_record; 
            thestate = integer;
            thiscolumn = char(10);
            totalrows = integer) = {
   inittable c23_tf;
   }

'Go1' = {
/* Load the parameters for passing: */

   x.attribute_1 = c23class_integer;
   x.attribute_2 = c23class_varchar;

   totalrows = 0;
   unloadtable c23_tf (thiscolumn = column1,thestate=_state) {
      if thestate > 0 and thestate < 4 then
        y[totalrows+1].column1 = thiscolumn;
        totalrows = totalrows + 1;
      endif;
      };

/* Call a 4GL procedure to change the values of the passed parameters: */
   callproc oja03_4p1 (y = y, x=x, totalrows=totalrows);

/* Reinitialize the fields on the form with the values passed back: */
   c23class_integer = x.attribute_1;
   c23class_varchar = x.attribute_2;

   inittable c23_tf;
   totalrows = 0;
   unloadtable y (thiscolumn = column1) {
      insertrow c23_tf (column1 = thiscolumn);
      totalrows = totalrows + 1;
      };
   redisplay;
   }

'Go2' = {
/* Load the parameters for passing: */

   x.attribute_1 = c23class_integer;
   x.attribute_2 = c23class_varchar;

   totalrows = 0;
   unloadtable c23_tf (thiscolumn = column1,thestate=_state) {
      if thestate > 0 and thestate < 4 then
        y[totalrows+1].column1 = thiscolumn;
        totalrows = totalrows + 1;
      endif;
      };

   callframe oja03uf2 (y=y; x=x; totalrows=totalrows);
   redisplay;
   }

'End', key frskey3 = {
   clear screen;
   return;
   }
!!
? fill oja03uf2.osq 
!!
initialize (y = array of type of table field oja03uf1.c23_tf;
	    x = oja03_record; 
            thiscolumn = char(10);
            totalrows = integer) = {
   inittable c23_tf read;

   c23class_integer = x.attribute_1;
   c23class_varchar = x.attribute_2;
   c23_totalrows = totalrows;

   totalrows = 0;
     unloadtable y {
        insertrow c23_tf[totalrows] (column1 = y[totalrows+1].column1);
        totalrows = totalrows + 1;
     }
   }

'End',key frskey3 = {
   clear screen;
   return;
   }
!!
? fill oja03_4p1.osq 
!!
procedure oja03_4p1 (y = array of type of table field oja03uf1.c23_tf,
	    x = oja03_record,
            rowcount = integer,
            totalrows = integer) = {
   rowcount = 0;
   while rowcount < totalrows do
      rowcount = rowcount + 1;
      y[rowcount].column1 = 'NEW' + varchar(rowcount);
   endwhile;

   x.attribute_1 = x.attribute_1 / 2;
   x.attribute_2 = 'NEWTEXT';

   return;
}
!!
