/*  pla16 - 4GL test of blobs behavior
**  NOTE: This test tests create table with long datatype columns;
**        table pla16_tbl is in init script         
*/

initialize(
        x = type of table pla16_tbl,            /* compile warning */  
	y = array of type of table pla16_tbl    /* compile warning */
	) =
{
        delete from pla16_tbl;
}

'CreateTbl' =
{
	message 'creating table with long datatype columns...'; sleep 2;
        create table longtypes (col1 long varchar not null,
				col2 byte not null,
	                        col3 byte varying not null, 
				col4 long byte not null);  
}

'Copy' =
{
        message 'testing copy with long datatypes...'; sleep 2;
	copy table longtypes () into 'pla16.dat';
	copy table longtypes () from 'pla16.dat';
}

'Insert/select' =
{
  /* pla16_tbl: long varchar, byte, byte varying, long byte, varchar cols */

	insert into pla16_tbl values ('Inserting a row of data '
	+ 'into the table named pla16_tbl.  This table is used '
	+ 'to test long datatype behavior in 4gl.  These types are not '
	+ 'currently supported in 4gl, but it should be possible to coerce '
	+ 'them into varchar variables', 'This is the byte column', 'This '
	+ 'is the byte varying column', 'This is the long byte column', 'a');
        commit;

	clear field all;
        pla16_uf = select longvarchar = col1, byte = col2, 
		   bytevarying = col3, longbyte = col4 from pla16_tbl;
        commit;
}

'Update' =
{
	update pla16_tbl set col1 = 'Updating the long varchar column with '
	+ 'new data', col2 = 'Updating the byte column with new data', 
	col3 = 'Updating the byte varying column with new data',
	col4 = 'Updating the long byte column with new data';
        commit;

	clear field all;
        pla16_uf = select longvarchar = col1, byte = col2, 
		   bytevarying = col3, longbyte = col4 from pla16_tbl;
        commit;
}

'Delete' =
{
	delete from pla16_tbl;
        commit;

	clear field all;
        pla16_uf = select longvarchar = col1, byte = col2, 
		   bytevarying = col3, longbyte = col4 from pla16_tbl;
        commit;
}

'End' = 
{
	return;
}
!!
