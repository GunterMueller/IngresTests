/* 
** Using strform 
** Testing the following operators:comparison, boolean, pattern match
*/

initialize =
{
  message 'Testing expressions...Enter values for string1 and string2 field';
  sleep 2;
}

'Compare' =
{

  if string1 = string2 then
    message 'string1 = string2';
    sleep 2;
  elseif string1 != string2 then
    message 'string1 != string2';
    sleep 2;
   endif;

  if string1  > string2 then
    message 'string1 >= string2';
    sleep 2;
  elseif string1  >= string2 then
    message 'string1 >= string2';
    sleep 2;
  endif;

  if string1 < string2 then
    message 'string1 < string2';
    sleep 2;
  elseif string1 <= string2 then
    message 'string1 <= string2';
    sleep 2;
  endif;
}

'Boolean' =
{
  if string1 is null and string2 is null then
    message 'string1 is null and string2 is null';
    sleep 2;
  endif;

  if string1 is null or string2 is null then
    message 'string1 is null or string2 is null';
    sleep 2;
  endif;

  if string1 is not null then
    message 'string1 is not null';
    sleep 2;
  endif;
    
  if not string2 is null then
    message 'string2 is not null';
    sleep 2;
  endif;
}

'PatternMatch' =
{
  if ((string1 is not null) and (string1 like '%ing')) then
    message 'string1 ends with ing';
    sleep 2;
  else
    message 'string1 does not end with ing';
    sleep 2;
  endif;

  if ((string1 is not null) and (string1 not like '%a%')) then
    message 'string1 does not contain the letter a';
    sleep 2; 
  elseif string1 is not null then
    message 'string1 contains the letter a';
    sleep 2;
  endif;

/*
	if string1 like '_\[Aa\-Mm\]%' escape '\' then
    message 'the second letter of string1 is between [Aa-Mm]';
    sleep 2; 
  else
    message 'the second letter of string1 is not between [Aa-Mm]';
    sleep 2;
  endif;
*/
}

'End' =
{
  return;
}
