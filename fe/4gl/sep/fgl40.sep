? fill pka06uf1.osq 
!!

initialize (rowno = integer, passrec1 = type of form pka06_x1,
            passrec2 = array of type of table field pka06_x2.table_1) = {
   simple_1 = 1;
   simple_2 = 2.222;
   simple_3 = 'Three';
   simple_4 = 'FOUR';
   inittable table_1;
   insertrow table_1[0]
      (t1_col1 = 1, t1_col2 = 1.111, t1_col3 = 'One', t1_col4 = 'ONE');
   insertrow table_1[1]
      (t1_col1 = 2, t1_col2 = 2.222, t1_col3 = 'Two', t1_col4 = 'TWO');
   insertrow table_1[2]
      (t1_col1 = 3, t1_col2 = 3.333, t1_col3 = 'Three', t1_col4 = 'THREE');
   insertrow table_1[3]
      (t1_col1 = 4, t1_col2 = 4.444, t1_col3 = 'Four', t1_col4 = 'FOUR');
   set_forms form (mode = 'read');
}

'CallSimple' = {
   passrec1.simple_1 = simple_1;
   passrec1.simple_2 = simple_2;
   passrec1.simple_3 = simple_3;
   passrec1.simple_4 = simple_4;
   callframe pka06uf2 (passrec = passrec1);
}

'CallTable' = {
   rowno = 0;
   unloadtable table_1 {
      rowno = rowno + 1;
      passrec2[rowno].t1_col1 = table_1[rowno].t1_col1;  
      passrec2[rowno].t1_col2 = table_1[rowno].t1_col2;  
      passrec2[rowno].t1_col3 = table_1[rowno].t1_col3;  
      passrec2[rowno].t1_col4 = table_1[rowno].t1_col4;  
   };
   callframe pka06uf3 (passrec = passrec2);
}

'End', key frskey3 = {
   clear screen;
   return;
}

!!
? fill pka06uf2.osq 
!!

initialize (passrec = type of form pka06_x1) = {
   simple_1 = passrec.simple_1;
   simple_2 = passrec.simple_2;
   simple_3 = passrec.simple_3;
   simple_4 = passrec.simple_4;
}

'End', key frskey3 = {
   clear screen;
   return;
}

!!
? fill pka06uf3.osq 
!!

initialize (passrec = array of type of table field pka06_x2.table_1,
            rowno = integer) = {

   inittable table_1;
   rowno = 0;
   unloadtable passrec {
      insertrow table_1[rowno] (t1_col1 = passrec[rowno+1].t1_col1,
                                t1_col2 = passrec[rowno+1].t1_col2,
                                t1_col3 = passrec[rowno+1].t1_col3,
                                t1_col4 = passrec[rowno+1].t1_col4);
      rowno = rowno + 1;
   };
}

'End', key frskey3 = {
   clear screen;
   return;
}


