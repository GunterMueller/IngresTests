initialize =
    begin
	wwwchar = 'Character Field';
	wwwfloat = 123456.789;
	wwwdate  = '12-Jun-1989';
	wwwmoney = 19.95;

	wwwcharinv = wwwchar;
	wwwfloatinv = wwwfloat;
	wwwdateinv  = wwwdate;
        wwwmoneyinv = wwwmoney;

   	set_forms field vma06 (displayonly(wwwchar) = 0);
	set_forms field vma06 (displayonly(wwwfloat) = 0);
	set_forms field vma06 (displayonly(wwwmoney) = 0);
	set_forms field vma06 (displayonly(wwwdate) = 0);


   	set_forms field vma06 (displayonly(wwwcharinq) = 0);
	set_forms field vma06 (displayonly(wwwfloatinq) = 0);
	set_forms field vma06 (displayonly(wwwmoneyinq) = 0);
	set_forms field vma06 (displayonly(wwwdateinq) = 0);

   	set_forms field vma06 (displayonly(wwwcharinv) = 1);
	set_forms field vma06 (displayonly(wwwfloatinv) = 1);
	set_forms field vma06 (displayonly(wwwmoneyinv) = 1);
	set_forms field vma06 (displayonly(wwwdateinv) = 1);

	set_forms field vma06 (invisible(wwwchar) = 0);
	set_forms field vma06 (invisible(wwwfloat) = 0);
	set_forms field vma06 (invisible(wwwdate) = 0);
	set_forms field vma06 (invisible(wwwmoney) = 0);

	set_forms field vma06 (invisible(wwwcharinv) = 0);
	set_forms field vma06 (invisible(wwwfloatinv) = 0);
	set_forms field vma06 (invisible(wwwdateinv) = 0);
	set_forms field vma06 (invisible(wwwmoneyinv) = 0);

	inquire_forms field vma06 (wwwcharinq=format(wwwchar));
	set_forms field vma06 (format(wwwcharinv)=wwwcharinq);
	inquire_forms field vma06 (wwwfloatinq=format(wwwfloat));
	set_forms field vma06 (format(wwwfloatinv)=wwwfloatinq);
	inquire_forms field vma06 (wwwdateinq=format(wwwdate));
	set_forms field vma06 (format(wwwdateinv)=wwwdateinq);
	inquire_forms field vma06 (wwwmoneyinq=format(wwwmoney));
	set_forms field vma06 (format(wwwmoneyinv) = wwwmoneyinq);
	redisplay;
    end

'Disappear' =
    begin
	set_forms field vma06 (invisible(wwwcharinv) = 1);
	set_forms field vma06 (invisible(wwwfloatinv) = 1);
	set_forms field vma06 (invisible(wwwdateinv) = 1);
	set_forms field vma06 (invisible(wwwmoneyinv) = 1);
    end

'Reappear' =
    begin
	set_forms field vma06 (invisible(wwwcharinv) = 0);
	set_forms field vma06 (invisible(wwwfloatinv) = 0);
	set_forms field vma06 (invisible(wwwdateinv) = 0);
	set_forms field vma06 (invisible(wwwmoneyinv) = 0);
    end

'Reset' =
    begin
	inquire_forms field vma06 (wwwcharinq=format(wwwchar));
	set_forms field vma06 (format(wwwcharinv)=wwwcharinq);
	inquire_forms field vma06 (wwwfloatinq=format(wwwfloat));
	set_forms field vma06 (format(wwwfloatinv)=wwwfloatinq);
	inquire_forms field vma06 (wwwdateinq=format(wwwdate));
	set_forms field vma06 (format(wwwdateinv)=wwwdateinq);
	inquire_forms field vma06 (wwwmoneyinq=format(wwwmoney));
	set_forms field vma06 (format(wwwmoneyinv) = wwwmoneyinq);
	resume field wwwchar;
    end

'SpecifyFormats' =
    begin
	resume field wwwcharinq;
    end

field 'wwwcharinq'  =
    begin
	set_forms field vma06 (format(wwwcharinv)=wwwcharinq);
	resume next;
    end

field 'wwwfloatinq' =
    begin
        set_forms field vma06 (format(wwwfloatinv)=wwwfloatinq);
	resume next;
    end

field 'wwwdateinq' =
    begin
	set_forms field vma06 (format(wwwdateinv)=wwwdateinq);
	resume next;
    end

field 'wwwmoneyinq' =
    begin
	set_forms field vma06 (format(wwwmoneyinv)=wwwmoneyinq);
	resume next;
    end

field 'wwwchar' =
    begin
	wwwcharinv  = wwwchar;
	resume next;
    end

field 'wwwdate' =
    begin
	wwwdateinv = wwwdate;
	resume next;
    end

field 'wwwfloat' =
    begin
	wwwfloatinv = wwwfloat;
	resume next;
    end

field 'wwwmoney' =
    begin
	wwwmoneyinv = wwwmoney;
	resume field wwwchar;
    end

'DisplayOnly' =
    begin
   	set_forms field vma06 (displayonly(wwwchar) = 1);
	set_forms field vma06 (displayonly(wwwfloat) = 1);
	set_forms field vma06 (displayonly(wwwmoney) = 1);
	set_forms field vma06 (displayonly(wwwdate) = 1);
    end

'Enterable' = 
    begin
   	set_forms field vma06 (displayonly(wwwchar) = 0);
	set_forms field vma06 (displayonly(wwwfloat) = 0);
	set_forms field vma06 (displayonly(wwwmoney) = 0);
	set_forms field vma06 (displayonly(wwwdate) = 0);
	resume field wwwchar; 
   end

'Exit',key frskey2 =
    begin
    	exit;
    end

