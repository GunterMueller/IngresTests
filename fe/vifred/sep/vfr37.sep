!!
initialize = 
    {
        insertrow vma07 (wwwchar = 'Character Field',
                         wwwdate = '13-Jun-1989',
                         wwwfloat = 12345.6789,
                         wwwmoney = 199.95);

	set_forms column vma07 vma07 (format(wwwchar)='c15');
	set_forms column vma07 vma07 (format(wwwdate)='c20');
	set_forms column vma07 vma07 (format(wwwfloat)='-n20.3');
	set_forms column vma07 vma07 (format(wwwmoney)='-"$---------------.nn"');

	wwwcharfmt = 'c15';
	wwwdatefmt = 'c20';
        wwwfloatfmt = '-n20.3';
        wwwmoneyfmt = '-"$---------------.nn"';
    }
field wwwcharfmt =
    {
        set_forms column vma07 vma07 (format(wwwchar)=wwwcharfmt);
	resume next;
    }
field wwwdatefmt =
    {
	set_forms column vma07 vma07 (format(wwwdate)=wwwdatefmt);
	resume next;
    }
field wwwfloatfmt =
    {
	set_forms column vma07 vma07 (format(wwwfloat)=wwwfloatfmt);
	resume next;
    }
field wwwmoneyfmt =
    {
	set_forms column vma07 vma07 (format(wwwmoney)=wwwmoneyfmt);
	resume next;
    }

'SpecifyFormat' =
    {
	resume field wwwcharfmt;
    }

'Reset' =
    {
	set_forms column vma07 vma07 (format(wwwchar)='c15');
	set_forms column vma07 vma07 (format(wwwdate)='c20');
	set_forms column vma07 vma07 (format(wwwfloat)='-n20.3');
	set_forms column vma07 vma07 (format(wwwmoney)='-"$---------------.nn"');

	wwwcharfmt = 'c15';
	wwwdatefmt = 'c20';
        wwwfloatfmt = '-n20.3';
        wwwmoneyfmt = '-"$---------------.nn"';
    }

'Disappear' =
    {
	set_forms field vma07 (invisible(vma07)=1);
    }

'Reappear' =
    {
	set_forms field vma07 (invisible(vma07)=0);
    }

'DisplayOnly' =
    {
	set_forms column vma07 vma07 (displayonly(wwwchar)=1);
    }

'Enterable' =
    {
	set_forms column vma07 vma07 (displayonly(wwwchar)=0);
    }

'Reverse' =
    {
	set_forms column vma07 vma07 (reverse(wwwchar)=1);
    }

'UnReverse' =
    {
	set_forms column vma07 vma07 (reverse(wwwchar)=0);
    }

'Blink' =
    {
	set_forms column vma07 vma07 (blink(wwwchar)=1);
    }

'UnBlink' =
    {
	set_forms column vma07 vma07 (blink(wwwchar)=0);
    }

'Bright' =
    {
	set_forms column vma07 vma07 (intensity(wwwchar)=1);
    }

'UnBright' =
    {
	set_forms column vma07 vma07 (intensity(wwwchar)=0);
    }

'Normal' =
    {
	set_forms column vma07 vma07 (normal(wwwchar)=1);
    }

'End', key frskey3 =
    {
	exit;
    }
!!
