? fill vma03.osq 
!!
initialize = 
    begin
	number0 = 0;
	number1 = 0;
	number2 = 0;
	number3 = 0;
	number4 = 0;
	number5 = 0;
	number6 = 0;
	number7 = 0;
	number8 = 0;
	number9 = 0;
    end
'Next' =
    begin
	number0 = (number0 + 1) * 10;
	number1 = (number1 + 1) * 10;
	number2 = (number2 + 1) * 10;
	number3 = (number3 + 1) * 10;
	number4 = (number4 + 1) * 10;
	number5 = (number5 + 1) * 10;
	number6 = (number6 + 1) * 10;
	number7 = (number7 + 1) * 10;
	number8 = (number8 + 1) * 10;
	number9 = (number9 + 1) * 10;
    end
'End' =
    begin
	clear screen;
	exit;
    end

!!
