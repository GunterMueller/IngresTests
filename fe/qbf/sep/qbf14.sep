? fill qga03.qry 
!!
drop table qga03_1 \p\g
drop table qga03_2 \p\g
delete from table_j3	\p\g
delete from table_j4	\p\g
!!
? fill qga04.qry 
!!
create table qga03_1 as select *from table_j3	\p\g
create table qga03_2 as select *from table_j4	\p\g
!!
? sql -s SEPPARAMDB 
<<

>>
* \i qga03.qry 
<<
~
>>
* \i qga04.qry
<<
create table qga03_1 as select *from table_j3
(0 rows)
create table qga03_2 as select *from table_j4
(0 rows)

>>
* \q 
<<
>>
? delobj SEPPARAMDB -joindef j1_qga03 
<<
~
>>
? qbf SEPPARAMDB -e 
