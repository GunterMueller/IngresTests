<<
~
>>
? delobj SEPPARAMDB -form form_Joindef_#10 
<<
~
>>
? delobj SEPPARAMDB -qbfname form_Joindef_#10 
<<
~
>>
? fill qea02a.qry 
!!
drop table qea02_qedept;\p\g
drop table qea02_qeemp;\p\g
commit;\p\g
!!
? fill qea02b.qry 
!!
create table qea02_qedept as select * from qedept;\p\g
create table qea02_qeemp as select * from qeemp;\p\g
commit;\p\g
!!
? sql -s SEPPARAMDB 
