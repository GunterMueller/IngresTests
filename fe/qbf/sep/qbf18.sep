? delobj SEPPARAMDB -joindef qja02_j1 
<<
~
>>
? delobj SEPPARAMDB -form qja02_j1 
<<
~
>>
? fill qja02a.qry 
!!
drop table qja02_1;\p\g
drop table qja02_2;\p\g
!!
? fill qja02b.qry 
!!
create table qja02_1 as select * from table_j1;\p\g
create table qja02_2 as select * from table_j2;\p\g
!!
? sql -s SEPPARAMDB 
<<

>>
* \i qja02a.qry
<<
~
>>
* \i qja02b.qry
<<
create table qja02_1 as select * from table_j1;
(0 rows)
create table qja02_2 as select * from table_j2;
(0 rows)
>>
* \quit
<<

>>
? qbf SEPPARAMDB -e 
<<
~
>>
^^ JoinDefs`CR'`ESC'
<<
~
>>
^^ Create`CR'`ESC'
<<
~
>>
^^ Select`CR'qja02_j1`CR'm`TAB'qja02_1`TAB'`TAB'a`CR'`ESC'
<<

