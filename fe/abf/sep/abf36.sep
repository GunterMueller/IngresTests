>>
? fill coboda29.scb 
!!
IDENTIFICATION DIVISION.
PROGRAM-ID. coboda29.
ENVIRONMENT DIVISION.
DATA DIVISION.
WORKING-STORAGE SECTION.
01 j PICTURE Z(8).
LINKAGE SECTION.
01 i PICTURE S9(8) USAGE COMP.
01 s PICTURE X(4).
PROCEDURE DIVISION USING i,s.
BEGIN.
SHOW-MSG.
        MOVE i TO j.
        DISPLAY "The values passed to this COBOL procedure are:" NO ADVANCING.
        DISPLAY j, " and " NO ADVANCING.
        DISPLAY s.
END PROGRAM coboda29.
!!
? fill coda29.sc 
!!
coda29 (i,s)
int i;
char *s;
{
   SIprintf("The values passed to this C procedure are:\n");
   SIprintf("%d. %s\n", i, s);
}
!!
? fill doda29.sql 
!!
drop procedure doda29;\p\g
create procedure doda29 as
   begin
      message 'You have entered a DB procedure.';
   end;
\p\g
!!
? fill fgloda29.osq 
!!
procedure fgloda29 (i = integer, s = char(4)) = 
{
   message 'The values passed to this 4GL procedure are: ' + varchar(i) + 
           ' and ' + s with style = popup;
}
!!
? fill foroda29.sf 
!!
       subroutine foroda29(i,s)
       integer i
       character *(*)s
          print *, 'The values passed to this Fortran procedure are:'
          print *, s
          print *, i
       end
!!
? fill uoda29.frm 
!!
COPYFORM:	6.4	1994_04_28 23:37:07 GMT  
FORM:	uoda29		
	80	23	0	0	0	0	6	9	0	0	0	0	0	0	0	0
FIELD:
TRIM:
	8	5	1 - 4gl procedure	0	0	0	0
	8	6	2 - C procedure	0	0	0	0
	8	7	3 - Sql procedure	0	0	0	0
	8	8	4 - Fortran procedure	0	0	0	0
	8	9	5 - COBOL procedure	0	0	0	0
	8	10	X - Exit	0	0	0	0
!!
? fill uoda29.osq 
!!
initialize ( i = integer, nine = char(4), s = char(4)) = { }

'4' = { callproc fgloda29(i=9, s='Nine'); }

'C' = { callproc coda29(9, 'Nine'); }

'D' = { callproc doda29(); }

'F' = { callproc foroda29(9, 'Nine') }

'L' = { nine = 'Nine'; callproc coboda29(999, nine); }

'X' = { exit; }
!!
