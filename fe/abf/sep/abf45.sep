	Test Name: oha20.sep
	Time: 17-SEP-1990 16:43:42
	User Name: saundra
 
	Test ID : oha20
	Module  : abf
	Filename: oha20.sep
	Purpose	: in-depth test for changing invisible and display-only 
   		  attribute and display format from 4gl procedures at runtime
	Input Files  :
	Output Files :
	Database     : SEPPARAMDB
	Tables       : suppliers
	Synopsis     : at runtime, set all fields of the form to invisible, then
		       change the display format and disply-only attribute of
		       the fields from a 4gl procedure, put some values to the
		       fields and reset the fields back to non-invisible, verify
		       the fields are shown with new display formats, values
		       and display-only attribute
	Port Note    : Change source path in copyapp file

	History	: 74-SEP-1990	Created				Author: saundra
                 23-Nov-1993    (jpark)
                        moved over to sep directory.  
                        currently only a vms only test.
          15-Dec-1994	((motje01) Take conditional canons for Unix abort
          10-Apr-1996	(clate01) take conditional canon for NT abort
          17-Oct-1998   (kinte01) update copyright
          30-Jan-2006	(boija02) Updated copyright info for Ingres Corp.

*/
.if (VMS) 
? fill top905.osq 
!!
'Invisible' = { callproc proc1_905;
                sno := 'C2';
                tf.sname := 'Robertson';
                tf.status := 50;
                tf.city := 'Montreal';
              }

'Change' = { callproc proc2_905; }

'Quit' = { exit; }
!!
? fill proc1_905.osq 
!!
procedure proc1_905 =
{ message 'Setting all fields to invisible...';
  sleep 2;
  set_forms field suppliersform (invisible(sno) = 1);
  set_forms column suppliersform tf (invisible(sname) = 1);
  set_forms column suppliersform tf (invisible(status) = 1);
  set_forms column suppliersform tf (invisible(city) = 1);
}
!!
? fill proc2_905.osq 
!!
procedure proc2_905 =
{ message 'Changing display formats...';
  sleep 2;
  set_forms field suppliersform (format(sno) = 'c2');
  set_forms column suppliersform tf (format(status) = '-n6.2');
  set_forms column suppliersform tf (format(sname) = 'c5');
  message 'Changing fields to display-only...';
  sleep 2;
  set_forms field suppliersform (displayonly(sno) = 1);
  set_forms column suppliersform tf (displayonly(status) = 1);
  set_forms column suppliersform tf (displayonly(city) = 1);
  callproc proc3_905;
}
!!
? fill proc3_905.osq 
!!
procedure proc3_905 =
{ message 'Setting all fields to non-invisible...';
  sleep 2;
  set_forms field suppliersform (invisible(sno) = 0);
  set_forms column suppliersform tf (invisible(sname) = 0);
  set_forms column suppliersform tf (invisible(status) = 0);
  set_forms column suppliersform tf (invisible(city) = 0);
}
!!
