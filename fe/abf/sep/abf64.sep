    EXEC 4GL GET GLOBAL CONSTANT (:var11 = gc11);

    sprintf(str, "AFTER GET GC: var1 = %s, var2 = %s, var3 = %s, var4 = %s, var5 = %-
lf, var6 = %lf, var7 = %lf, var8 = %lf, var9 = %lf, var10 = %d, var11 = %d", var1, v-
