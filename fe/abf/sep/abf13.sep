initialize (rval = integer4) =
{
    inittable tf1 fill;
    insertrow tf1[1] (c1 = '1abc');
    insertrow tf1[1] (c1 = '2def');
    insertrow tf1[2] (c1 = '3ghi');
    insertrow tf1[3] (c1 = '4jkl');
    insertrow tf1[4] (c1 = '5mno');
    insertrow tf1[5] (c1 = '6pqr');
    insertrow tf1[6] (c1 = '7stuv');
}


'test1' =
{
    rval = callproc find_record (table_field = 'tf1',
				 column = 'c1',
				 value = '1abc',
				 noscroll = 0);
    message 'rval = ' + varchar(:rval)
	with style = popup;
}

