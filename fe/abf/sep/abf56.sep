void init_sqlda(num_elem, type)
int	num_elem;
int	type;
{
	if (type == G3REC1) {
	     if (g3rec1da)
		free((char *)g3rec1da);
	     g3rec1da = (IISQLDA *)calloc(1,
	     IISQDA_HEAD_SIZE + (num_elem * IISQDA_VAR_SIZE));
	     g3rec1da->sqln = num_elem;
	}
	else if (type == G3REC2) {
	     if (g3rec2da)
		free((char *)g3rec2da);
	     g3rec2da = (IISQLDA *)calloc(1,
	     IISQDA_HEAD_SIZE + (num_elem * IISQDA_VAR_SIZE));
	     g3rec2da->sqln = num_elem;
	}
	else if (type == G4REC3) {
	     if (g4rec3da)
		free((char *)g4rec3da);
	     g4rec3da = (IISQLDA *)calloc(1,
	     IISQDA_HEAD_SIZE + (num_elem * IISQDA_VAR_SIZE));
	     g4rec3da->sqln = num_elem;
	}
	else if (type == G4REC3REC1) {
	     if (g4rec3rec1da)
		free((char *)g4rec3rec1da);
	     g4rec3rec1da = (IISQLDA *)calloc(1,
	     IISQDA_HEAD_SIZE + (num_elem * IISQDA_VAR_SIZE));
	     g4rec3rec1da->sqln = num_elem;
	}
	else if (type == G4REC3REC2) {
	     if (g4rec3rec2da)
		free((char *)g4rec3rec2da);
	     g4rec3rec2da = (IISQLDA *)calloc(1,
	     IISQDA_HEAD_SIZE + (num_elem * IISQDA_VAR_SIZE));
	     g4rec3rec2da->sqln = num_elem;
	}
	else {
	     if (objda)
		free((char *)objda);
	     objda = (IISQLDA *)calloc(1,
	     IISQDA_HEAD_SIZE + (num_elem * IISQDA_VAR_SIZE));
	     objda->sqln = num_elem;
	}
}

void alloc_buffer(type)
int	type;
{
	int		i;			/* Index into 'sqlvar' */
	IISQLVAR	*sqv;			/* Pointer to 'sqlvar */
	int		base_type;		/* Base type w/o nullability */
	int		res_cur_size;		/* Result size required */
        int             round;                  /* Alignment */
        int             num_elem;            

	/*
	** For each column  accumulate
	** the size of the result data area.
	*/
	if (type == G3REC1) 
		num_elem = g3rec1da->sqld;
	else if (type == G3REC2)
		num_elem = g3rec2da->sqld;
	else if (type == G4REC3)
		num_elem = g4rec3da->sqld;
	else if (type == G4REC3REC1)
		num_elem = g4rec3rec1da->sqld;
	else if (type == G4REC3REC2)
		num_elem = g4rec3rec2da->sqld;
	else
		num_elem = objda->sqld;

	for (res_cur_size = 0, i = 0; i < num_elem; i++)
	{
	    if (type == G3REC1) 
		sqv = &g3rec1da->sqlvar[i];
	    else if (type == G3REC2)
		sqv = &g3rec2da->sqlvar[i];
	    else if (type == G4REC3)
		sqv = &g4rec3da->sqlvar[i];
	    else if (type == G4REC3REC1)
		sqv = &g4rec3rec1da->sqlvar[i];
	    else if (type == G4REC3REC2)
		sqv = &g4rec3rec2da->sqlvar[i];
	    else
		sqv = &objda->sqlvar[i];

	    /* Find the base-type of the result (non-nullable) */
	    if ((base_type = sqv->sqltype) < 0)
		base_type = -base_type;

	    /* Collapse different types into INT, FLOAT or CHAR */
	    switch (base_type)
	    {
	      case IISQ_OBJ_TYPE:
		/* Always retrieve into a long integer */
		if (sqv->sqltype < 0)
		    sqv->sqltype = -IISQ_INT_TYPE;
		else
		    sqv->sqltype = IISQ_INT_TYPE;

	      case IISQ_INT_TYPE:
		res_cur_size += sizeof(long);
		sqv->sqllen = sizeof(long);
		break;

	      case IISQ_MNY_TYPE:
	      case IISQ_DEC_TYPE:
		/* Always retrieve into a double floating point */
		if (sqv->sqltype < 0)
		    sqv->sqltype = -IISQ_FLT_TYPE;
		else
		    sqv->sqltype = IISQ_FLT_TYPE;
		res_cur_size += sizeof(double);
		sqv->sqllen = sizeof(double);
		break;

	      case IISQ_FLT_TYPE:
		res_cur_size += sizeof(double);
		sqv->sqllen = sizeof(double);
		break;

	      case IISQ_DTE_TYPE:
		sqv->sqllen = DATESIZE;
		/* Fall through to handle like CHAR */

	      case IISQ_CHA_TYPE:
	      case IISQ_VCH_TYPE:
		/*
		** Assume no binary data is returned from the CHAR type.
		** Also allocate one extra byte for the null terminator.
		*/
		res_cur_size += sqv->sqllen + 1;
		/* Always round off to aligned data boundary */
		round = res_cur_size % 4;
		if (round)
		    res_cur_size += 4 - round;
		if (sqv->sqltype < 0)
		    sqv->sqltype = -IISQ_CHA_TYPE;
		else
		    sqv->sqltype = IISQ_CHA_TYPE;
		break;
	    } /* switch on base type */

	    /* Save away space for the null indicator */
	    if (sqv->sqltype < 0)
		res_cur_size += sizeof(short);
	} /* for each column */

	/* 
        ** At this point we've converted
	** all types to one of INT, CHAR or FLOAT.  Now we allocate a
	** single result buffer, and point all the result column data
	** areas into it.
	**
	** If we have an old result data area that is not large enough then
	** free it and allocate a new one.  Otherwise we can reuse the last
	** one.
	*/

	if (res_buf.res_length > 0 && res_buf.res_length < res_cur_size)
	{
	    free(res_buf.res_data);
	    res_buf.res_length = 0;
	}
	if (res_buf.res_length == 0)
	{
	    res_buf.res_data = (char *)calloc(1, res_cur_size);
	    res_buf.res_length = res_cur_size;
	}

	if (type == G3REC1) 
		num_elem = g3rec1da->sqld;
	else if (type == G3REC2)
		num_elem = g3rec2da->sqld;
	else if (type == G4REC3)
		num_elem = g4rec3da->sqld;
	else if (type == G4REC3REC1)
		num_elem = g4rec3rec1da->sqld;
	else if (type == G4REC3REC2)
		num_elem = g4rec3rec2da->sqld;
	else
		num_elem = objda->sqld;
	/*
	** Now for each column now assign the result address (sqldata) and
	** indicator address (sqlind) from the result data area.
	*/
	for (res_cur_size = 0, i = 0; i < num_elem; i++)
	{
	    if (type == G3REC1) 
		sqv = &g3rec1da->sqlvar[i];
	    else if (type == G3REC2)
		sqv = &g3rec2da->sqlvar[i];
	    else if (type == G4REC3)
		sqv = &g4rec3da->sqlvar[i];
	    else if (type == G4REC3REC1)
		sqv = &g4rec3rec1da->sqlvar[i];
	    else if (type == G4REC3REC2)
		sqv = &g4rec3rec2da->sqlvar[i];
	    else
		sqv = &objda->sqlvar[i];

	    /* Find the base-type of the result (non-nullable) */
	    if ((base_type = sqv->sqltype) < 0)
		base_type = -base_type;

	    /* Current data points at current offset */
	    sqv->sqldata = (char *)&res_buf.res_data[res_cur_size];
	    res_cur_size += sqv->sqllen;

	    if (base_type == IISQ_CHA_TYPE) 
	    {
		res_cur_size++;		/* Add one for null */
		round = res_cur_size % 4;       /* Round to aligned boundary */
		if (round)	
	                res_cur_size += 4 - round;
	    }

	    /* Point at result indicator variable */
	    if (sqv->sqltype < 0)
	    {
		sqv->sqlind = (short *)&res_buf.res_data[res_cur_size];
		res_cur_size += sizeof(int);
	    }
	    else
	    {
		sqv->sqlind = (short *)0;
	    } /* if type is nullable */
	} /* for each column */
} /* alloc_buffer */

void assign_sqlda(num_elem, type)
int	num_elem;
int	type;
{
	int		i, sqltype;
	IISQLVAR	*sqv;
	char 		colname[SHORTSTRLEN];

	if (type == 1) {
		strcpy(objda->sqlvar[0].sqldata, "New One\0");
		strcpy(objda->sqlvar[1].sqldata, "New Two\0");
		strcpy(objda->sqlvar[2].sqldata, "New Three\0");
		strcpy(objda->sqlvar[3].sqldata, "New Four\0");
	}
	else if (type == 3) 
		*(double *)objda->sqlvar[0].sqldata = *(double *)objda->sqlvar[0].sqldata + (doubl-
e)5;
	else if (type == G3REC1) {
		strcpy(g3rec1da->sqlvar[0].sqldata, "New Five\0");
		strcpy(g3rec1da->sqlvar[1].sqldata, "New Six\0");
		strcpy(g3rec1da->sqlvar[2].sqldata, "New Seven\0");
		strcpy(g3rec1da->sqlvar[3].sqldata, "New Eight\0");
	}
	else if (type == 4)
		strcpy(objda->sqlvar[0].sqldata, "21-oct-1991 08:09:55\0");
	else if (type == G4REC3) 
		*(double *)g4rec3da->sqlvar[0].sqldata = *(double *)g4rec3da->sqlvar[0].sqldata + -
(double)5;
	else if (type == G4REC3REC1) {
		strcpy(g4rec3rec1da->sqlvar[0].sqldata, "New Nine\0");
		strcpy(g4rec3rec1da->sqlvar[1].sqldata, "New Ten\0");
		strcpy(g4rec3rec1da->sqlvar[2].sqldata, "New Eleven\0");
		strcpy(g4rec3rec1da->sqlvar[3].sqldata, "New Twelve\0");
	}
	else if (type == ARRREC11) {
		strcpy(objda->sqlvar[0].sqldata, "New Value One\0");
		strcpy(objda->sqlvar[1].sqldata, "New Value Two\0");
		strcpy(objda->sqlvar[2].sqldata, "New Value Three\0");
		strcpy(objda->sqlvar[3].sqldata, "New Value Four\0");
	}
	else if (type == ARRREC12) {
		strcpy(objda->sqlvar[0].sqldata, "New Value Five\0");
		strcpy(objda->sqlvar[1].sqldata, "New Value Six\0");
		strcpy(objda->sqlvar[2].sqldata, "New Value Seven\0");
		strcpy(objda->sqlvar[3].sqldata, "New Value Eight\0");
	}
	else {
	     for (i=0; i<num_elem; i++) {

		if (type == G3REC2)
			sqv = &g3rec2da->sqlvar[i];
		else if (type == G4REC3REC2)
			sqv = &g4rec3rec2da->sqlvar[i];
		else
			sqv = &objda->sqlvar[i];

                if ((sqltype = sqv->sqltype) < 0)
			sqltype = -sqltype;
		switch (sqltype) {
		     case IISQ_INT_TYPE:
		        /* for array of type table only: 
			   leave last member alone */
			if ((i == (num_elem-1)) && (type == ARRTBL1 || type == ARRTBL2)) 
			   break;
			else {
                           *(long *)sqv->sqldata = *(long *)sqv->sqldata + (long)5;
			   break;
			}

		     case IISQ_FLT_TYPE:
                        *(double *)sqv->sqldata = *(double *)sqv->sqldata + (double)-
5;
			break;

		     case IISQ_CHA_TYPE:
/* workaround to bug 57124:  */ 
			sprintf(colname, "%s", sqv->sqlname.sqlnamec);
			if (!strcmp(colname,"ccol")) {
     			   if (type == RTBL) 
				strcpy(sqv->sqldata, "New FR\0");
