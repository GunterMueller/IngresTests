void gsarv(r1, r2, r3, r4)
EXEC SQL BEGIN DECLARE SECTION;
	int	r1;
	int	r2;
	int	r3;
	int	r4;
EXEC SQL END DECLARE SECTION;
{ 

	EXEC 4GL GET ATTRIBUTE :r1 (:r1varvar = varatt, :r1textvar = textatt, 
				    :r1charvar = charatt, :r1cvar = catt); 
	inq4gl(r1, NOTARRAY);
	sprintf(str,"GET ATTRIBUTE r1: %s_%s_%s_%s", r1cvar, r1charvar, r1textvar, r1varvar-
);
	EXEC 4GL CALLPROC viewstr(string = :str);

	EXEC 4GL GET ATTRIBUTE :r2 
		(:r2decvar = decatt, :r2decivar = deciatt, :r2f4var = f4att, 
		 :r2f8var = f8att, :r2floatvar = floatatt, :r2intvar = intatt, 
		 :r2smallvar = smallatt);
	inq4gl(r2, NOTARRAY);
	sprintf(str,"GET ATTRIBUTE r2: %lf_%.0lf_%lf_%lf_%lf_%d_%d", r2decvar, r2decivar, r-
2f4var, r2f8var, r2floatvar, r2intvar, r2smallvar);
	EXEC 4GL CALLPROC viewstr(string = :str);
	
	EXEC 4GL GET ATTRIBUTE :r3 (:r3moneyvar = moneyatt, 
				    :r3rec1var = rec1att); 
	inq4gl(r3, NOTARRAY);
	EXEC 4GL GET ATTRIBUTE :r3rec1var 
		(:r3cvar = catt, :r3charvar = charatt, :r3textvar = textatt, 
		 :r3varvar = varatt);
	inq4gl(r3rec1var, NOTARRAY);
	EXEC 4GL GET ATTRIBUTE :r3 (:r3rec2var = rec2att); 
	inq4gl(r3, NOTARRAY);
	EXEC 4GL GET ATTRIBUTE :r3rec2var 
		(:r3decvar = decatt, :r3decivar = deciatt, :r3f4var = f4att, 
		 :r3f8var = f8att, :r3floatvar = floatatt, :r3intvar = intatt,
		 :r3smallvar = smallatt); 
	inq4gl(r3rec2var, NOTARRAY);
	sprintf(str, "GET ATTRIBUTE r3: %lf REC1: %s_%s_%s_%s REC2: %lf_%lf_%lf_%lf_%lf_%d_-
%d", r3moneyvar, r3cvar, r3charvar, r3textvar, r3varvar, r3decvar, r3decivar, r3f4va-
r, r3f8var, r3floatvar, r3intvar, r3smallvar); 
	EXEC 4GL CALLPROC viewstr(string = :str);
	
	EXEC 4GL GET ATTRIBUTE :r4 (:r4datevar = dateatt, :r4rec3var = rec3att);
	inq4gl(r4, NOTARRAY);
	EXEC 4GL GET ATTRIBUTE :r4rec3var (:r4moneyvar = moneyatt, 
					   :r4rec1var = rec1att); 
	inq4gl(r4rec3var, NOTARRAY);
	EXEC 4GL GET ATTRIBUTE :r4rec1var (:r4cvar = catt, :r4charvar = charatt,
					   :r4textvar = textatt,
					   :r4varvar = varatt);
	inq4gl(r4rec1var, NOTARRAY);
	EXEC 4GL GET ATTRIBUTE :r4rec3var (:r4rec2var = rec2att); 
	inq4gl(r4rec3var, NOTARRAY);
	EXEC 4GL GET ATTRIBUTE :r4rec2var 
		(:r4decvar = decatt, :r4decivar = deciatt, :r4f4var = f4att, 
		 :r4f8var = f8att, :r4floatvar = floatatt, :r4intvar = intatt,
		 :r4smallvar = smallatt); 
	inq4gl(r4rec2var, NOTARRAY);
	sprintf(str, "GET ATTRIBUTE r4: %s REC3: %lf REC1: %s_%s_%s_%s REC2: %lf_%lf_%lf_%l-
f_%lf_%d_%d", r4datevar, r4moneyvar, r4cvar, r4charvar, r4textvar, r4varvar, r4decva-
r, r4decivar, r4f4var, r4f8var, r4floatvar, r4intvar, r4smallvar);
	EXEC 4GL CALLPROC viewstr(string = :str);
 
        strcpy (r1varvar, "NewVar");
        strcpy (r1textvar, "NewText");
        strcpy (r1charvar, "NewChar");
        strcpy (r1cvar, "NewC");
	EXEC 4GL SET ATTRIBUTE :r1 (varatt = :r1varvar, textatt = :r1textvar,
				    charatt = :r1charvar, catt = :r1cvar); 
	inq4gl(r1, NOTARRAY);
   /*   Somehow, the following is not acceptable => gives compilation error 
	EXEC 4GL SET ATTRIBUTE :r1 (catt = 'NewC' + :r1cvar); 
   */
	sprintf(str, "After SET ATTRIBUTE r1 (Note only r1 should change from its initial v-
alue, the rest should not) ...");
	EXEC 4GL CALLPROC viewstr(string = :str);
	EXEC 4GL CALLPROC viewatt();

	r2decvar = 1 + r2decvar;
	r2decivar = 1 + r2decivar;
	r2f4var = 1 + r2f4var;
	r2f8var = 1 + r2f8var;
	r2floatvar = 1 + r2floatvar;
	r2intvar = 1 + r2intvar;
	r2smallvar = 1 + r2smallvar;
	EXEC 4GL SET ATTRIBUTE :r2 
		(decatt = :r2decvar, deciatt = :r2decivar, f4att = :r2f4var, 
		 f8att = :r2f8var, floatatt = :r2floatvar, intatt = :r2intvar,
		 smallatt = :r2smallvar); 
	inq4gl(r2, NOTARRAY);
	sprintf(str, "After SET ATTRIBUTE r2 (Note only r1 and r2 should change from their -
initial values, the rest should not) ...");
	EXEC 4GL CALLPROC viewstr(string = :str);
	EXEC 4GL CALLPROC viewatt();

	r3moneyvar = 1 + r3moneyvar;
	EXEC 4GL SET ATTRIBUTE :r3 
		(moneyatt = :r3moneyvar, rec1att = :r1, rec2att = :r2); 
	sprintf(str, "After SET ATTRIBUTE r3 where rec1att = :r1, rec2att = :r2 (Note only -
r1, r2, and r3.money should change from their initial values, the rest should not ---
 SIR #54437) ...");
	EXEC 4GL CALLPROC viewstr(string = :str);
	EXEC 4GL CALLPROC viewatt();

        strcpy (r4datevar, "22-apr-1988 16:46:54");
	EXEC 4GL SET ATTRIBUTE :r4 (dateatt = :r4datevar, rec3att = :r3); 
	sprintf(str, "After SET ATTRIBUTE r4 where rec3att = :r3; note r1, r2, r3.money, an-
d r4.date should change ...");
	EXEC 4GL CALLPROC viewstr(string = :str);
	EXEC 4GL CALLPROC viewatt();

	r3moneyvar = 1 + r3moneyvar;
	EXEC 4GL SET ATTRIBUTE :r3 (moneyatt = :r3moneyvar);
        strcpy (r3varvar, "3ewVar");
        strcpy (r3textvar, "3ewText");
        strcpy (r3charvar, "3ewChar");
        strcpy (r3cvar, "3ewC");
	EXEC 4GL SET ATTRIBUTE :r3rec1var 
		(catt = :r3cvar, charatt = :r3charvar, textatt = :r3textvar,
		 varatt = :r3varvar);
	r3decvar = 1 + r3decvar;
	r3decivar = 1 + r3decivar;
	r3f4var = 1 + r3f4var;
	r3f8var = 1 + r3f8var;
	r3floatvar = 1 + r3floatvar;
	r3intvar = 1 + r3intvar;
	r3smallvar = 1 + r3smallvar;
	EXEC 4GL SET ATTRIBUTE :r3rec2var 
		(decatt = :r3decvar, deciatt = :r3decivar, f4att = :r3f4var, 
		 f8att = :r3f8var, floatatt = :r3floatvar, intatt = :r3intvar,
		 smallatt = :r3smallvar); 

	sprintf(str, "After SET ATTRIBUTE r3 (Note only r1, r2, r3 should change from their-
 initial values, the rest should not -- workaround to SIR #54437) ...");
	EXEC 4GL CALLPROC viewstr(string = :str);
	EXEC 4GL CALLPROC viewatt();

        strcpy (r4datevar, "22-apr-1988 16:46:54");
	EXEC 4GL SET ATTRIBUTE :r4 (dateatt = :r4datevar);
	r4moneyvar = 1 + r3moneyvar;
	EXEC 4GL SET ATTRIBUTE :r4rec3var (moneyatt = :r4moneyvar);
        strcpy (r4varvar, "4ewVar");
        strcpy (r4textvar, "4ewText");
        strcpy (r4charvar, "4ewChar");
        strcpy (r4cvar, "4ewC");
	EXEC 4GL SET ATTRIBUTE :r4rec1var 
		(catt = :r4cvar, charatt = :r4charvar, textatt = :r4textvar,
		 varatt = :r4varvar);
	r4decvar = 1 + r4decvar;
	r4decivar = 1 + r4decivar;
	r4f4var = 1 + r4f4var;
	r4f8var = 1 + r4f8var;
	r4floatvar = 1 + r4floatvar;
	r4intvar = 1 + r4intvar;
	r4smallvar = 1 + r4smallvar;
	EXEC 4GL SET ATTRIBUTE :r4rec2var 
		(decatt = :r4decvar, deciatt = :r4decivar, f4att = :r4f4var, 
		 f8att = :r4f8var, floatatt = :r4floatvar, intatt = :r4intvar,
		 smallatt = :r4smallvar); 

	sprintf(str, "After SET ATTRIBUTE r4 (Note all r1, r2, r3, r4 should change from th-
eir initial values -- workaround to SIR #54437) ...");
	EXEC 4GL CALLPROC viewstr(string = :str);
	EXEC 4GL CALLPROC viewatt();

/* ****************** other commands to be tested *************** 

	EXEC 4GL GET ATTRIBUTE :obj (:var:ind = attribute [,...]); 

	EXEC 4GL SET ATTRIBUTE :obj (attribute = :var:ind [,...]); 
*/
}

