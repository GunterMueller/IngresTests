EXEC SQL BEGIN DECLARE SECTION;
	int	r1;
	int	r2;
	int	r3;
	int	r4;
	int	index;
EXEC SQL END DECLARE SECTION;
{ 

	EXEC 4GL GET ATTRIBUTE :r1 :index (:r1varvar = varatt, 
						:r1textvar = textatt, 
						:r1charvar = charatt, 
						:r1cvar = catt); 
	inq4gl(r1, ISARRAY);
	sprintf(str,"GET ATTRIBUTE r1 [%d]: %s_%s_%s_%s", index, r1cvar, r1charvar, r1textv-
ar, r1varvar);
