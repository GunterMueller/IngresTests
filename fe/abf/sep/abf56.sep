void printsql(num_elem, strp, type)
int	num_elem;
char	*strp;
int	type;
{
   	IISQLVAR	*sqv;
   	int		i, sqltype;
   	char		str1[STRLEN];
   	char 		str2[SHORTSTRLEN];

	for (i=0; i<num_elem; i++) {

		if (type == G3REC1) 
	    		sqv = &g3rec1da->sqlvar[i];
	    	else if (type == G3REC2)
	    		sqv = &g3rec2da->sqlvar[i];
	    	else if (type == G4REC3)
	    		sqv = &g4rec3da->sqlvar[i];
	    	else if (type == G4REC3REC1)
	    		sqv = &g4rec3rec1da->sqlvar[i];
	    	else if (type == G4REC3REC2)
	    		sqv = &g4rec3rec2da->sqlvar[i];
	    	else
			sqv = &objda->sqlvar[i];

		sprintf(str1,"sqlvar[%d]: %d_%d_%s_%s_%d_",i, sqv->sqltype, sqv->sqllen, sqv->sqli-
nd, sqv->sqlname.sqlnamec, sqv->sqlname.sqlnamel);

		if (sqv->sqlind && *sqv->sqlind < 0)
		     sprintf(str2,"N/A");
		else {
                     if ((sqltype = sqv->sqltype) < 0)
			sqltype = -sqltype;
		     switch (sqltype) {
			case IISQ_INT_TYPE:
				sprintf(str2,"%d_", *(long *)sqv->sqldata);
				break;

			case IISQ_FLT_TYPE:
				sprintf(str2,"%g_", *(double *)sqv->sqldata);
				break;

			case IISQ_CHA_TYPE:
				sprintf(str2,"%s_", (char *)sqv->sqldata);
				break;
		     }
		} 
		strcat(str1,str2);
		strcat(strp,str1);
	}
}
