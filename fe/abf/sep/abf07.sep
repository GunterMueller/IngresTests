call_3p =
begin
    message 'calling user frame passing three parameters';
    sleep 2;
    callframe c40us1
    ( c40us1.field1 = 'Hi tester';
      c40us1.field2 = 1234 ;
      c40us1.field3 = 2345 );
end

call_2p =
begin
    message 'calling user frame passing two parameters';
    sleep 2;
    callframe c40us1
    ( c40us1.field1 = 'Hi again';
      c40us1.field2 = 5678 );
end

call_4p =
begin
    message 'calling user frame passing four parameters';
    sleep 2;
    callframe c40us1
    ( c40us1.field1 = 'Hi Hi';
      c40us1.field2 = 6666;
      c40us1.field3 = 7777;
      c40us1.field4 = 8888 );
end  
