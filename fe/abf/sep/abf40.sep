? fill cproc908.sc 
!!
cproc908 ()
{
   exec sql begin declare section;
      char bldg[3];
      char st_adr[30];
      char city[15];
      char state[3];
      char zip[6];
   exec sql end declare section;

   exec sql include sqlca;

   exec sql whenever sqlerror stop;

   exec frs forminit bldgformtbl;
   exec frs display bldgformtbl update;

   exec frs initialize;

   exec frs activate menuitem 'BldgInfo';
   exec frs begin;
      exec sql declare cursor1 cursor for
         select bldg, st_adr, city, state, zip from bldg;
      exec sql open cursor1;
      exec sql whenever not found goto cursor1_done;

      exec sql fetch cursor1 into :bldg, :st_adr, :city, :state, :zip;
      exec frs putrow bldgformtbl bldg 1 (bldg = :bldg, st_adr = :st_adr,
          city = :city, state = :state, zip = :zip);
      /* loop indefinitely */
      while (1)
      {
         exec sql fetch cursor1 into :bldg, :st_adr, :city, :state, :zip;
         exec frs insertrow bldgformtbl bldg (bldg = :bldg, st_adr = :st_adr,
             city = :city, state = :state, zip = :zip);
      }
   cursor1_done:
      exec sql close cursor1;
   exec frs end;

   exec frs activate menuitem 'Quit';
   exec frs begin;
      exec frs breakdisplay;
   exec frs end;

   return;
} 
