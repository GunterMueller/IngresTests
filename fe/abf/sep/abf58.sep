EXEC SQL BEGIN DECLARE SECTION;
	int	r1;
	int	r2;
	int	r3;
	int	r4;
EXEC SQL END DECLARE SECTION;
{ 

	EXEC 4GL GET ATTRIBUTE :r1 (:r1varvar = varatt, :r1textvar = textatt, 
				    :r1charvar = charatt, :r1cvar = catt); 
	inq4gl(r1, NOTARRAY);
	sprintf(str,"GET ATTRIBUTE r1: %s_%s_%s_%s", r1cvar, r1charvar, r1textvar, r1varvar-
);
	EXEC 4GL CALLPROC viewstr(string = :str);

	EXEC 4GL GET ATTRIBUTE :r2 
		(:r2decvar = decatt, :r2decivar = deciatt, :r2f4var = f4att, 
		 :r2f8var = f8att, :r2floatvar = floatatt, :r2intvar = intatt, 
		 :r2smallvar = smallatt);
	inq4gl(r2, NOTARRAY);
	sprintf(str,"GET ATTRIBUTE r2: %lf_%.0lf_%lf_%lf_%lf_%d_%d", r2decvar, r2decivar, r-
2f4var, r2f8var, r2floatvar, r2intvar, r2smallvar);
	EXEC 4GL CALLPROC viewstr(string = :str);
	
	EXEC 4GL GET ATTRIBUTE :r3 (:r3moneyvar = moneyatt, 
