	inq4gl(r3, NOTARRAY);
	EXEC 4GL GET ATTRIBUTE :r3rec1var 
		(:r3cvar = catt, :r3charvar = charatt, :r3textvar = textatt, 
		 :r3varvar = varatt);
	inq4gl(r3rec1var, NOTARRAY);
	EXEC 4GL GET ATTRIBUTE :r3 (:r3rec2var = rec2att); 
	inq4gl(r3, NOTARRAY);
	EXEC 4GL GET ATTRIBUTE :r3rec2var 
		(:r3decvar = decatt, :r3decivar = deciatt, :r3f4var = f4att, 
		 :r3f8var = f8att, :r3floatvar = floatatt, :r3intvar = intatt,
		 :r3smallvar = smallatt); 
	inq4gl(r3rec2var, NOTARRAY);
	sprintf(str, "GET ATTRIBUTE r3: %lf REC1: %s_%s_%s_%s REC2: %lf_%lf_%lf_%lf_%lf_%d_-
%d", r3moneyvar, r3cvar, r3charvar, r3textvar, r3varvar, r3decvar, r3decivar, r3f4va-
r, r3f8var, r3floatvar, r3intvar, r3smallvar); 
	EXEC 4GL CALLPROC viewstr(string = :str);
	
