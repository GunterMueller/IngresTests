? fill xwha17a.rw 
!!
.NAME	wha17a
.SREM   Testing 6.5 feature : Nofirstformfeed.  Checking for error messages
.QUERY
	SELECT	qevalue=e.qesalary, qeclass=int2(float8(e.qesalary/10000.))
	FROM	qeemp e
.SORT	qeclass : a
	,qevalue : a
/* Incorrect command - checking for error  */
.NOFISTFF
.HEADER	report
	.CE
	.PR "Salary Breakdown by Category"
	.NL 2
	.UNDERLINE
		.TAB 10 .PR "10000's"
		.TAB 20 .PR "20000's"
		.TAB 30 .PR "30000's"
		.TAB 40 .PR "40000's"
		.TAB 50 .PR "50000's"
	.NOUNDERLINE
	.NL 2
	.LM 0
	.BLOCK
.HEADER	qeclass
	.LM +10
	.TOP
	.LINESTART
.DETAIL
	.PRINTLINE qevalue(f5.0)
.FOOTER	report
	.ENDBLOCK
!!
? fill xwha17b.rw 
!!
.NAME	wha17b
.SREM   Testing 6.5 feature : No first formfeed - checking for error messages
.QUERY
	SELECT	e.qename, d.qebldg
	FROM	qeemp e, qedept d
	WHERE	e.qedept=d.qedname and e.qediv=d.qediv
.SORT	qebldg : a
	,qename : a
/* Incorrect command - check for error */
.FIRSTFF
.HEADER	report
	.LM 0
	.RM 60
	.UL
		.CE
		.PR "Listing of Employees by Building"
	.NOU
	.NL 3
	.UL
		.TAB 10 .PR "A"
		.TAB 20 .PR "B"
		.TAB 30 .PR "C"
		.TAB 40 .PR "D"
		.TAB 50 .PR "E"
	.NOU
	.NL 2
	.BLOCK
.HEADER	qebldg
	.LM +10
	.TOP
	.LINESTART
.DETAIL
	.PR qename(c10)
	.NL
.FOOTER	report
	.ENDBLOCK
!!
