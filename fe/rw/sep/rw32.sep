/*
**	Report: wfa03
**
**		Last Modified: 1993_11_24 19:10:55 GMT  
**		Level: 1
*/

.NAME	wfa03
.SHORTREMARK	This is a short remark!
.OUTPUT	wfa03
.PAGEWIDTH	80
.SETUP
	set lockmode session where readlock = nolock ; create view podetail_view as select -
orderno , det_tot
	 = qty*unit_pr from wfa03a ; create view tot_orders_view as select ord_sum = sum (d-
et_tot ) from
	podetail_view d , wfa03b h where d.orderno = h.orderno ;
.DECLARE
	rpt_title = varchar(80),
	col_1 = integer,
	col_2 = integer,
	col_3 = integer,
	col_4 = integer,
	col_5 = integer
.QUERY
	SELECT  v.vendorno, v.vname, h.orderno, d.det_tot, t.ord_sum
	FROM	 wfa03c v, wfa03b h, podetail_view d, tot_orders_view t
	WHERE	 d.orderno = h.orderno and h.vendorno = v.vendorno
.SORT	vendorno : a

.HEADER	report
	.LET col_1=0
	.LET col_2=25
	.LET col_3=35
	.LET col_4=50
	.LET col_5=65
	.LET rpt_title="Vendor Order Summary"
	.PAGELENGTH 22
	.LEFTMARGIN 0
	.RIGHTMARGIN 80
	.NEWPAGE 

.HEADER	page
	.NEWLINE 
	.UNDERLINE 
		.CENTER 
		.PRINT $rpt_title
	.NOUNDERLINE 
	.IF page_number!=1 .THEN
		.RIGHT 
		.PRINT page_number("n")
	.ENDIF 
	.NEWLINE 2
	.BLOCK 
		.NL 2
		.TAB $col_1
		.PRINTLN "Vendor"
		.TAB $col_2
		.PRINTLN "------"
		.TOP 
		.CENTER $col_2+3/2
		.PRINTLN "Number"
		.CENTER $col_2+3/2
		.PRINTLN "of"
		.CENTER $col_2+3/2
		.PRINTLN "Orders"
		.TAB $col_2
		.PRINTLN "------"
		.TOP 
		.NL 
		.CENTER $col_3+9/2
		.PRINTLN "Total"
		.CENTER $col_3+9/2
		.PRINTLN "Amount"
		.TAB $col_3
		.PRINTLN "---------"
		.TOP 
		.NL 
		.CENTER $col_4+9/2
		.PRINTLN "Average"
		.CENTER $col_4+9/2
		.PRINTLN "Amount"
		.TAB $col_4
		.PRINTLN "---------"
		.TOP 
		.CENTER $col_5+4/2
		.PRINTLN "Percentage"
		.CENTER $col_5+4/2
		.PRINTLN "of all"
		.CENTER $col_5+4/2
		.PRINTLN "Orders"
		.TAB $col_5
		.PRINTLN "----------"
	.ENDBLOCK 
	.NL 

.FOOTER	page
	.NEWLINE 
	.LEFT 
	.PRINT "ABC Corporation"
	.IF page_number=1 .THEN
		.CENTER 
		.PRINT page_number("zn")
	.ENDIF 

.FOOTER	vendorno
	.TAB $col_1
	.PRINT vname
	.TAB $col_2
	.PRINT count(orderno)("zzn")
	.TAB $col_3
	.PRINT sum(det_tot)("$$,$$$.$$")
	.TAB $col_4
	.PRINT avg(det_tot)("$$,$$$.$$")
	.TAB $col_5
	.PRINT (sum(det_tot)/ord_sum)*100(f4.2),"%"
	.NL 

.CLEANUP
	drop tot_orders_view ; drop podetail_view ;

