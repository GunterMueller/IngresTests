<<
~
>>
? fill wfa03.rw 
!!
.name  wfa03
.output wfa03 

.declare
	/*
	** set according to order type, used in page header
	*/
	rpt_title = varchar(80),

	/*
	** Column positioning  variables
	*/
	col_1 = integer, col_2 = integer, col_3 = integer,
	col_4 = integer, col_5 = integer

.setup
	/*
	** Exclusive access unecessary for summary report
	*/
	set lockmode session where readlock=nolock;

	/*
	** Create view with calculated total for each detail
	** in order to be able to sum() the value
	*/
	create view podetail_view
	as select orderno, det_tot=qty*unit_pr
   	     from wfa03a;

	/*
	** Create a view which contains the sum of all orders
	** of this type.
	*/
	create view tot_orders_view as
		select ord_sum = sum(det_tot)
		from podetail_view d, wfa03b h
		where d.orderno = h.orderno;

.cleanup
	drop tot_orders_view;
	drop podetail_view;

.query
	select v.vendorno, v.vname, h.orderno, d.det_tot, t.ord_sum
	from wfa03c v, wfa03b h, podetail_view d, tot_orders_view t
	where d.orderno = h.orderno and h.vendorno = v.vendorno

.sort vendorno 

.header report
	/*
	** Initialize column posistions and report title
	*/
	.let col_1 = 0
	.let col_2 = 25
	.let col_3 = 35
	.let col_4 = 50
	.let col_5 = 65
	.let rpt_title = "Vendor Order Summary"
	.include wfa03rpt.rw

.header page
	.include wfa03_pg.rw
	/* 
	** Center column headings for each column
	** Column headings may be more than one line
	*/
	.block
		.nl 2
		.tab $col_1          .println "Vendor"
		.tab $col_2          .println "------"
		.top
		.center $col_2 + 3/2 .println "Number"
		.center $col_2 + 3/2 .println "of"
		.center $col_2 + 3/2 .println "Orders"
		.tab    $col_2       .println "------"
		.top
		.nl
		.center $col_3 + 9/2 .println "Total"
		.center $col_3 + 9/2 .println "Amount"
		.tab    $col_3       .println "---------"
		.top
		.nl
		.center $col_4 + 9/2 .println "Average"
		.center $col_4 + 9/2 .println "Amount"
		.tab    $col_4       .println "---------"
		.top
		.center $col_5 + 4/2 .println "Percentage"
		.center $col_5 + 4/2 .println "of all"
		.center $col_5 + 4/2 .println "Orders"
		.tab    $col_5       .println "----------"
	.endblock
	.nl

.footer page
	.include wfa03ftr.rw

.footer vendorno
	.tab $col_1 .print vname
	.tab $col_2 .print count(orderno) ("zzn")
	.tab $col_3 .print sum(det_tot) ("$$,$$$.$$")
	.tab $col_4 .print avg(det_tot) ("$$,$$$.$$")
	.tab $col_5 .print (sum(det_tot)/ord_sum) * 100 (f4.2),"%"
	.nl
!!
? fill wfa03rpt.rw 
!!
.pagewidth   80
.pagelength  22
.leftmargin   0
.rightmargin 80
.newpage
!!
? fill wfa03_pg.rw 
!!
.newline
.underline
.center
.print $rpt_title
.nounderline
.if page_number != 1 .then
	.right
	.print page_number ("n")
.endif
.newline 2
!!
? fill wfa03ftr.rw 
!!
.newline
.left
.print "ABC Corporation"
.if page_number = 1 .then
	.center
	.print page_number ("zn")
.endif
!!
