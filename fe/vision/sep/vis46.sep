? fill copy.in 
!!
copy book_list(
	book_no= c0tab,
	title= varchar(0),
	author= varchar(0),
	price= c0tab,
	category= varchar(0),
	stock= c0tab,
	dist_no= c0nl,
	nl= d1)
from 'book_lis.doc';
\p\g
modify book_list to btree unique on
	book_no
with nonleaffill = 80,
leaffill = 70,
fillfactor = 80;
\p\g
grant all on book_list to public;
\p\g
copy cust_info(
	cust_no= c0tab,
	name= varchar(0),
	company= varchar(0) with null ('NULL'),
	street= varchar(0),
	city= varchar(0),
	state= varchar(0),
	zip= varchar(0),
	card_no= varchar(0),
	exp_date= c0tab,
	ship_to= varchar(0),
	nl= d1)
from 'cust_inf.doc';
\p\g
modify cust_info to btree unique on
	cust_no
with nonleaffill = 80,
leaffill = 70,
fillfactor = 80;
\p\g
grant all on cust_info to public;
\p\g
copy cust_orders(
	order_no= c0tab,
	cust_no= c0tab,
	order_date= c0tab,
	status= varchar(0),
	order_total= c0nl with null ('NULL'),
	nl= d1)
from 'cust_ord.doc';
\p\g
modify cust_orders to btree unique on
	order_no
with nonleaffill = 80,
leaffill = 70,
fillfactor = 80;
\p\g
grant all on cust_orders to public;
\p\g
copy book_orders(
	order_no= c0tab,
	book_no= c0tab,
	sale_price= c0tab with null ('NULL'),
	quantity= c0tab with null ('NULL'),
	extension= c0nl with null ('NULL'),
	nl= d1)
from 'book_ord.doc';
\p\g
modify book_orders to btree unique on
	order_no,
	book_no
with nonleaffill = 80,
leaffill = 70,
fillfactor = 80;
\p\g
grant all on book_orders to public;
\p\g
!!
