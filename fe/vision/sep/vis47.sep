!!
copy book_list(
	book_no= c0tab,
	title= varchar(0),
	author= varchar(0),
	price= c0tab,
	category= varchar(0),
	stock= c0tab,
	dist_no= c0nl,
	nl= d1)
from 'book2.doc';
\p\g
modify book_list to btree unique on
	book_no
with nonleaffill = 80,
leaffill = 70,
fillfactor = 80;
\p\g
grant all on book_list to public;
\p\g
!!
