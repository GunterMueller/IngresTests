? fill s3102.osq 
!!
initialize (
   slv = ARRAY OF a, 
   j   = integer,
   i = integer) =
   {
   message 'Select Print to see the array values';
   sleep 2;
   i := 0;
   }

'Print' = 
   { 
   while i < 10 do
      i := i + 1;
      j := slv[i].x;
      message 'the value of slv[' + varchar(i) + '] is ' + varchar(j);
      sleep 2;
   endwhile;
   return;
   }
!!
