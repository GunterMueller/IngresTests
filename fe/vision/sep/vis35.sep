? fill s3004.osq 
!!
initialize =
   {
   message 'Select Values to view the values of preset Global Constants.';
   sleep 2;
   }

'Values' = 
   {
   message 'Char (long)' + a;
   sleep 3;
   message 'Pie in char format looks like  ' + varchar(z);
   sleep 3;

   message 'Pie in varchar format appears as - ' + y + '-';
   sleep 3;
   message 'Meadow: ' + b;
   sleep 3;
 
   message 'Pie in integer format is represented by the numeral ' + varchar(x);
   sleep 3;
   message 'Printed integer should be (1234134) ' + varchar(w);
   sleep 3;

   message 'Pie in float format looks like  ' + varchar(f);
   sleep 3;
   message 'Several decimal places ' + varchar(gg);
   sleep 3;
   }

'Field' =
   {
   int1 := x;
   int2 := w;
   flt1 := f;
   flt2 := gg;
   chr1 := a; 
   chr2 := z;
   vch1 := b;
   vch2 := y;
   }

'End' =
   {
   return;
   }
!!
