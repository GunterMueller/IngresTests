/*
**  Frame:         u2602
**  Form:          u2602
**  Description:   First Update frame in application 2602
**
**  Source File:   u2602.osq
**  Frame Type:    Update
**  Template File: msupdate.tf, Version: 1.0
*/
INITIALIZE (
    /* hidden versions of primary keys & join fields */
    iih_prod_code = I4 WITH NULL,

    /* flag fields needed for NULLable key column(s) */
    iiNullKeyFlag1 = INTEGER(2) NOT NULL,

    /* working variables needed by template file */
    IIchar1     = CHAR(1),              /* holds answer to Yes/No prompts */
    IIerrorno   = INTEGER(4),           /* holds DBMS statement error nmbr */
    IIint       = INTEGER(4),           /* general purpose integer */
    IIint2      = INTEGER(4),           /* general purpose integer */
    IIobjname   = CHAR(32) NOT NULL,    /* holds an object name */
    IIrowcount  = INTEGER(4),           /* holds DBMS statement row count */
    IIrowsfound = CHAR(1),              /* tells if query selected >0 rows */
    IIupdrules  = INTEGER(2)            /* update rule status */
        ) =
BEGIN
    IIretval = 1;       /* Success. This built-in global is used for
                        ** communication between frames.
                        */
    SET_FORMS FRS (VALIDATE(NEXTFIELD) = 1, VALIDATE(PREVIOUSFIELD) = 1,
        ACTIVATE(NEXTFIELD) = 1, ACTIVATE(PREVIOUSFIELD) = 1,
        ACTIVATE(MENUITEM) = 1,  ACTIVATE(KEYS) = 1,
        GETMESSAGES = 0);
    /* query mode required for qualification() function */
    SET_FORMS FORM (MODE = 'fill');

    message 'I changed the mode from update to fill and removed whouse' +
	    ' from display.';

    sleep 2;

    COMMIT WORK;
    SET AUTOCOMMIT OFF;
END

'Go' (EXPLANATION = 'Run query'), KEY FRSKEY4 =
BEGIN
    IIrowsfound = 'n';

    MESSAGE 'Selecting data . . .';

						    /*# BEGIN Select\Master */
    u2602 := SELECT
    prod_code = m.prod_code, iih_prod_code = m.prod_code, 
      quantity = m.quantity, wsale_price = m.wsale_price, 
      retail_price = m.retail_price 
    FROM inventory m 
    WHERE
    QUALIFICATION(m.prod_code = prod_code, m.quantity = quantity, 
      m.wsale_price = wsale_price, m.retail_price = retail_price) 

						      /*# END Select\Master */
    BEGIN
    INITIALIZE =
    BEGIN
        COMMIT WORK;                    /* release locks */
        /* submenu temporarily puts form in update mode */
        IIrowsfound = 'y';    /* indicate that >0 rows qualified */
        SET_FORMS FORM (CHANGE = 0);    /* typing query Qual set change=1 */
    END
    
    'Next' (VALIDATE = 0, ACTIVATE = 0,
            EXPLANATION = 'Display next row of selected data'),
            KEY FRSKEY4 (VALIDATE = 0, ACTIVATE = 0) =
    BEGIN
        INQUIRE_FORMS FORM (IIint = CHANGE);
        IF (IIint = 1) THEN
            IIchar1 = PROMPT
                'Do you wish to "Next" without saving changes (y/n)? ';
            IF (UPPERCASE(:IIchar1) != 'Y') THEN
                MESSAGE 'Select menuitem "Save" to save changes.';
                SLEEP 2;
                RESUME MENU;
            ENDIF;
        ENDIF;
        SET_FORMS FORM (CHANGE = 0);
        NEXT;
        COMMIT WORK;    /* Release any locks acquired while selecting
                        ** data for the "NEXT" statement.
                        */
    END
    
    'Save' (ACTIVATE = 1,
            EXPLANATION = 'Update database with current screen data'),
            KEY FRSKEY8 (ACTIVATE = 1) =
    BEGIN
    
        INQUIRE_FORMS FORM (IIint = CHANGE);
        IF (IIint = 1) THEN
            VALIDATE;   /* validate all fields on form */
    
            MESSAGE 'Saving changes . . .';
    
            /* set NULLable-key flag fields */
            IF (iih_prod_code IS NULL) THEN
                iiNullKeyFlag1 = 1;
            ELSE
                iiNullKeyFlag1 = 0;
            ENDIF;
            
            REPEATED UPDATE inventory
            SET prod_code = :prod_code, quantity = :quantity, 
              wsale_price = :wsale_price, retail_price = :retail_price, 
              whouse_no = :whouse_no 
            WHERE ((:iiNullKeyFlag1 = 1 AND prod_code IS NULL) OR 
              (prod_code = :iih_prod_code)) ; 
            
            INQUIRE_INGRES (IIerrorno = ERRORNO);
            IF IIerrorno != 0 THEN
                ROLLBACK WORK;
                MESSAGE 'An error occurred while saving your changes to this'
                      + ' (master) data. Details about the error were'
                      + ' described by the message immediately preceding'
                      + ' this one. The "Save" operation was not performed.'
                      + ' Please correct the error and select "Save" again.'
                      WITH STYLE = POPUP;
                RESUME;
            ENDIF;
    
            COMMIT WORK;
            SET_FORMS FORM (CHANGE = 0);
    
            NEXT;
            COMMIT WORK;    /* Release any locks acquired while selecting
                            ** data for the "NEXT" statement.
                            */
        ELSE
            MESSAGE 'No changes to Save.';
            SLEEP 2;
        ENDIF;
    END                     /* end of 'Save' menuitem */
    
    'Delete' (VALIDATE = 0, ACTIVATE = 0,
            EXPLANATION = 'Delete current screen data from database') =
    BEGIN
        IIchar1 = PROMPT
            'Delete this data from the Database (y/n)?';
        IF (UPPERCASE(:IIchar1) != 'Y') THEN
            RESUME;
        ENDIF;
    
        MESSAGE 'Deleting . . .';
    
        /* set NULLable-key flag fields */
        IF (iih_prod_code IS NULL) THEN
            iiNullKeyFlag1 = 1;
        ELSE
            iiNullKeyFlag1 = 0;
        ENDIF;
        
        REPEATED DELETE FROM inventory
        WHERE ((:iiNullKeyFlag1 = 1 AND prod_code IS NULL) OR 
          (prod_code = :iih_prod_code)) ; 
        
        INQUIRE_INGRES (IIerrorno = ERRORNO);
        IF (IIerrorno != 0) THEN
            ROLLBACK WORK;
            MESSAGE 'An error occurred while Deleting this (master) data.'
                  + ' Details about the error were described by the'
                  + ' message immediately preceding this one.'
                  + ' This data was not Deleted.'
                  + ' Correct the error and select "Delete" again.'
                  WITH STYLE = POPUP;
            RESUME;
        ENDIF;
    
        COMMIT WORK;
        SET_FORMS FORM (CHANGE = 0);
    
        NEXT;       /* Data Deleted. Get Next screen of data. */
    
        COMMIT WORK;    /* Release any locks acquired while selecting
                        ** data for the "NEXT" statement.
                        */
    END    /* end of 'Delete' menuitem */
    
    'ListChoices' (VALIDATE = 0, ACTIVATE = 0,
            EXPLANATION = 'Show valid values for current field'),
            KEY FRSKEY10 (VALIDATE = 0, ACTIVATE = 0) =
    BEGIN
        IIint = -1;
        IIint = CALLPROC help_field();
    
        IF (IIint > 0) THEN
            /* value was selected onto form */
            SET_FORMS FORM (CHANGE = 1);
        ENDIF;
    END

    'Help' (VALIDATE = 0, ACTIVATE = 0,
        EXPLANATION = 'Display help for this frame'),
        KEY FRSKEY1 (VALIDATE = 0, ACTIVATE = 0) =
    BEGIN
        HELPFILE 'First Update frame in application 2602'
        	'/pv1/testenv/fe/emerald/sep/u2602.hlp';
    END

    'End' (VALIDATE = 0, ACTIVATE = 0,
        EXPLANATION = 'Return from Update mode to Query mode'),
        KEY FRSKEY3 (VALIDATE = 0, ACTIVATE = 0) =
    BEGIN
        INQUIRE_FORMS FORM (IIint = CHANGE);
        IF (IIint = 1) THEN
            IIchar1 = PROMPT 'Do you wish to leave Update'
                           + ' without saving changes (y/n)? ';
            IF (UPPERCASE(:IIchar1) != 'Y') THEN
                MESSAGE 'Select menuitem "Save" to save changes.';
                SLEEP 2;
                RESUME MENU;
            ENDIF;
        ENDIF;
        ENDLOOP;        /* exit submenu */
    END
    END;        /* end of submenu in 'Go' menuitem */

    /* display mode reverts to prior mode ('query') after submenu */
    IF (IIrowsfound = 'y') THEN
        CLEAR FIELD ALL;
        SET_FORMS FORM (CHANGE = 0);
    ENDIF;
END /* end of 'Go' menuitem */

'Clear' (VALIDATE = 0, ACTIVATE = 0, EXPLANATION = 'Clear all fields') =
BEGIN
    CLEAR FIELD ALL;
END

'ListChoices' (VALIDATE = 0, ACTIVATE = 0,
        EXPLANATION = 'Show valid values for current field'),
        KEY FRSKEY10 (VALIDATE = 0, ACTIVATE = 0) =
BEGIN
    IIint = -1;
    IIint = CALLPROC help_field();

    IF (IIint > 0) THEN
        /* value was selected onto form */
        SET_FORMS FORM (CHANGE = 1);
    ENDIF;
END

'Help' (VALIDATE = 0, ACTIVATE = 0,
        EXPLANATION = 'Display help for this frame'),
        KEY FRSKEY1 (VALIDATE = 0, ACTIVATE = 0) =
BEGIN
    HELPFILE 'First Update frame in application 2602'
    	'/pv1/testenv/fe/emerald/sep/u2602.hlq';
END

'TopFrame' (VALIDATE = 0, ACTIVATE = 0,
        EXPLANATION = 'Return to top frame in application') =
BEGIN
    IIretval = 2;       /* return to top frame */
    RETURN ;
END

'End' (VALIDATE = 0, ACTIVATE = 0,
        EXPLANATION = 'Return to previous frame'),
        KEY FRSKEY3 (VALIDATE = 0, ACTIVATE = 0) =
BEGIN
    RETURN ;
END
