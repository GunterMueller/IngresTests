? fill proc1 
!!
PROCEDURE proc1() =
BEGIN
        message 'Assigning values to local variables...'; sleep 2;
        intvar = 10;
        cvar   = 'hello from cvar';
        message 'Proc1 calling proc2...'; sleep 2;
        callproc proc2;
END
!!
? fill proc2 
!!
PROCEDURE proc2(retval = decimal(3,1) not null) =
BEGIN
        message 'intvar = ' + varchar(intvar) with style = popup;
        message 'cvar = ' + cvar with style = popup;
        message 'Proc2 calling proc3...'; sleep 2;
        retval = callproc proc3;
        message 'Value returned from proc3 is: ' + varchar(retval)
                with style = popup;
END
!!
? fill proc3 
!!
PROCEDURE proc3() =
BEGIN
        return 1.0;
END
!!
? fill dummy_proc 
!!
PROCEDURE dummy_proc() =
BEGIN
        message 'this is a dummy procedure' with style = popup;
END
!!
? copyapp in -a -r SEPPARAMDB oma68.app 
