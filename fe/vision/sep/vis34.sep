? fill s3003.osq 
!!
initialize = 
   { 
   message 'Select Load to assign values to the global variables and Values' 
         + ' to view the values.' with style = popup;
   }

'Load' = 
   {
   aa := 12345;
   bb := 1.2345;
   cc := '12345c';
   dd := '12345t';
   ee := '12345char';
   ff := '12345v';
   gg := '12-mar-90';
   hh := '$123.45';

   ll := 12345;

   iivar := '12345x.v';
   iiint := 12345;

   mmtf.var := '12345x.v';
   mmtf.int := 12345;

   jjvar := '12345x.v';
   jjint := 12345;

   jj.var := '12345x.v';
   jj.int := 12345;
   }

'Values' = 
   {
   message 'AA = ' + varchar(aa) with style = popup;
   message 'BB = ' + varchar(bb) with style = popup;
   message 'CC = ' + varchar(cc) with style = popup;
   message 'DD = ' + varchar(dd) with style = popup;
   message 'EE = ' + varchar(ee) with style = popup;
   message 'FF = ' + varchar(ff) with style = popup;
   message 'GG = ' + varchar(gg) with style = popup;
   message 'HH = ' + varchar(hh) with style = popup;
   message 'LL = ' + varchar(ll) with style = popup;
   message 'MMTF.var = ' + varchar(mmtf.var) with style = popup;
   message 'MMTF.int = ' + varchar(mmtf.int) with style = popup;
   message 'JJ = ' + varchar(jj.var) with style = popup;
   message 'JJ = ' + varchar(jj.int) with style = popup;
   }

'End' =
   {
   return;
   }
!!
