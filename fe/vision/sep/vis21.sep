? fill t2404a.sql 
!!
create table cust_history (
   cname                char(30),
   cust_no              smallint,
   cust_since_date      date,
   credit_rating        char(4) with null,
   contact              char(30),
   loc_no               smallint with null);
\p\g

create table customer_info (
   cust_no      smallint,
   cname        char(30),
   loc_no       smallint,
   addr1        char(20) with null,
   addr2        char(20) with null,
   city         char(20),
   state        char(2) with null,
   country      char(20),
   zip          char(10),
   phone        i4 with null,
   contact      char(30),
   c_title      char(20) with null,
   bus_type     char(10) with null,
   sales_region char(2));
\p\g

create table inventory (
   prod_code    i4,
   quantity     i4,
   wsale_price  money,
   retail_price money with null,
   whouse_no    smallint);
\p\g

create table invoice (
   ky           smallint,
   invoiceno    i4,
   vendorno     i4,
   orderno      smallint,
   ordate       date with null,
   status       char(1));
\p\g

create table orders (
   cust_no      smallint,
   ordate       date with null,
   sales_rep    char(15),
   orderno      smallint with null,
   invoiceno    i4);
\p\g

create table product_lookup (
   prod_code    i4,
   prod_type    varchar(30));
\p\g

create table product_units (
   prod_code    i4,
   sales_price  money,
   unit_size    smallint,
   vendorno     smallint);
\p\g

create table ship_receive (
   prod_code    i4,
   quantity     i4,
   ship_date    date with null,
   arrive_date  date with null,
   cust_no      i2,
   loc_no       i2 with null,
   status       char(1));
\p\g

create table vendor (
   prod_code    i4,
   vendorno     i2,
   vname        char(30),
   addr1        char(20) with null,
   addr2        char(20) with null,
   city         char(20),
   state        char(2) with null,
   country      char(20),
   zip          char(10),
   phone        i4 with null,
   contact      char(30),
   c_title      char(20) with null,
   c_language   char(10) with null);
\p\g

create table warehouse (
   whouse_no    smallint,
   name         char(30),
   contact      char(30),
   c_title      char(20),
   addr1        char(20) with null,
   addr2        char(20) with null,
   city         char(20),
   state        char(2),
   country      char(20),
   zip          char(10),
   phone        i4 with null,
   c_language   char(10) with null);
\p\g

copy table cust_history (
   cname                = c13,
   cust_no              = c4,
   cust_since_date      = c11,
   credit_rating        = c4,
   contact              = c14,
   loc_no               = c0nl)
from 'cust_hist.d';
\p\g

copy table customer_info (
   cust_no      = c4,
   cname        = c14,
   loc_no       = c2,
   addr1        = c18,
   city         = c15,
   state        = c2,
   country      = c3,
   zip          = c5,
   phone        = c7,
   contact      = c14,
   c_title      = c7,
   bus_type     = c10,
   sales_region = c0nl)
from 'cust_info.d';
\p\g

copy table inventory (
   prod_code    = c4,
   quantity     = c4,
   wsale_price  = c6,
   retail_price = c6,
   whouse_no    = c0nl)
from 'inventory.d';
\p\g

modify inventory to isam unique on prod_code;\p\g

copy table invoice (
   ky = c1,
   invoiceno = c4,
   vendorno = c4,
   orderno = c2,
   ordate = c11,
   status = c0nl)
from 'invoice.d';
\p\g

copy table orders (
   cust_no      = c4,
   ordate       = c11,
   sales_rep    = c13,
   orderno      = c2,
   invoiceno    = c0nl)
from 'orders.d';
\p\g

copy table product_lookup (
   prod_code    = c3,
   prod_type    = c0nl)
from 'prod_look.d';
\p\g

copy table product_units (
   prod_code    = c3,
   sales_price  = c5,
   unit_size    = c3,
   vendorno     = c0nl)
from 'prod_units.d';
\p\g

copy table ship_receive (
   prod_code    = c4,
   quantity     = c4,
   ship_date    = c12,
   arrive_date  = c12,
   cust_no      = c5,
   loc_no       = c2,
   status       = c0nl)
from 'ship_rec.d';
\p\g

copy table vendor (
)
from 'vendor.d';
\p\g

copy table warehouse (
)
from 'warehouse.d';
\p\g\q
