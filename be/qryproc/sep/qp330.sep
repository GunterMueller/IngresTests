        Purpose      : Testing Ingres 2.6 SP2 'Lock Timeout of NOWAIT' 
        Input Files  : none
        Output Files : none
        Database     : SEPPARAMDB
        Tables       : qp330_tbl1, qp330_tbl2 
        Synopsis     : Testing that 'SET LOCKMODE ... TIMEOUT=NOWAIT' has 
                       no adverse effect when locks are requested on  
                       resources that are not currently locked. Savepoints
                       are created and rolled back during the MQT and
                       the resources are left in the correct post-MQT state. 
                       Locking Level = SESSION. 
        Setup        :

        History:  27-Feb-2003 (sykga01)  Created.
	  14-Jun-2004   (sinra04) Replaced esqlc command with sepesqlc command
			to make tests when run on VMS use same canon as that
			of  Unix & Windows.

*/

/*
** Define the SQL script to create
** and populate the test tables (qp330_tbl1 and qp330_tbl2)
*/

? fill setup.sql
!!
create table qp330_tbl1 (lname char(24), fname char(24), telephone char(14))\p\g
insert into qp330_tbl1 values ('Davis', 'Steven', '(516) 735-4647')\p\g
insert into qp330_tbl1 values ('Davidson', 'Mary', '(516) 735-8181')\p\g
insert into qp330_tbl1 values ('Davidson', 'John', '(516) 735-8181')\p\g
insert into qp330_tbl1 values ('Smith', 'William', '(631) 442-5242')\p\g
insert into qp330_tbl1 values ('Johnson', 'James', '(516) 675-6154')\p\g
insert into qp330_tbl1 values ('Walsh', 'Joan', '(516) 331-2365')\p\g
insert into qp330_tbl1 values ('Williams', 'Charles', '(212) 599-1001')\p\g
insert into qp330_tbl1 values ('Andrews', 'Alison', '(212) 664-3001')\p\g
create table qp330_tbl2 (lname char(24), fname char(24), telephone char(14))\p\g
insert into qp330_tbl2 values ('Bush', 'George', '(516) 735-4647')\p\g
insert into qp330_tbl2 values ('Blair', 'Tony', '(516) 735-8181')\p\g
insert into qp330_tbl2 values ('Hussain', 'Saddam', '(516) 735-8181')\p\g
!!

/*
** Define the main C executable
*/ 

? fill qp330.sc 
!!
/*
** Standard C Runtime header file
*/
#include <stdio.h>
/*
**  SQLCA required by Ingres LIBQ runtime for Ingres error handling
*/
EXEC SQL INCLUDE sqlca;   

/*
**  Local variable: program syntax message 
*/
char  *syntax = "\nSyntax:  qp330.sep, dbname: qryprocdb\n";

/*
** The main() program
*/

main(int argc, char *argv[])
{
/*
**  Declare host language variables used in queries.
*/
   EXEC SQL BEGIN DECLARE SECTION;
        char   *dbname;
        char   qp1[72];
        char   qp2[72];
        char   err_msg[101];
   EXEC SQL END DECLARE SECTION;


/*
**  Checks that 2 arguments were specified on the command
**  line that launched the program (1 arg plus the program name).
**  If not enough args, then print a message and exit. 
*/
   if (argc < 2)
   {
      printf(syntax);
      exit(-1);
   }
/*
**  Set local variables: dbname is the database passed in at the 
**  command line.
*/
   dbname = argv[1];

/*
** Connect to Session 1
*/

   printf("\nConnecting to Session 1\n");

   EXEC SQL CONNECT :dbname session 1;


/*
**  Set the error handler to 
**  call an error routine but continue program execution. 
*/

    EXEC SQL WHENEVER NOT FOUND CONTINUE;

        EXEC SQL WHENEVER SQLWARNING CONTINUE;
        EXEC SQL WHENEVER SQLERROR goto ERR; 
    EXEC SQL COMMIT;

    START:

/*
** Set lockmode session timeout to NOWAIT and query the 
** 'locked' qp330_tbl1 table (Level=SESSION). 
*/

   printf("\n");

   printf("\nSetting Lockmode Timeout NOWAIT (Level=SESSION)\n");

   EXEC SQL SET LOCKMODE SESSION WHERE LEVEL=SESSION, TIMEOUT=NOWAIT;

/*
**  START MQT
**  INSERT a row into the qp330_tbl2 table 
*/

   printf("\nINSERT first row into the qp330_tbl2 table in Session 1\n");

   EXEC SQL INSERT INTO 
                qp330_tbl2 
                (
                lname,
                fname,
                telephone
                )
            VALUES
                (
                'Clinton',
                'Bill',
                '012345678'
                );

/*
** Create a Savepoint for the qp330_tbl2 table
*/

    EXEC SQL savepoint a;


   printf("\nINSERT second row into the qp330_tbl2 table in Session 1\n");

   EXEC SQL INSERT INTO 
                qp330_tbl2 
                (
                lname,
                fname,
                telephone
                )
            VALUES
                (
                'Regan',
                'Ronald',
                '98765432'
                );

/*
** Create a Savepoint for the qp330_tbl2 table
*/

    EXEC SQL savepoint b;

/*
** Undo the second append.
*/

    EXEC SQL ROLLBACK TO a;

/*
**  Query the qp330_tbl1 table 
**  The qp330_tbl1 table is not locked and hence the query should
**  be successful
*/

   printf("\n");

   printf("\nSELECT from qp330_tbl1 table in Session 1\n");

   printf("\n");

   EXEC SQL SELECT 
                TRIM(lname) + ', ' + TRIM(fname) + ' ' + telephone
            INTO 
                :qp1
            FROM 
                qp330_tbl1
            ORDER BY lname, fname, telephone;
   EXEC SQL BEGIN;
      printf("%s\n", qp1);
   EXEC SQL END;


  EXEC SQL COMMIT;

/*
**  End of MQT
*/

      goto END;

/*
** ERROR ROUTINE:
** Return an error message and roll back the MQT
*/

   ERR:
       EXEC SQL WHENEVER SQLERROR CALL SQLPRINT;

           if (sqlca.sqlcode != 0) 
              { 
               EXEC SQL inquire_sql (:err_msg=ERRORTEXT);
               printf("\n%s\n", err_msg);
               EXEC SQL ROLLBACK; 
   	       printf("\nLock Timeout - MQT Rolled back\n");
              }

   END:

/*
** Check the MQT has completed successfully
*/

   printf("\nSELECT from qp330_tbl2 table to check Savepoint\n");

   printf("\n");

   EXEC SQL SELECT 
                TRIM(lname) + ', ' + TRIM(fname) + ' ' + telephone
            INTO 
                :qp2 
            FROM 
                qp330_tbl2 
            ORDER BY lname, fname, telephone;
   EXEC SQL BEGIN;
      printf("%s\n", qp2);
   EXEC SQL END;

/*  
**  Disconnect from Session 1.
*/

   printf("\nDisconnecting from Session 1\n");

   EXEC SQL disconnect session 1;

/*
**  END OF EXEC SQL Script. 
*/
}
!!

