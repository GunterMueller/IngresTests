#include    <stdio.h>
#define MEfill(n,v,s) memset(s,v,n)
#define MEcopy(source, n, dest)         memcpy(dest, source, n)
#define STcopy(a,b)   strcpy(b,a)

#if defined(size_t) || defined(_SIZE_T_DEFINED)
#define SIZE_TYPE	size_t
#else
#define SIZE_TYPE	unsigned int
#endif

#if defined(NT_GENERIC)
extern char * IIMEreqmem(unsigned short, SIZE_TYPE, int, long *);
#else
extern char * IIMEreqmem(unsigned short, SIZE_TYPE, char, int *);
#endif

EXEC SQL include sqlca;   

#define OK 0
#define FAIL 1
#define TRUE 1

/*
**
**  Name: blob_endselect.sc
**
**  Description:
**	This program creates a table with  serveral char columns and a     
**	20,000-byte blob. The blob is filled with characters. Then, this
**	program repeatedly selects rows, ending with an ENDSELECT on the
**	fifth row.
**
**  History:
**	12-feb-1999 (somsa01)
**	    Created (thanks to loera01).
*/

typedef struct
{
    char	*arg;
    int		arg_int;
} HDLR_PARM;

EXEC SQL begin declare section;
char	dbname[32];
EXEC SQL end declare section;

void blob_insert(void);
void loop(void);
int Get_Handler(HDLR_PARM *hdlr);
void print_err(void);

main(argc, argv)
int	argc;
char	*argv[];
{
    if (argc != 2)
    {
	printf("Usage:  blob_endselect <dbname>\n");
	PCexit(FAIL);
    }

    STcopy(argv[1], dbname);

    EXEC SQL whenever sqlerror continue;
    printf("Creating test table ...\n");
    EXEC SQL connect :dbname;
    EXEC SQL drop loop;

    EXEC SQL whenever sqlerror call print_err;
    EXEC SQL create table loop( x1 char(10), x2 char(10), x3 char(10), 
	x4 char(10), x5 char(10), x6 char(10), x7 char(10), x8 char(10), 
	x9 char(10), x10 char(10), x11 char(10), x12 char(10), x13 char(10), 
	x14 char(10), x15 char(10), x16 char(10), x17 char(10), x18 char(10), 
	x19 char(10), x20 char(10), x21 char(10), x22 char(10), x23 char(10), 
	x24 char(10), x25 char(10), x26 char(10), x27 char(10), x28 char(10), 
	x29 char(10), x30 long varchar) with page_size=4096;

    blob_insert();
    loop();

    printf("Done.\n");
    EXEC SQL disconnect;
    PCexit(OK);
}

void
blob_insert()
{
    EXEC SQL begin declare section;
    char	blob_val[20001];
    EXEC SQL end declare section;
    int		i;

    MEfill(20000, 'y', blob_val);
    blob_val[20000] = '\0';

    printf("Inserting values into table ...\n");
    for (i = 0; i < 30; i++)
    {
	EXEC SQL insert into loop values ('x', 'x', 'x', 'x', 'x', 'x', 'x',
					  'x', 'x', 'x', 'x', 'x', 'x', 'x',
					  'x', 'x', 'x', 'x', 'x', 'x', 'x',
					  'x', 'x', 'x', 'x', 'x', 'x', 'x',
					  'x', :blob_val); 
    }
}

void
loop()
{
    EXEC SQL begin declare section;
    char	x1[10];
    char	x2[10];
    char	x3[10];
    char	x4[10];
    char	x5[10];
    char	x6[10];
    char	x7[10];
    char	x8[10];
    char	x9[10];
    char	x10[10];
    char	x11[10];
    char	x12[10];
    char	x13[10];
    char	x14[10];
    char	x15[10];
    char	x16[10];
    char	x17[10];
    char	x18[10];
    char	x19[10];
    char	x20[10];
    char	x21[10];
    char	x22[10];
    char	x23[10];
    char	x24[10];
    char	x25[10];
    char	x26[10];
    char	x27[10];
    char	x28[10];
    char	x29[10];
    int		i=0;
    short	nullind, x1ind, x2ind, x3ind, x4ind, x5ind, x6ind, x7ind;
    short	x8ind, x9ind, x10ind, x11ind, x12ind, x13ind, x14ind, x15ind;
    short	x16ind, x17ind, x18ind, x19ind, x20ind, x21ind, x22ind;
    short	x23ind, x24ind, x25ind, x26ind, x27ind, x28ind, x29ind;
    EXEC SQL end declare section;
    int		j=0;
    char	hdlrdisp[6];
    HDLR_PARM	hdlr;
  
    hdlr.arg = (char *)IIMEreqmem(0, 20000, TRUE, NULL);

    printf("Selecting from test table ...\n");
    while (j++ < 20) 
    {
	i = 0;
	STcopy("*********", x1);
	STcopy("*********", x2);
	EXEC SQL repeated select x1, x2, x3, x4, x5, x6, x7, x8, x9, x10,
				x11, x12, x13, x14, x15, x16, x17, x18, x19,
				x20, x21, x22, x23, x24, x25, x26, x27, x28,
				x29, x30
			  into :x1:x1ind, :x2:x2ind, :x3:x3ind, :x4:x4ind,
				:x5:x5ind, :x6:x6ind, :x7:x7ind, :x8:x8ind,
				:x9:x9ind, :x10:x10ind, :x11:x11ind,
				:x12:x12ind, :x13:x13ind, :x14:x14ind,
				:x15:x15ind, :x16:x16ind, :x17:x17ind,
				:x18:x18ind, :x19:x19ind, :x20:x20ind,
				:x21:x21ind, :x22:x22ind, :x23:x23ind,
				:x24:x24ind, :x25:x25ind, :x26:x26ind,
				:x27:x27ind, :x28:x28ind, :x29:x29ind,
				datahandler(Get_Handler(&hdlr)):nullind
			  from loop;

EXEC SQL begin;
        hdlrdisp[0] = hdlr.arg[19995]; 
        hdlrdisp[1] = hdlr.arg[19996]; 
        hdlrdisp[2] = hdlr.arg[19997]; 
        hdlrdisp[3] = hdlr.arg[19998]; 
        hdlrdisp[4] = hdlr.arg[19999]; 
        hdlrdisp[5] = '\0';
        printf("\t%d. %s\t%s\t%s\n",i+1,x1,x2,hdlrdisp);
 	if (++i == 5) 
        {
            printf("Doing endselect ...\n"); 
	    EXEC SQL endselect; 
            printf("%d. Done.\n",j); 
	}  
EXEC SQL end; 
    }
}

int
Get_Handler(HDLR_PARM *hdlr)
{ 
    EXEC SQL begin declare section;
    char	seg_buf[2000];
    int		seg_len;
    int		data_end = 0;
    EXEC SQL end declare section;
    char	hdlrdisp[6];
    int		i = 0, j = 0, k = 0;

    printf("	Data handler for blob.\n");
    while (!data_end)
    {
	for (i = 0; i < 2000; i++)
	    seg_buf[i] = '*';

	EXEC SQL get data ( :seg_buf = segment, :seg_len = segmentlength,
			    :data_end = dataend ) with maxlength = 2000; 

	MEcopy(seg_buf, seg_len, &hdlr->arg[i]);
	i += seg_len;
	hdlrdisp[0] = seg_buf[1995]; 
	hdlrdisp[1] = seg_buf[1996]; 
	hdlrdisp[2] = seg_buf[1997]; 
	hdlrdisp[3] = seg_buf[1998]; 
	hdlrdisp[4] = seg_buf[1999]; 
	hdlrdisp[5] = '\0';
	printf("	Retrieving segment %d %s\n", ++j, hdlrdisp);

	if (j > 20)
	    PCexit(-1);

	for (k = 0; k < seg_len; k++)
	{
           if (seg_buf[k] != 'y')
	    	printf("	Bogus blob value of %c at pos %d\n",
			 seg_buf[k], k);
	}
   }

   return(0);
}

void
print_err() 
{
    EXEC SQL begin declare section;
    char	dsp[550];
    EXEC SQL end declare section;

    EXEC SQL whenever sqlerror goto bye;
    EXEC SQL whenever not found goto bye;

    EXEC SQL inquire_sql (:dsp = ERRORTEXT);
    printf("%s\n",dsp);

bye:
    EXEC SQL disconnect;
    PCexit(FAIL);
}    
