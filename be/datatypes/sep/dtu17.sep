    concat(nchar_col, vchar_col)
    concat(nvchar_col, char_col)
    concat(nvchar_col, vchar_col)
    trim(nchar_col) + '-' + trim(char_col)
    trim(nchar_col) + '+' + vchar_col
    nvchar_col + '**' + trim(char_col)
    nvchar_col + '_' + vchar_col
    

