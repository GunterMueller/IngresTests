? fill dtu17_combine.sc 
!!
/* This program is to create and execute database queries to compare
** and search the tables using the SQL + with LIKE/NOT LIKE predicates,
** trim(), and concat() functions.
*/

#include <stddef.h>
#include <stdio.h>
#include <stdlib.h>
#include <wchar.h>
EXEC SQL INCLUDE SQLCA;

#define UC_ASSIGN(l, r) wcscpy((l.text), (r)); (l).len = wcslen((l).text)
#define UC_TERM(v)      (v).text[(v).len] = L'\0'

EXEC SQL BEGIN DECLARE SECTION;
  char      *dbname;
  int       i1;
  char      st1[18];
  char      st2[31];
  wchar_t wc1[7], k1[7];
  wchar_t wc2[26];
  wchar_t wcc[128];
  nvarchar struct
  {
    short len;
    wchar_t text[26];
  } nv1, nv2, k2;

EXEC SQL END DECLARE SECTION;

static char ln[] = "|===============================================================-
==|\n";

void print_trailer()
{
  printf(ln);
}
void print_header(char *s)
{
  printf("%s\n%s", s, ln);
}

int main(int argc, char *argv[])
{
   int testid;

   dbname = argv[1];
   testid = atoi(argv[2]);
   EXEC SQL WHENEVER SQLERROR stop;
   EXEC SQL CONNECT :dbname;

   EXEC SQL WHENEVER SQLERROR call sqlprint;

   switch (testid)
   {
      case 1:
         print_header("Select UNICODE table...");
         EXEC SQL SELECT nchar_col, nvchar_col
         INTO :wc1, :wc2
         FROM dtu17_uni
         order by nchar_col, nvchar_col;
         EXEC SQL BEGIN;
            printf("|%S|%S|\n", wc1, wc2);
         EXEC SQL END;
         break;

      case 2:
         print_header("Select NON-UNICODE table...");
         EXEC SQL SELECT char_col, vchar_col
         INTO :st1, :st2
         FROM dtu17_char
         order by char_col, vchar_col;
         EXEC SQL BEGIN;
            printf("|%s|%s|\n", st1, st2);
         EXEC SQL END;
         break;

      case 3:
         print_header("nchar_col + char_col...");
         EXEC SQL SELECT dtu17_uni.nchar_col, dtu17_char.char_col,
         dtu17_uni.nchar_col + dtu17_char.char_col 
         INTO :wc1, :st1, :wcc
         FROM dtu17_uni, dtu17_char
         WHERE dtu17_uni.nchar_col LIKE 'on%' AND dtu17_char.char_col
         LIKE 'hun%'
         order by dtu17_uni.nchar_col, dtu17_char.char_col;
         EXEC SQL BEGIN;
            printf("|%S|%s|%S|\n", wc1, st1, wcc);
         EXEC SQL END;
         break;

      case 4:
         print_header("nchar_col + vchar_col...");
         EXEC SQL SELECT dtu17_uni.nchar_col, dtu17_char.vchar_col,
         dtu17_uni.nchar_col + dtu17_char.vchar_col
         INTO :wc1, :st2, :wcc
         FROM dtu17_uni, dtu17_char
         WHERE dtu17_uni.nchar_col LIKE 'eig%' AND dtu17_char.vchar_col
         LIKE 'TEN%'
         order by dtu17_uni.nchar_col, dtu17_char.vchar_col;
         EXEC SQL BEGIN;
            printf("|%S|%s|%S|\n", wc1, st2, wcc);
         EXEC SQL END;
         break;

      case 5:
         print_header("nvchar_col + char_col...");
         EXEC SQL SELECT dtu17_uni.nvchar_col, dtu17_char.char_col,
         dtu17_uni.nvchar_col + dtu17_char.char_col
         INTO :wc2, :st1, :wcc
         FROM dtu17_uni, dtu17_char
         WHERE dtu17_uni.nvchar_col like 'S%' AND dtu17_char.char_col
         LIKE 'bill%'
         order by dtu17_uni.nvchar_col, dtu17_char.char_col;
         EXEC SQL BEGIN;
            printf("|%S|%s|%S|\n", wc2, st1, wcc);
         EXEC SQL END;
         break;

      case 6:
         print_header("nvchar_col + vchar_col...");
         EXEC SQL SELECT dtu17_uni.nvchar_col, dtu17_char.vchar_col,
         dtu17_uni.nvchar_col + dtu17_char.vchar_col
         INTO :wc2, :st2, :wcc
         FROM dtu17_uni, dtu17_char
         WHERE dtu17_uni.nvchar_col NOT LIKE '%TY'
         AND dtu17_char.vchar_col NOT LIKE '%LION'
         order by dtu17_uni.nvchar_col, dtu17_char.vchar_col;
         EXEC SQL BEGIN;
            printf("|%S|%s|%S|\n", wc2, st2, wcc);
         EXEC SQL END;
         break;

      case 7:
         print_header("concat(nchar_col, char_col)...");
         EXEC SQL SELECT dtu17_uni.nchar_col, dtu17_char.char_col,
         concat(dtu17_uni.nchar_col, dtu17_char.char_col) 
         INTO :wc1, :st1, :wcc
         FROM dtu17_uni, dtu17_char
         order by dtu17_uni.nchar_col, dtu17_char.char_col;
         EXEC SQL BEGIN;
            printf("|%S|%s|%S|\n", wc1, st1, wcc);
         EXEC SQL END;
         break;

      case 8:
         print_header("concat(nchar_col, vchar_col)...");
         EXEC SQL SELECT dtu17_uni.nchar_col, dtu17_char.vchar_col,
         concat(dtu17_uni.nchar_col, dtu17_char.vchar_col)
         INTO :wc1, :st2, :wcc
         FROM dtu17_uni, dtu17_char
         order by dtu17_uni.nchar_col, dtu17_char.vchar_col;
         EXEC SQL BEGIN;
            printf("|%S|%s|%S|\n", wc1, st2, wcc);
         EXEC SQL END;
         break;

      case 9:
         print_header("concat(nvchar_col, char_col)...");
         EXEC SQL SELECT dtu17_uni.nvchar_col, dtu17_char.char_col,
         concat(dtu17_uni.nvchar_col, dtu17_char.char_col)
         INTO :wc2, :st1, :wcc
         FROM dtu17_uni, dtu17_char
         order by dtu17_uni.nvchar_col, dtu17_char.char_col;
         EXEC SQL BEGIN;
            printf("|%S|%s|%S|\n", wc2, st1, wcc);
         EXEC SQL END;
         break;

      case 10:
         print_header("concat(nvchar_col, vchar_col)...");
         EXEC SQL SELECT dtu17_uni.nvchar_col, dtu17_char.vchar_col,
         concat(dtu17_uni.nvchar_col+' ', dtu17_char.vchar_col)
         INTO :wc2, :st2, :wcc
         FROM dtu17_uni, dtu17_char
         order by dtu17_uni.nvchar_col, dtu17_char.vchar_col;
         EXEC SQL BEGIN;
            printf("|%S|%s|%S|\n", wc2, st2, wcc);
         EXEC SQL END;
         break;

      case 11:
         print_header("trim(nchar_col) + '-' + trim(char_col)...");
         EXEC SQL SELECT dtu17_uni.nchar_col, dtu17_char.char_col,
         trim(dtu17_uni.nchar_col) + '-' + trim(dtu17_char.char_col)
         INTO :wc1, :st1, :wcc
         FROM dtu17_uni, dtu17_char 
         order by dtu17_uni.nchar_col, dtu17_char.char_col;
         EXEC SQL BEGIN;
            printf("|%S|%s|%S|\n", wc1, st1, wcc);
         EXEC SQL END;
         break;

      case 12:
         print_header("trim(nchar_col) + '+' + vchar_col...");
         EXEC SQL SELECT dtu17_uni.nchar_col, dtu17_char.vchar_col,
         trim(dtu17_uni.nchar_col) + '+' + dtu17_char.vchar_col
         INTO :wc1, :st2, :wcc
         FROM dtu17_uni, dtu17_char 
         order by dtu17_uni.nchar_col, dtu17_char.vchar_col;
         EXEC SQL BEGIN;
            printf("|%S|%s|%S|\n", wc1, st2, wcc);
         EXEC SQL END;
         break;
 
      case 13:
         print_header("nvchar_col + '**' + trim(char_col)...");
         EXEC SQL SELECT dtu17_uni.nvchar_col, dtu17_char.char_col,
         dtu17_uni.nvchar_col + '**' + trim(dtu17_char.char_col)
         INTO :wc2, :st1, :wcc
         FROM dtu17_uni, dtu17_char 
         order by dtu17_uni.nvchar_col, dtu17_char.char_col;
         EXEC SQL BEGIN;
            printf("|%S|%s|%S|\n", wc2, st1, wcc);
         EXEC SQL END;
         break;

      case 14:
         print_header("nvchar_col + '_' + vchar_col...");
         EXEC SQL SELECT dtu17_uni.nvchar_col, dtu17_char.vchar_col,
         dtu17_uni.nvchar_col + '_' + dtu17_char.vchar_col
         INTO :wc2, :st2, :wcc
         FROM dtu17_uni, dtu17_char 
         order by dtu17_uni.nvchar_col, dtu17_char.vchar_col;
         EXEC SQL BEGIN;
            printf("|%S|%s|%S|\n", wc2, st2, wcc);
         EXEC SQL END;
         break;

   }
   print_trailer();

   EXEC SQL DISCONNECT;
}
