? fill dtu17_tbl.sc 
!!
/* This program is to create and populate 2 tables:
** a Unicode table with the data types of nchar and nvarchar,
** another table with the data types of char and varchar.
*/

#include <stddef.h>
#include <stdio.h>
#include <stdlib.h>
#include <wchar.h>
EXEC SQL INCLUDE SQLCA;

#define UC_ASSIGN(l, r) wcscpy((l.text), (r)); (l).len = wcslen((l).text)
#define UC_TERM(v)      (v).text[(v).len] = L'\0'

EXEC SQL BEGIN DECLARE SECTION;
typedef struct
{
    short len;
    wchar_t text[1];
} NVCH_STRUCT;
EXEC SQL END DECLARE SECTION;

static wchar_t *col2[] =
    {
    L"one",
    L"two",
    L"Three",
    L"four",
    L"Five",
    L"six",
    L"Seven",
    L"eight",
    };
static wchar_t *col3[] =
    {
    L"nine",
    L"TEN",
    L"twenty",
    L"THIRTY",
    L"FIFTY",
    L"SEVENTY",
    L"EIGHTY",
    L"NINTY",
    };
#define NUMROWS 8


int main(int argc, char *argv[])
{
   EXEC SQL BEGIN DECLARE SECTION;
      int   i;
      char  *dbname;
      wchar_t wc1[7];
      wchar_t wc2[26];
      nvarchar struct
      {
         short len;
         wchar_t text[7];
      } nv1;
      nvarchar struct
      {
         short len;
         wchar_t text[26];
      } nv2;
   EXEC SQL END DECLARE SECTION;

   dbname = argv[1];
   EXEC SQL WHENEVER SQLERROR stop;
   EXEC SQL CONNECT :dbname;

   EXEC SQL WHENEVER SQLERROR continue;

   EXEC SQL DROP TABLE dtu17_uni;
   EXEC SQL DROP TABLE dtu17_char;


   EXEC SQL WHENEVER SQLERROR call sqlprint;

   printf("Creating tables...\n");

   EXEC SQL CREATE TABLE dtu17_uni (
	nchar_col NCHAR(6) not null,
	nvchar_col NVARCHAR(25) not null);

   EXEC SQL CREATE TABLE dtu17_char (
	char_col CHAR(17) not null,
 	vchar_col VARCHAR(30) not null);

   printf("Inserting rows into unicode table...\n");

   for (i = 0; i < NUMROWS; i++)
   {
      wcscpy(wc1, col2[i]);
      wcscpy(wc2, col3[i]);
      UC_ASSIGN(nv1, col2[i]);
      UC_ASSIGN(nv2, col3[i]);

      EXEC SQL INSERT INTO dtu17_uni VALUES (:wc1, :wc2);

   }

  printf("Inserting rows into non-unicode table...\n");

  EXEC SQL INSERT INTO dtu17_char values ('hundred', 'HUNDRED');
  EXEC SQL INSERT INTO dtu17_char values ('thousand', 'THOUSAND');
  EXEC SQL INSERT INTO dtu17_char values ('ten-thousand', 'TEN_THOUSAND');
  EXEC SQL INSERT INTO dtu17_char values ('hundred^thousand', 'HUNDRED&THOUSAND');
  EXEC SQL INSERT INTO dtu17_char values ('million', 'MILLION');
  EXEC SQL INSERT INTO dtu17_char values ('ten*million', 'TEN MILLION');
  EXEC SQL INSERT INTO dtu17_char values ('HUNDRED million', 'hundred MILLION');
  EXEC SQL INSERT INTO dtu17_char values ('billion', 'BILLION');

  printf("Operation complete\n");

  EXEC SQL DISCONNECT;
}
