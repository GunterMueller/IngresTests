            printf("|%S|%s|\n", wc1, st2);
         EXEC SQL END;
         break;

      case 9:
         print_header("nvchar_col < char_col...");
         EXEC SQL SELECT dtu16_uni.nvchar_col, dtu16_char.char_col
         INTO :wc2, :st1
         FROM dtu16_uni, dtu16_char
