set autocommit on \p\g
drop session.gt \p\g
drop procedure p1 \p\g
drop procedure p2 \p\g
drop procedure p3 \p\g
drop procedure p4 \p\g
drop procedure p5 \p\g
drop procedure p6 \p\g
drop table t1 \p\g
drop table t2 \p\g


declare global temporary table session.gt(col1 int, col2 long byte) on commit 
preserve rows with norecovery, page_size= 4096 \p\g
insert into session.gt values (1000, 'gt.one') \p\g
insert into session.gt values (2000, 'gt.two') \p\g
insert into session.gt values (3000, 'gt.three') \p\g
insert into session.gt values (4000, 'gt.four') \p\g
insert into session.gt values (5000, 'gt.five') \p\g

create table t1(col1 byte(8) ) with page_size=4096 \p\g
insert into t1 values('t1.one') \p\g
insert into t1 values('t1.two') \p\g

create table t2(col1 int, col2 byte(8)) with page_size= 4096 \p\g

create procedure p1( gtt1 set of (col1 int, col2 long byte )) as 
begin 
    insert into t1 select char(col2,8) from gtt1 where char(col2,8)='gt.five';
end; \p\g

create procedure p2( gtt1 set of (col1 int, col2 long byte )) as 
begin 
    insert into t2 select col1, char(col2,8) from gtt1;
end; \p\g

create procedure p3( gtt1 set of (col1 int, col2 long byte )) as 
begin 
    insert into gtt1 values (500,'gt.new');
    insert into t2 select col1, char(col2,8) from gtt1 
    where char(col2,8)='gt.new';
end; \p\g

create procedure p4( gtt1 set of (col1 int, col2 long byte )) as 
begin 
    update t2 from gtt1 set col2='update' 
    where char(t2.col1,8)=char(gtt1.col1,8) and gtt1.col1=4000;
end; \p\g

create procedure p5( gtt1 set of (col1 int, col2 long byte )) as
begin
   delete from t2 where t2.col1 in (select gtt1.col1 from gtt1 
   where t2.col1=gtt1.col1 and 
   char(gtt1.col2,8)='gt.new' and gtt1.col1<=3000) ;
   delete from t1 where char(t1.col1,8) in (select char(col2,8) from gtt1);
end \p\g

create procedure p6 (gtt1 set of (col1 int, col2 long byte)) as
begin
   delete from t2 where t2.col1 in (select gtt1.col1 from gtt1);
end \p\g

select * from t1 \p\g
execute procedure p1 (gtt1=session.gt) \p\g
select * from t1 \p\g

select * from t2 \p\g
execute procedure p2 (gtt1=session.gt) \p\g
select * from t2 \p\g

execute procedure p3 (gtt1=session.gt) \p\g
select * from t2 \p\g

execute procedure p4 (gtt1=session.gt) \p\g
select * from t2 \p\g
select * from t1 \p\g

execute procedure p5 (gtt1=session.gt) \p\g
select * from t2 \p\g
select * from t1 \p\g

execute procedure p6 (gtt1=session.gt) \p\g
select * from t2 \p\g

drop table t1 \p\g
drop table t2 \p\g        
drop procedure p1 \p\g
drop procedure p2 \p\g
drop procedure p3 \p\g
drop procedure p4 \p\g
drop procedure p5 \p\g
drop procedure p6 \p\g
drop table session.gt \p\g
