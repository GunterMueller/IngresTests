? usermod utl41db -uingres 
<<
INGRES COPYDB Copyright 2007 Ingres Corporation
Unload directory is '/tmp'.
Reload directory is '/tmp'.
There are 5 tables owned by user 'ingres'.
INGRES TERMINAL MONITOR Copyright 2007 Ingres Corporation
set autocommit on
set nojournaling
set session with privileges=all

	/* TABLES */

modify "endfor" to heap
with extend = 16,
	allocation = 4,
	page_size = 2048,
	nodependency_check
(3 rows)
modify "endrepeat" to hash on
	b
with minpages = 10,
	fillfactor = 50,
	extend = 16,
	page_size = 2048,
	nodependency_check
(3 rows)
modify "leave" to isam on
	c1
with fillfactor = 80,
	extend = 16,
	page_size = 2048,
	nodependency_check
(3 rows)
create index ("select" on "leave" (
	c1)
with structure = isam,
	page_size = 2048,
	nocompression,
	key = (c1),
	fillfactor = 80,
	location = (ii_database))
(3 rows)
modify "result" to btree on
	date
with nonleaffill = 80,
	leaffill = 70,
	fillfactor = 80,
	extend = 16,
	page_size = 2048,
	nodependency_check
(3 rows)
modify "row" to btree on
	c1
with nonleaffill = 80,
	leaffill = 70,
	fillfactor = 80,
	extend = 16,
	page_size = 2048,
	nodependency_check
(3 rows)
>>
